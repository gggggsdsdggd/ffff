				.. i
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################'})
				.. plr.Name
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','#################################################################################################','####################################################################################################################','################################'})
				.. target.Name
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'############################################','################################','################################################################################','###############################################################################################################','###################################################################################################################','#########################################################################################################','####################################################################################################################','#########################################################################################################','###############################################################################################################','##############################################################################################################','#############################################################'})
				.. tostring(shootPos)
		)
		task.wait(assistShotDelay)
	end
end

