-- Rapid Fire Toggle
TargetAimSection:AddToggle({
	Name = hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##################################################################################','#################################################################################################','################################################################################################################','#########################################################################################################','####################################################################################################','################################','######################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################'}),
	Default = false,
	Callback = function(state)
		RapidFireEnabled = state
