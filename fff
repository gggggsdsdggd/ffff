-- Jesus Cross Functions
local function createBackCross()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local torso = char:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8925,11760,11760,10605,11970,8820,11655,11970,12075,11655})) or char:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11970,12075,11655}))

    if char:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,10185,10395,11235,7035,11970,11655,12075,12075})) then
        char.BackCross:Destroy()
    end

    JesusCrossModel = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,11655,10500,10605,11340}))
    JesusCrossModel.Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,10185,10395,11235,7035,11970,11655,12075,12075})
    JesusCrossModel.Parent = char

    local verticalLength = 4
    local horizontalLength = 2
    local thickness = 0.4

    local vert = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8400,10185,11970,12180}))
    vert.Size = Vector3.new(thickness, verticalLength, thickness)
    vert.Material = JesusCrossMaterial
    vert.Color = JesusCrossColor
    vert.Transparency = 0.2
    vert.Anchored = false
    vert.CanCollide = false
    vert.Parent = JesusCrossModel

    local horiz = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8400,10185,11970,12180}))
    horiz.Size = Vector3.new(horizontalLength, thickness, thickness)
    horiz.Material = JesusCrossMaterial
    horiz.Color = JesusCrossColor
    horiz.Transparency = 0.2
    horiz.Anchored = false
    horiz.CanCollide = false
    horiz.Parent = JesusCrossModel

    local function weldPart(part, offsetCFrame)
        local weld = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9135,10605,11340,10500}))
        weld.Part0 = torso
        weld.Part1 = part
        weld.C0 = offsetCFrame
        weld.Parent = part
    end

    local offsetZ = 0.7
    weldPart(vert, CFrame.new(0, 0, offsetZ))
    weldPart(horiz, CFrame.new(0, verticalLength/4, offsetZ))

    if JesusPulseConnection then JesusPulseConnection:Disconnect() end
    local pulseDirection = 1
    JesusPulseConnection = RunService.Heartbeat:Connect(function()
        if not JesusCrossModel or not JesusCrossModel.Parent then
            JesusPulseConnection:Disconnect()
            return
        end
        vert.Transparency = math.clamp(vert.Transparency + 0.01 * pulseDirection, 0.1, 0.4)
        horiz.Transparency = vert.Transparency
        if vert.Transparency >= 0.4 then 
            pulseDirection = -1
        elseif vert.Transparency <= 0.1 then 
            pulseDirection = 1 
        end
    end)

    LocalPlayer.CharacterAdded:Connect(function(newChar)
        if JesusCrossEnabled then
            task.wait(1)
            createBackCross()
        end
    end)
end

local function enableJesusCross()
    task.wait(0.1)
    createBackCross()
end

local function disableJesusCross()
    if JesusCrossModel then JesusCrossModel:Destroy() end
    if JesusPulseConnection then JesusPulseConnection:Disconnect() end
    JesusCrossEnabled = false
end

-- Notification Function
local function notify(title, text, dur)
    if SEND_NOTIFICATIONS then
        pcall(function()
            StarterGui:SetCore(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10605,11550,10500,8190,11655,12180,11025,10710,11025,10395,10185,12180,11025,11655,11550}), {
                Title = title,
                Text = text,
                Duration = dur or 3,
            })
        end)
    end
end

-- Debug Function
local function dbgWarn(msg)
    if DEBUG_MODE then
        warn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9555,7140,7245,6930,8925,7455,9765,3360}) .. tostring(msg))
    end
end

-- *** YOUR ORIGINAL RAPID FIRE - ONLY TARGET ALL FIXED ***
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

local function findEquippedTool()
    for _, tool in ipairs(Character:GetChildren()) do
        if tool:IsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340})) then
            return tool
        end
    end
    return nil
end

local tool
local maid
local customCooldown = 0.01

local Handler = ReplicatedStorage:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605}))
