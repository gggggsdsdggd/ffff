				3
			)
			dbgWarn(
				hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################################################','##################################################################################################################','###############################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#########################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','####################################################################################################################','##########################################################','################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################','#################################################################################################','############################################################################################################','##################################################################################################################','#####################################################################################################','#################################################################################################','####################################################################################################','#########################################################################################################################','################################','#################################################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#####################################################################################################','####################################################################################################','##########################################################','################################'})
					.. target.DisplayName
			)
			return false
		end
	end
	-- Add player to assist list and enable assisting
	table.insert(AssistOthers.AllowedPlayers, userId)
	AssistOthers.Enabled = true
	lastProcessedInput = value
	notify(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################'}), hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##############################################################################','###############################################################################################################','#######################################################################################################################','################################','#################################################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################','################################'}) .. target.DisplayName, 3)
	dbgWarn(
		hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################################################','##################################################################################################################','###############################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#########################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','#################################################################################