local function addLabel(player)
    if LocalPlayer.UserId == MAIN_USERID and player ~= LocalPlayer and player:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) and player.Character and player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,10605,10185,10500})) then
        local head = player.Character.Head
        if head:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})) then return end
        local billboard = Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
