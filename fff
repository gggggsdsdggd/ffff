        local billboard = Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})
