-- Q INPUT
UserInputService.InputBegan:Connect(function(input, processed)
    if processed or input.UserInputType ~= Enum.UserInputType.Keyboard then return end
    
    if input.KeyCode == Keybinds.TargetLock and BulletTPEnabled then
        if Target then
            -- UNLOCK
            Target = nil
            Line.Visible = false
            Circle.Visible = false
            Text.Visible = false
            currentText = HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({})
            textIndex = 1
            if ViewEnabled then
                CurrentCamera.CameraSubject = Client.Character and Client.Character:FindFirstChildWhichIsA(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,12285,11445,10185,11550,11655,11025,10500}))
            end
        else
            -- LOCK
            Target = GetClosestTarget()
            if Target then
                if ViewEnabled then
                    local targetPart = Target.Character:FindFirstChild(TargetPart)
                    if targetPart then
                        CurrentCamera.CameraSubject = targetPart
                    end
                end
            end
        end
        UpdateViewTarget()
    end
    
    if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
        tpwalking = not tpwalking
    end
end)

-- Spinning Swastika Controls
SpinStructureSection:AddToggle({
    Name = HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,12495,10185,12075,12180,11025,11235,10185}),
    Default = false,
    Callback = function(state)
        SpinStructureEnabled = state
        if state then
            if game.Players.LocalPlayer.Character then
                setupStructure(game.Players.LocalPlayer.Character)
            end
            game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
                if SpinStructureEnabled then
                    task.wait(1)
                    setupStructure(char)
                end
            end)
        else
            removeStructure()
        end
    end,
})

SpinStructureSection:AddColorPicker({
    Name = HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,12495,10185,12075,12180,11025,11235,10185,3360,7035,11655,11340,11655,11970}),
    Default = SpinStructureColor,
    Callback = function(value)
        SpinStructureColor = value
        for _, data in ipairs(PartsAndWelds) do
            if data.part then
                data.part.Color = value
            end
        end
    end,
})

