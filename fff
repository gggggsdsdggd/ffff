local labelOffsetY = 0 -- Track vertical offset for stacking labels
local function CreateLabel(section, text)
	local label = Instance.new(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
	label.Text = text
	label.TextSize = 15
	label.TextColor3 = Color3.new(1, 1, 1)
	label.BackgroundTransparency = 1
	label.Size = UDim2.new(1, 0, 0, 20)
	label.Position = UDim2.new(0, 0, 0, labelOffsetY)
	label.Parent = ScreenGui
	labelOffsetY = labelOffsetY + 25
	return label
end

