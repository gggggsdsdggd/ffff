-- Tracer, FOV, and Text Animation
RunService.Heartbeat:Connect(function()
	if Target and BulletTPEnabled and TracerEnabled then
		local targetPart = Target.Character
			and Target.Character:FindFirstChild(Settings.Aimpart)
		if targetPart then
			local predictedPos = targetPart.Position
				+ (targetPart.Velocity * Settings.Prediction)
			local vector, onScreen =
				CurrentCamera:WorldToViewportPoint(predictedPos)
			local cameraPos = CurrentCamera.CFrame.Position
			local direction = (predictedPos - cameraPos).Unit
			local forward = CurrentCamera.CFrame.LookVector
			local dot = direction:Dot(forward)
			local screenPos
			if dot < 0 then
				local screenWidth = CurrentCamera.ViewportSize.X
				local screenHeight = CurrentCamera.ViewportSize.Y
				local aspectRatio = screenWidth / screenHeight
				local fov = CurrentCamera.FieldOfView
				local tanFov = math.tan(math.rad(fov / 2))
				local cameraRight = CurrentCamera.CFrame.RightVector
				local cameraUp = CurrentCamera.CFrame.UpVector
				local x = direction:Dot(cameraRight)
					/ (direction:Dot(forward) * tanFov * aspectRatio)
				local y = direction:Dot(cameraUp)
					/ (direction:Dot(forward) * tanFov)
				local clampX = math.clamp(x, -1, 1) * screenWidth / 2
					+ screenWidth / 2
				local clampY = math.clamp(y, -1, 1) * screenHeight / 2
					+ screenHeight / 2
				screenPos = Vector2.new(clampX, clampY + Inset)
			else
				screenPos = Vector2.new(vector.X, vector.Y)
			end
			local tracerColor = TracerRGBEffectEnabled
					and Color3.fromHSV((tick() % 5) / 5, 1, 1)
				or Settings.Color
			Line.Color = tracerColor
			Line.Transparency = Settings.Transparency
			Line.Thickness = Settings.Thickness
			Line.From = Vector2.new(Mouse.X, Mouse.Y + Inset)
			Line.To = screenPos
			Line.Visible = true
		else
			Line.Visible = false
		end
	else
		Line.Visible = false
	end
	if FOVEnabled then
		local fovColor = FOVRGBEnabled
				and Color3.fromHSV((tick() % 5) / 5, 1, 1)
			or Color3.fromRGB(255, 255, 255)
		Circle.Position = Vector2.new(Mouse.X, Mouse.Y + Inset)
		Circle.Visible = true
		Circle.Thickness = Settings.FOVThickness
		Circle.Radius = Settings.FOVRadius
		Circle.Color = fovColor
		if BulletTPEnabled and Target and Target.Character then
			local targetName = Target.DisplayName or YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
			if tick() - lastTextUpdate >= textUpdateDelay then
				if textIndex <= #targetName then
					currentText = string.sub(targetName, 1, textIndex)
					textIndex = textIndex + 1
				else
					textIndex = 1
					currentText = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
				end
				lastTextUpdate = tick()
			end
			Text.Text = currentText
			Text.Size = 16
			Text.Color = fovColor
			Text.Position =
				Vector2.new(Mouse.X, Mouse.Y + Inset + Settings.FOVRadius + 10)
			Text.Visible = true
			Text.Center = true
			Text.Outline = true
		else
			Text.Visible = false
			currentText = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
			textIndex = 1
		end
	else
		Circle.Visible = false
		Text.Visible = false
		currentText = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
		textIndex = 1
	end
	-- Assist Others firing
	if AssistOthers.Enabled then
		local now = tick()
		if now - lastAssistTime >= AssistOthers.Cooldown then
			for _, id in ipairs(AssistOthers.AllowedPlayers) do
				local plr = Players:GetPlayerByUserId(id)
				if
					plr
					and plr.Character
					and plr.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
					and getEquippedGun(plr)
				then
					pcall(function()
						fireGunForPlayer(plr)
					end)
				else
					dbgWarn(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,12075,12075,11025,12075,12180,3360,12075,11235,11025,11760,11760,10605,10500,3360,10710,11655,11970,3360,8925,12075,10605,11970,7665,10500,3360})
							.. id
							.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360,8400,11340,10185,12705,10605,11970,6405})
							.. tostring(plr)
							.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,7035,10920,10185,11970,10185,10395,12180,10605,11970,6405})
							.. tostring(plr and plr.Character)
					)
				end
			end
			lastAssistTime = now
		end
	end
	-- Lag bullets (all)
	if Lag.AllEnabled then
		local now = tick()
		if now - lastLagTime >= Lag.DelayTime then
			for _, plr in ipairs(Players:GetPlayers()) do
				if plr ~= LocalPlayer then
					pcall(function()
						fireLagBulletsAroundPlayer(plr)
					end)
				end
			end
			lastLagTime = now
		end
	end
end)

-- TP Walk Logic
RunService.Heartbeat:Connect(function(delta)
	if tpWalkEnabled and tpwalking then
		local chr = Client.Character
		local hum = chr and chr:FindFirstChildWhichIsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500}))
		local root = chr and chr:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
		if chr and hum and root and hum.MoveDirection.Magnitude > 0 then
			local success, err = pcall(function()
				root.CFrame = root.CFrame
					+ (hum.MoveDirection * tpWalkSpeed * delta * 10)
			end)
			if not success then
				-- Fallback to Humanoid:MoveTo
				local targetPos = root.Position
					+ (hum.MoveDirection * tpWalkSpeed * delta * 10)
				hum:MoveTo(targetPos)
				dbgWarn(
					YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,8400,3360,9135,10185,11340,11235,3360,7035,7350,11970,10185,11445,10605,3360,10710,10185,11025,11340,10605,10500,4620,3360,12285,12075,11025,11550,10815,3360,8085,11655,12390,10605,8820,11655,6090,3360}) .. tostring(err)
				)
			else
				dbgWarn(
					YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,8400,3360,9135,10185,11340,11235,3360,10185,11760,11760,11340,11025,10605,10500,6090,3360,8085,11655,12390,10605,10500,3360,10290,12705,3360})
						.. tostring(
							hum.MoveDirection * tpWalkSpeed * delta * 10
						)
				)
			end
		else
			dbgWarn(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,8400,3360,9135,10185,11340,11235,3360,10710,10185,11025,11340,10605,10500,6090,3360,7035,10920,10185,11970,10185,10395,12180,10605,11970,6405})
					.. tostring(chr)
					.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,7560,12285,11445,10185,11550,11655,11025,10500,6405})
					.. tostring(hum)
					.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,8610,11655,11655,12180,6405})
					.. tostring(root)
			)
		end
	end
end)

-- Prediction Function
local function PredictPosition(targetPart, bulletSpeed)
	if not targetPart or not targetPart:IsDescendantOf(workspace) then
		return targetPart and targetPart.Position or Vector3.zero
	end
	local char = Client.Character
	local hrp = char and char:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
	if not hrp then
		return targetPart.Position
	end
	local playerPos = hrp.Position
	local targetPos = targetPart.Position
	local targetVel = targetPart.Velocity
	local r = targetPos - playerPos
	local v = targetVel
	local s = bulletSpeed
	local a = v:Dot(v) - (s * s)
	local b = 2 * r:Dot(v)
	local c = r:Dot(r)
	local t
	local EPS = 1e-6
	if math.abs(a) < EPS then
		if math.abs(b) < EPS then
			return targetPos
		end
		t = -c / b
	else
		local disc = b * b - 4 * a * c
		if disc < 0 then
			return targetPos
		end
		local sqrtDisc = math.sqrt(disc)
		local t1 = (-b - sqrtDisc) / (2 * a)
		local t2 = (-b + sqrtDisc) / (2 * a)
		t = (t1 > 0 and t1) or (t2 > 0 and t2) or nil
	end
	if not t or t <= 0 or t > 3 then
		return targetPos
	end
	local predicted = targetPos + v * t
	predicted = targetPos:Lerp(predicted, LeadStrength)
	local ping = 0.08
	local ok, p = pcall(function()
		return Client:GetNetworkPing()
	end)
	if ok and type(p) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11550,12285,11445,10290,10605,11970}) then
		ping = p
	end
	ping = math.clamp(ping, 0.05, 0.5)
	predicted += v * ping
	if v.Magnitude > 0 then
		predicted += v.Unit * 0.1
	end
	return predicted
end

-- Bullet Hook for Target Aim
local function InstallBulletHook()
	local mt = getrawmetatable(game)
	setreadonly(mt, false)
	local previous = mt.__index
	local function hook(self, key)
		local ok, result = pcall(function()
			if
				not checkcaller()
				and self == Mouse
				and BulletTPEnabled
				and not RapidFireEnabled
				and not TargetAll
				and Target
				and (
					key == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,11025,12180})
					or (key == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180}) and game.PlaceId == 2788229376)
				)
			then
				local part = Target.Character
					and Target.Character:FindFirstChild(TargetPart)
				if part then
					local pos = PredictPosition(part, BulletSpeed)
					if typeof(pos) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9030,10605,10395,12180,11655,11970,5355}) then
						return CFrame.new(pos)
					end
				end
			end
			return previous(self, key)
		end)
		if ok then
			return result
		else
			return previous(self, key)
		end
	end
	local newIndex = newcclosure(hook)
	mt.__index = newIndex
	setreadonly(mt, true)
	return newIndex
end

local currentMouseIndex = InstallBulletHook()
task.spawn(function()
	while true do
		task.wait(0.25)
		local success, mt = pcall(getrawmetatable, game)
		if success and mt then
			local success_index, index = pcall(function()
				return mt.__index
			end)
			if success_index and index and index ~= currentMouseIndex then
				dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9135,10185,12180,10395,10920,10500,11655,10815,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,9975,9975,11025,11550,10500,10605,12600,3360,10395,10920,10185,11550,10815,10605,4620,3360,11970,10605,11025,11550,12075,12180,10185,11340,11340,11025,11550,10815,3360,10920,11655,11655,11235}))
				currentMouseIndex = InstallBulletHook()
			end
		end
	end
end)

-- Wallbang Module
local function applyWallbang()
	local success, err = pcall(function()
		local Handler = ReplicatedStorage:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605}))
		if not Handler then
			error(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605}))
		end
		local Module = require(Handler)
		Module.Ignored = {
			workspace:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9030,10605,10920,11025,10395,11340,10605,12075})),
			workspace:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,6825,8400})),
			workspace:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7665,10815,11550,11655,11970,10605,10500})),
		}
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9135,10185,11340,11340,10290,10185,11550,10815,3360,10185,11760,11760,11340,11025,10605,10500,6090,3360,7665,10815,11550,11655,11970,10605,10500,3360,9030,10605,10920,11025,10395,11340,10605,12075,4620,3360,8085,6825,8400,4620,3360,7665,10815,11550,11655,11970,10605,10500}))
	end)
	if not success then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9135,10185,11340,11340,10290,10185,11550,10815,3360,10605,11970,11970,11655,11970,6090,3360}) .. tostring(err))
	end
end

-- Hook GunHandler Shoot
local function hookGunHandlerShoot()
	local success, err = pcall(function()
		local gunHandler =
			require(ReplicatedStorage.Modules:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970})))
		local oldShoot = gunHandler.shoot
		gunHandler.shoot = function(args)
			if RapidFireEnabled or TargetAll then
				args.Range = RapidFire.InfiniteRange
				dbgWarn(
					YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7350,11655,11970,10395,10605,10500,3360,8610,10185,11550,10815,10605,6405})
						.. RapidFire.InfiniteRange
						.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,11025,11550,3360,10815,12285,11550,7560,10185,11550,10500,11340,10605,11970,4830,12075,10920,11655,11655,12180})
				)
			end
			return oldShoot(args)
		end
	end)
	if not success then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7245,11970,11970,11655,11970,3360,10920,11655,11655,11235,11025,11550,10815,3360,7455,12285,11550,7560,10185,11550,10500,11340,10605,11970,6090,3360}) .. tostring(err))
	end
end

task.spawn(function()
	applyWallbang()
	hookGunHandlerShoot()
end)

-- Rapid Fire Logic
local function randomVector3(maxOffset)
	return Vector3.new(
		(math.random() - 0.5) * 2 * maxOffset,
		(math.random() - 0.5) * 2 * maxOffset,
		(math.random() - 0.5) * 2 * maxOffset
	)
end

local function patchGun(tool)
	if not GUN_NAMES[tool.Name] or PatchedGuns[tool] then
		return
	end
	PatchedGuns[tool] = true
	local ammo = tool:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,11445,11445,11655}))
	local handle = tool:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,10185,11550,10500,11340,10605}))
	local gunHandler =
		require(ReplicatedStorage.Modules:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970})))
	local remote = tool:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
	local range = tool:WaitForChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10185,11550,10815,10605}))
	local maid = require(ReplicatedStorage.Modules.Maid).new()
	GunMaids[tool] = maid

	applyWallbang()

	task.spawn(function()
		while tool.Parent and tool.Parent == Client.Character do
			local success, err = pcall(function()
				ammo.Value = RapidFire.InfiniteAmmo
				range.Value = RapidFire.InfiniteRange
				dbgWarn(
					YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10605,12180,3360})
						.. tool.Name
						.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,6825,11445,11445,11655,6405})
						.. ammo.Value
						.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,8610,10185,11550,10815,10605,6405})
						.. range.Value
				)
			end)
			if not success then
				dbgWarn(
					YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7245,11970,11970,11655,11970,3360,12075,10605,12180,12180,11025,11550,10815,3360,10185,11445,11445,11655,4935,11970,10185,11550,10815,10605,3360,10710,11655,11970,3360})
						.. tool.Name
						.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360})
						.. tostring(err)
				)
			end
			task.wait(0.1)
		end
		maid:Destroy()
		GunMaids[tool] = nil
		PatchedGuns[tool] = nil
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7455,12285,11550,3360}) .. tool.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,12285,11550,10605,11865,12285,11025,11760,11760,10605,10500,3360,11655,11970,3360,11970,10605,11445,11655,12390,10605,10500,4620,3360,10395,11340,10605,10185,11550,10605,10500,3360,12285,11760,3360,8085,10185,11025,10500})
		)
	end)

	maid:GiveTask(RunService.Heartbeat:Connect(function()
		if
			not (RapidFireEnabled or TargetAll)
			or not mouseHeld
			or ammo.Value <= 0
			or tool.Parent ~= Client.Character
		then
			return
		end
		local success, err = pcall(function()
			range.Value = RapidFire.InfiniteRange
			applyWallbang()
			local muzzle = tool:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7140,10605,10710,10185,12285,11340,12180}))
				and tool.Default:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,10605,12075,10920}))
				and tool.Default.Mesh:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,12285,12810,12810,11340,10605}))
			local position = muzzle and muzzle.WorldPosition
				or handle.CFrame.Position
			if TargetAll then
				for _, p in pairs(Players:GetPlayers()) do
					if
						p ~= LocalPlayer
						and p.Character
						and p.Character:FindFirstChild(TargetPart)
					then
						local targetPart =
							p.Character:FindFirstChild(TargetPart)
						for i = 1, RapidFire.BulletsPerShot do
							local targetPos =
								PredictPosition(targetPart, BulletSpeed)
							local aimOffset =
								randomVector3(RapidFire.SpreadAmount)
							local finalPos = targetPos + aimOffset
							pcall(function()
								setidentity(2)
							end)
							local vShoot, v2, v3 = gunHandler.shoot({
								Shooter = LocalPlayer.Character,
								Handle = handle,
								ForcedOrigin = position,
								AimPosition = finalPos,
								BeamColor = Color3.new(1, 0.545, 0.149),
								Range = RapidFire.InfiniteRange,
							})
							pcall(function()
								setidentity(8)
							end)
							addShot()
							ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
								YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180,7455,12285,11550}),
								handle,
								position,
								vShoot,
								v2,
								v3
							)
							remote:FireServer(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180}))
							dbgWarn(
								YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,6825,11340,11340,3360,12075,10920,11655,12180,3360,10185,12180,3360})
									.. p.Name
									.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
									.. tostring(finalPos)
							)
						end
					end
				end
			elseif
				RapidFireEnabled
				and Target
				and Target.Character
				and Target.Character:FindFirstChild(TargetPart)
			then
				for i = 1, RapidFire.BulletsPerShot do
					local targetPos = PredictPosition(
						Target.Character:FindFirstChild(TargetPart),
						BulletSpeed
					)
					local aimOffset = randomVector3(RapidFire.SpreadAmount)
					local finalPos = targetPos + aimOffset
					pcall(function()
						setidentity(2)
					end)
					local vShoot, v2, v3 = gunHandler.shoot({
						Shooter = LocalPlayer.Character,
						Handle = handle,
						ForcedOrigin = position,
						AimPosition = finalPos,
						BeamColor = Color3.new(1, 0.545, 0.149),
						Range = RapidFire.InfiniteRange,
					})
					pcall(function()
						setidentity(8)
					end)
					addShot()
					ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180,7455,12285,11550}),
						handle,
						position,
						vShoot,
						v2,
						v3
					)
					remote:FireServer(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180}))
					dbgWarn(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,12075,10920,11655,12180,3360,10185,12180,3360})
							.. Target.Name
							.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
							.. tostring(finalPos)
					)
				end
			elseif RapidFireEnabled then
				for i = 1, RapidFire.BulletsPerShot do
					local aimOffset = randomVector3(RapidFire.SpreadAmount)
					local targetPos = position
						+ gunHandler.getAim(position) * RapidFire.InfiniteRange
						+ aimOffset
					pcall(function()
						setidentity(2)
					end)
					local vShoot, v2, v3 = gunHandler.shoot({
						Shooter = LocalPlayer.Character,
						Handle = handle,
						ForcedOrigin = position,
						AimPosition = targetPos,
						BeamColor = Color3.new(1, 0.545, 0.149),
						Range = RapidFire.InfiniteRange,
					})
					pcall(function()
						setidentity(8)
					end)
					addShot()
					ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180,7455,12285,11550}),
						handle,
						position,
						vShoot,
						v2,
						v3
					)
					remote:FireServer(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180}))
					dbgWarn(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,11445,11655,12285,12075,10605,3360,12075,10920,11655,12180,4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
							.. tostring(targetPos)
					)
				end
			end
		end)
		if not success then
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7350,11025,11970,11025,11550,10815,3360,10605,11970,11970,11655,11970,3360,10710,11655,11970,3360}) .. tool.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360}) .. tostring(err))
		end
	end))

	notify(
		YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605}),
		YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8400,10185,12180,10395,10920,10605,10500,3360}) .. tool.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10710,11655,11970,3360,8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,4515,3360,7665,11550,10710,11025,11550,11025,12180,10605,3360,6825,11445,11445,11655}),
		3
	)
	dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8400,10185,12180,10395,10920,10605,10500,3360}) .. tool.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10710,11655,11970,3360,8610,10185,11760,11025,10500,3360,7350,11025,11970,10605}))
	return maid
end

-- Auto Patch New Guns
Client.Backpack.ChildAdded:Connect(function(child)
	if GUN_NAMES[child.Name] then
		task.wait(0.1)
		patchGun(child)
	end
end)

-- Patch Existing Gun
local equippedGun = Client.Character
	and Client.Character:FindFirstChildOfClass(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340}))
if equippedGun and GUN_NAMES[equippedGun.Name] then
	patchGun(equippedGun)
end

-- Prevent Duplicate Firing
local oldFireServer
oldFireServer = hookmetamethod(game, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9975,9975,11550,10185,11445,10605,10395,10185,11340,11340}), function(self, ...)
	local method = getnamecallmethod()
	local args = { ... }
	if
		method == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7350,11025,11970,10605,8715,10605,11970,12390,10605,11970})
		and self.Name == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})
		and args[1] == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180})
		and (RapidFireEnabled or TargetAll)
	then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6930,11340,11655,10395,11235,10605,10500,3360,10500,10605,10710,10185,12285,11340,12180,3360,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180,6090,7350,11025,11970,10605,8715,10605,11970,12390,10605,11970,3360,10710,11655,11970,3360,8715,10920,11655,11655,12180,3360,12180,11655,3360,11760,11970,10605,12390,10605,11550,12180,3360,10500,12285,11760,11340,11025,10395,10185,12180,10605,3360,10710,11025,11970,11025,11550,10815})
		)
		return
	end
	return oldFireServer(self, ...)
end)

-- Cleanup and Re-patch on Respawn
Client.CharacterAdded:Connect(function(newCharacter)
	character = newCharacter
	tpwalking = false
	mouseHeld = false
	if
		ViewEnabled
		and Target
		and Target.Character
		and Target.Character:FindFirstChild(TargetPart)
	then
		local targetPart = Target.Character:FindFirstChild(TargetPart)
		if targetPart then
			CurrentCamera.CameraSubject = targetPart
			notify(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9030,11025,10605,12495,3360,8820,10185,11970,10815,10605,12180}),
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10605,4725,11340,11655,10395,11235,10605,10500,3360,10395,10185,11445,10605,11970,10185,3360,11655,11550,3360}) .. Target.DisplayName .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10185,10710,12180,10605,11970,3360,11970,10605,12075,11760,10185,12495,11550}),
				3
			)
		else
			ViewEnabled = false
			CurrentCamera.CameraSubject = newCharacter
				and newCharacter:FindFirstChildWhichIsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500}))
			notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9030,11025,10605,12495,3360,8820,10185,11970,10815,10605,12180}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,11760,10185,11970,12180,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,4620,3360,12390,11025,10605,12495,3360,10500,11025,12075,10185,10290,11340,10605,10500}), 3)
		end
	else
		CurrentCamera.CameraSubject = newCharacter
			and newCharacter:FindFirstChildWhichIsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500}))
	end
	Text.Visible = false
	Circle.Visible = false
	currentText = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
	textIndex = 1
	for tool, maid in pairs(GunMaids) do
		maid:Destroy()
		GunMaids[tool] = nil
		PatchedGuns[tool] = nil
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7035,11340,10605,10185,11550,10605,10500,3360,12285,11760,3360,8085,10185,11025,10500,3360,10710,11655,11970,3360}) .. tool.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,11655,11550,3360,11970,10605,12075,11760,10185,12495,11550}))
	end
	local equippedGun = newCharacter
		and newCharacter:FindFirstChildOfClass(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340}))
	if equippedGun and GUN_NAMES[equippedGun.Name] then
		patchGun(equippedGun)
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10605,4725,11760,10185,12180,10395,10920,10605,10500,3360,10605,11865,12285,11025,11760,11760,10605,10500,3360,10815,12285,11550,3360}) .. equippedGun.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,11655,11550,3360,11970,10605,12075,11760,10185,12495,11550}))
	end
	local childAddedConnection
	childAddedConnection = newCharacter.ChildAdded:Connect(function(child)
		if child:IsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340})) and GUN_NAMES[child.Name] then
			task.wait(0.1)
			patchGun(child)
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8400,10185,12180,10395,10920,10605,10500,3360,11550,10605,12495,3360,10815,12285,11550,3360}) .. child.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,11655,11550,3360,11550,10605,12495,3360,10395,10920,10185,11970,10185,10395,12180,10605,11970}))
		end
	end)
	newCharacter.AncestryChanged:Connect(function()
		if not newCharacter:IsDescendantOf(workspace) then
			childAddedConnection:Disconnect()
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7140,11025,12075,10395,11655,11550,11550,10605,10395,12180,10605,10500,3360,7035,10920,11025,11340,10500,6825,10500,10500,10605,10500,3360,11340,11025,12075,12180,10605,11550,10605,11970,3360,10710,11655,11970,3360,11655,11340,10500,3360,10395,10920,10185,11970,10185,10395,12180,10605,11970}))
		end
	end)
	applyWallbang()
	hookGunHandlerShoot()
	dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7035,10920,10185,11970,10185,10395,12180,10605,11970,3360,11970,10605,12075,11760,10185,12495,11550,10605,10500,4620,3360,11970,10605,4725,11760,10185,12180,10395,10920,10605,10500,3360,10815,12285,11550,12075}))
end)
    
