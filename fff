table.insert(
	BeamConnections,
	workspace.DescendantAdded:Connect(function(descendant)
		if not BeamVisualsEnabled or not descendant:IsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6930,10605,10185,11445})) then
			return
		end
		local isLocalPlayerBeam = false
		local debugReason = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
		local character = Client.Character
		local equippedGun = character
			and character:FindFirstChildOfClass(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340}))
		if descendant.Attachment0 or descendant.Attachment1 then
			local beamPart = descendant.Attachment0
					and descendant.Attachment0.Parent
				or descendant.Attachment1 and descendant.Attachment1.Parent
