					beamPart:IsDescendantOf(character)
					or (equippedGun and beamPart:IsDescendantOf(equippedGun))
				)
			then
				isLocalPlayerBeam = true
			else
				debugReason = debugReason
					.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180,3360,11550,11655,12180,3360,11340,11025,11550,11235,10605,10500,3360,12180,11655,3360,7035,10920,10185,11970,10185,10395,12180,10605,11970,4935,8820,11655,11655,11340,6195,3360})
			end
		end
