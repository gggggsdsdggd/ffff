	Callback = function()
		local listening = true
		notify(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','################################################################################','################################','#######################################################################################','#################################################################################################','############################################################################################################','###########################################################################################################','################################','###########################################################################','#####################################################################################################','#########################################################################################################################'}), hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################','##################################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','################################','#################################################################################################','################################','###########################################################################################################','#####################################################################################################','#########################################################################################################################','################################','####################################################################################################################','###############################################################################################################','################################','###################################################################################################################','#####################################################################################################','####################################################################################################################','##############################################','##############################################','##############################################'}), 5)
		local connection
		connection = UserInputService.InputBegan:Connect(
			function(input, processed)
				if
					processed
					or input.UserInputType ~= Enum.UserInputType.Keyboard
					or not listening
				then
					return
				end
				listening = false
				Keybinds.TPWalk = input.KeyCode
				TPWalkKeyLabel.Text = hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','################################################################################','################################','#######################################################################################','#################################################################################################','############################################################################################################','###########################################################################################################','################################','###########################################################################','#####################################################################################################','#########################################################################################################################','##########################################################','################################'}) .. input.KeyCode.Name
				dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','################################################################################','################################','#######################################################################################','#################################################################################################','############################################################################################################','###########################################################################################################','################################','###########################################################################################################','#####################################################################################################','#########################################################################################################################','################################','###################################################################################################################','#####################################################################################################','####################################################################################################################','################################','####################################################################################################################','###############################################################################################################','##########################################################','################################'}) .. input.KeyCode.Name)
				connection:Disconnect()
			end
		)
	end,
})

-- TP Walk Speed Slider
MovementSection:AddSlider({
	Name = hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','################################################################################','################################','#######################################################################################','#################################################################################################','############################################################################################################','###########################################################################################################','################################','###################################################################################','################################################################################################################','#####################################################################################################','#####################################################################################################','####################################################################################################'}),
	Min = 0,
	Max = 20,
	Default = 10,
	Callback = function(value)
		tpWalkSpeed = value
		notify(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','################################################################################','################################','#######################################################################################','#################################################################################################','############################################################################################################','###########################################################################################################'}), hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###################################################################################','################################################################################################################','#####################################################################################################','#####################################################################################################','####################################################################################################','################################','###################################################################################################################','#####################################################################################################','####################################################################################################################','################################','####################################################################################################################','###############################################################################################################','##########################################################','################################'}) .. tostr