        textLabel.BackgroundTransparency = 1
        textLabel.Text = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*******************************************************************************','***********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    end
end

local function removeLabel(player)
    if player.Character and player.Character:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'})) then
        local head = player.Character.Head
        if
            LocalPlayer.UserId == MAIN_USERID
            and head:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'}))
        then
            head.AltUserLabel:Destroy()
        elseif
            LocalPlayer.UserId ~= MAIN_USERID
            and LocalPlayer:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
            and head:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*******************************************************************************','***********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'}))
        then
            head.OwnerLabel:Destroy()
        end
    end
end

local function updateLabels()
    if not LocalPlayer:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})) then
        return
    end
    for _, player in ipairs(Players:GetPlayers()) do
        if
            (
                LocalPlayer.UserId == MAIN_USERID
                and player ~= LocalPlayer
                and player:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
            )
            or (
                LocalPlayer.UserId ~= MAIN_USERID
                and player.UserId == MAIN_USERID
            )
        then
            addLabel(player)
        else
            removeLabel(player)
        end
    end
end

if LocalPlayer:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})) then
    RunService.Heartbeat:Connect(updateLabels)
    Players.PlayerAdded:Connect(updateLabels)
    Players.PlayerRemoving:Connect(updateLabels)
end

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    local newFlag = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************','**************************************************************************************','*************************************************************************************************','************************************************************************************************************','*********************************************************************************************************************','*****************************************************************************************************'}))
    newFlag.Name = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})
    newFlag.Value = true
    newFlag.Parent = LocalPlayer
    updateLabels()
end)

updateLabels()    
