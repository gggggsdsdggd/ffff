#######################################################################################################','################################################################################################################','################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################','##########################################################','################################'}) .. value)
		return false
	end
	local target = findPlayer(value)
	if not target then
		notify(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################'}), hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################','##############################################################################################################','###############################################################################################################','####################################################################################################################','################################','######################################################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','##########################################################','################################'}) .. value, 3)
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################################################','##################################################################################################################','###############################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#########################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','####################################################################################################################','##########################################################','################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################','##############################################################################################################','###############################################################################################################','####################################################################################################################','################################','######################################################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################','#########################################################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','####################################################################################################################','##########################################################','################################'}) .. value)
		return false
	end
	local userId = target.UserId
	-- Check if player is already in the list
	for _, id in ipairs(AssistOthers.AllowedPlayers) do
		if id == userId then
			notify(
				hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################'}),
				target.DisplayName .. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','#########################################################################################################','###################################################################################################################','################################','#################################################################################################','############################################################################################################','##################################################################################################################','#####################################################################################################','#################################################################################################','####################################################################################################','#########################################################################################################################','################################','##################################################################################################','#####################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################','################################','#################################################################################################','###################################################################################################################','###########################################