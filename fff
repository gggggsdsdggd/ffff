-- Simplified Settings table
local Settings = {
    Aimpart = ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}),
    FOVRadius = 60,
    Prediction = 0.1,
    HitChamMaterial = ZuRdGvkgaGnppuwNtpwpGfeN({8190,10605,11655,11550}),
    Color = Color3.fromRGB(255, 255, 255),
}

-- *** ENHANCED JESUS CROSS FUNCTIONS (TEXT REMOVED) ***
local function createBackCross()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local torso = char:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8925,11760,11760,10605,11970,8820,11655,11970,12075,11655})) or char:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8820,11655,11970,12075,11655}))

    if char:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({6930,10185,10395,11235,7035,11970,11655,12075,12075})) then
        char.BackCross:Destroy()
    end

    JesusCrossModel = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8085,11655,10500,10605,11340}))
    JesusCrossModel.Name = ZuRdGvkgaGnppuwNtpwpGfeN({6930,10185,10395,11235,7035,11970,11655,12075,12075})
    JesusCrossModel.Parent = char

    local verticalLength = 4
    local horizontalLength = 2
    local thickness = 0.4

    local vert = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8400,10185,11970,12180}))
    vert.Size = Vector3.new(thickness, verticalLength, thickness)
    vert.Material = JesusCrossMaterial -- DYNAMIC MATERIAL!
    vert.Color = JesusCrossColor -- DYNAMIC COLOR!
    vert.Transparency = 0.2
    vert.Anchored = false
    vert.CanCollide = false
    vert.Parent = JesusCrossModel

    local horiz = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8400,10185,11970,12180}))
    horiz.Size = Vector3.new(horizontalLength, thickness, thickness)
    horiz.Material = JesusCrossMaterial -- DYNAMIC MATERIAL!
    horiz.Color = JesusCrossColor -- DYNAMIC COLOR!
    horiz.Transparency = 0.2
    horiz.Anchored = false
    horiz.CanCollide = false
    horiz.Parent = JesusCrossModel

    local function weldPart(part, offsetCFrame)
        local weld = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({9135,10605,11340,10500}))
        weld.Part0 = torso
        weld.Part1 = part
        weld.C0 = offsetCFrame
        weld.Parent = part
    end

    local offsetZ = 0.7
    weldPart(vert, CFrame.new(0, 0, offsetZ))
    weldPart(horiz, CFrame.new(0, verticalLength/4, offsetZ))

    -- Enhanced Pulse effect
    if JesusPulseConnection then JesusPulseConnection:Disconnect() end
    local pulseDirection = 1
    JesusPulseConnection = RunService.Heartbeat:Connect(function()
        if not JesusCrossModel or not JesusCrossModel.Parent then
            JesusPulseConnection:Disconnect()
            return
        end
        vert.Transparency = math.clamp(vert.Transparency + 0.01 * pulseDirection, 0.1, 0.4)
        horiz.Transparency = vert.Transparency
        if vert.Transparency >= 0.4 then 
            pulseDirection = -1
        elseif vert.Transparency <= 0.1 then 
            pulseDirection = 1 
        end
    end)

    LocalPlayer.CharacterAdded:Connect(function(newChar)
        if JesusCrossEnabled then
            task.wait(1)
            createBackCross()
        end
    end)
end

local function enableJesusCross()
    task.wait(0.1)
    createBackCross()
end

local function disableJesusCross()
    if JesusCrossModel then JesusCrossModel:Destroy() end
    if JesusPulseConnection then JesusPulseConnection:Disconnect() end
    JesusCrossEnabled = false
end

-- Notification Function
local function notify(title, text, dur)
    if SEND_NOTIFICATIONS then
        pcall(function()
            StarterGui:SetCore(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10605,11550,10500,8190,11655,12180,11025,10710,11025,10395,10185,12180,11025,11655,11550}), {
                Title = title,
                Text = text,
                Duration = dur or 3,
            })
        end)
    end
end

-- Debug Function
local function dbgWarn(msg)
    if DEBUG_MODE then
        warn(ZuRdGvkgaGnppuwNtpwpGfeN({9555,7140,7245,6930,8925,7455,9765,3360}) .. tostring(msg))
    end
end

-- Tracer Helper for Force Hit
local function createTracer()
    if tracerLine then
        tracerLine:Remove()
    end
    tracerLine = Drawing.new(ZuRdGvkgaGnppuwNtpwpGfeN({7980,11025,11550,10605}))
    tracerLine.Color = Color3.new(1, 0, 0)
    tracerLine.Thickness = 2
    tracerLine.Visible = false
end

local function updateTracer()
    if not (ForceHitEnabled and ForceHitTarget and tracerLine) then
        if tracerLine then
            tracerLine.Visible = false
        end
        return
    end

    local targetChar = ForceHitTarget.Character
    if not targetChar then
        tracerLine.Visible = false
        return
    end

    local cam = workspace.CurrentCamera
    local targetHead = targetChar:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}))

    if targetHead then
        local fromPos = Vector2.new(Mouse.X, Mouse.Y + Inset)
        local toPos, onScreen2 = cam:WorldToViewportPoint(targetHead.Position)
        if onScreen2 then
            tracerLine.From = fromPos
            tracerLine.To = Vector2.new(toPos.X, toPos.Y)
            tracerLine.Visible = true
        else
            tracerLine.Visible = false
        end
    else
        tracerLine.Visible = false
    end
end

local function removeTracer()
    if tracerLine then
        tracerLine:Remove()
        tracerLine = nil
    end
end

-- Hit Sound
local function playHitSound()
    local head = LocalPlayer.Character
        and LocalPlayer.Character:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}))
    if head then
        local sound = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8715,11655,12285,11550,10500}))
        sound.SoundId = hitSoundId
        sound.Volume = 1
        sound.Parent = head
        sound:Play()
        game.Debris:AddItem(sound, 2)
    end
end

-- Hit Chams Creation
local function createHitCham(targetChar)
    if
        not targetChar
        or not HitChamsEnabled
        or not targetChar:IsDescendantOf(workspace)
    then
        return
    end

    local chamModel = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8085,11655,10500,10605,11340}))
    chamModel.Name = ZuRdGvkgaGnppuwNtpwpGfeN({7560,11025,12180,7035,10920,10185,11445})
    chamModel.Parent = workspace

    for _, partName in ipairs(BodyParts) do
        local part = targetChar:FindFirstChild(partName)
        if part and part:IsA(ZuRdGvkgaGnppuwNtpwpGfeN({6930,10185,12075,10605,8400,10185,11970,12180})) then
            local chamPart = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8400,10185,11970,12180}))
            chamPart.Size = part.Size
            chamPart.CFrame = part.CFrame
            chamPart.Anchored = true
            chamPart.CanCollide = false
            chamPart.Material = HitChamMaterial
            chamPart.Color = HitChamColor
            chamPart.Transparency = 0.5
            chamPart.Parent = chamModel
        end
    end

    if #chamModel:GetChildren() == 0 then
        chamModel:Destroy()
        return
    end

    local startTime = tick()
    local conn
    conn = RunService.RenderStepped:Connect(function()
        local elapsed = tick() - startTime
        if elapsed >= ChamFadeTime then
            chamModel:Destroy()
            conn:Disconnect()
        else
            local t = elapsed / ChamFadeTime
            for _, p in ipairs(chamModel:GetChildren()) do
                if p:IsA(ZuRdGvkgaGnppuwNtpwpGfeN({6930,10185,12075,10605,8400,10185,11970,12180})) then
                    p.Transparency = 0.5 + 0.5 * t
                end
            end
        end
    end)
    dbgWarn(
        ZuRdGvkgaGnppuwNtpwpGfeN({7035,11970,10605,10185,12180,10605,10500,3360,7560,11025,12180,7035,10920,10185,11445,3360,10710,11655,11970,3360})
            .. (targetChar.Name or ZuRdGvkgaGnppuwNtpwpGfeN({12285,11550,11235,11550,11655,12495,11550}))
            .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,12495,11025,12180,10920,3360,11445,10185,12180,10605,11970,11025,10185,11340,6090,3360})
            .. tostring(HitChamMaterial)
    )
end

-- Fire Gun for Force Hit
local function fireGun()
    if not ForceHitEnabled or not ForceHitTarget then
        return
    end
    local char = LocalPlayer.Character
    local targetChar = ForceHitTarget.Character
    if not (char and targetChar) then
        return
    end
    local tool = char:FindFirstChildWhichIsA(ZuRdGvkgaGnppuwNtpwpGfeN({8820,11655,11655,11340}))
    if not tool or not tool:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10185,11550,10500,11340,10605})) then
        return
    end

    local handle = tool.Handle
    local muzzle = handle:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,12285,12810,12810,11340,10605}))
        or { WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position }
    local targetHead = targetChar:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}))
    if not targetHead or not muzzle.WorldPosition then
        return
    end

    local mainRemotes = ReplicatedStorage:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,12075}))
    local mainRemoteEvent = mainRemotes
        and mainRemotes:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
    if mainRemoteEvent then
        local offset = Vector3.new(
            math.random(-5, 5) / 100,
            math.random(-5, 5) / 100,
            math.random(-5, 5) / 100
        )
        mainRemoteEvent:FireServer(
            ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180,7455,12285,11550}),
            handle,
            muzzle.WorldPosition,
            targetHead.Position + offset,
            targetHead,
            Vector3.new(0, 0, 0)
        )
        lastShotTime = tick()
        playHitSound()
        if HitChamsEnabled then
            createHitCham(targetChar)
        end
    end
end

-- *** ORIGINAL Adonis Bypass (REMOVED - NEW ONE ABOVE HANDLES IT) ***

do
    local old
    old = hookmetamethod(game, ZuRdGvkgaGnppuwNtpwpGfeN({9975,9975,11550,10185,11445,10605,10395,10185,11340,11340}), function(self, ...)
        local method = getnamecallmethod()
        local args = { ... }
        if
            method == ZuRdGvkgaGnppuwNtpwpGfeN({7350,11025,11970,10605,8715,10605,11970,12390,10605,11970})
            and self.Name == ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})
            and args[1] == ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180})
            and BulletTPEnabled
            and not RapidFireEnabled
            and not TargetAll
            and Target
            and Target.Character
        then
            lastShotTime = tick()
            dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,12180,3360,10710,11025,11970,10605,10500,3360,11025,11550,3360,8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,11655,11550,3360}) .. Target.Name)
        end
        if
            method == ZuRdGvkgaGnppuwNtpwpGfeN({7350,11025,11970,10605,8715,10605,11970,12390,10605,11970})
            and self.Name == ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})
            and args[1] == ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180})
            and (RapidFireEnabled or TargetAll)
        then
            dbgWarn(
                ZuRdGvkgaGnppuwNtpwpGfeN({6930,11340,11655,10395,11235,10605,10500,3360,10500,10605,10710,10185,12285,11340,12180,3360,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180,6090,7350,11025,11970,10605,8715,10605,11970,12390,10605,11970,3360,10710,11655,11970,3360,8715,10920,11655,11655,12180,3360,12180,11655,3360,11760,11970,10605,12390,10605,11550,12180,3360,10500,12285,11760,11340,11025,10395,10185,12180,10605,3360,10710,11025,11970,11025,11550,10815})
            )
            return
        end
        if typeof(method) == ZuRdGvkgaGnppuwNtpwpGfeN({12075,12180,11970,11025,11550,10815}) and method:lower() == ZuRdGvkgaGnppuwNtpwpGfeN({11235,11025,10395,11235}) then
            notify(ZuRdGvkgaGnppuwNtpwpGfeN({6825,11550,12180,11025,4725,7875,11025,10395,11235}), ZuRdGvkgaGnppuwNtpwpGfeN({7875,11025,10395,11235,3360,10185,12180,12180,10605,11445,11760,12180,3360,10290,11340,11655,10395,11235,10605,10500}), 3)
            return nil
        end
        return old(self, ...)
    end)
end

local function GetSilentAimTarget()
    local MousePosition = UserInputService:GetMouseLocation()
    local closestTarget = nil
    local closestDistance = SilentAim.FOV.Radius

    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= LocalPlayer and Player.Character then
            local Character = Player.Character
            local HitPart = Character:FindFirstChild(SilentAim.HitPart)
            local Humanoid = Character:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500}))
            local BodyEffects = Character:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
            local KO = BodyEffects and BodyEffects:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7875,4830,8295}))

            if
                HitPart
                and Humanoid
                and Humanoid.Health > 0
                and BodyEffects
                and KO
                and KO.Value ~= true
            then
                local ScreenPosition, Visible =
                    CurrentCamera:WorldToScreenPoint(HitPart.Position)

                local Distance = (MousePosition - Vector2.new(
                    ScreenPosition.X,
                    ScreenPosition.Y
                )).Magnitude

                if Distance <= closestDistance then
                    closestDistance = Distance
                    closestTarget = HitPart
                    dbgWarn(
                        ZuRdGvkgaGnppuwNtpwpGfeN({8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,10395,11340,11655,12075,10605,12075,12180,3360,12180,10185,11970,10815,10605,12180,6090,3360})
                            .. Player.Name
                            .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,13020,3360,7140,11025,12075,12180,10185,11550,10395,10605,6090,3360})
                            .. math.floor(Distance)
                    )
                end
            end
        end
    end

    if closestTarget then
        dbgWarn(
            ZuRdGvkgaGnppuwNtpwpGfeN({8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12180,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360})
                .. closestTarget.Parent.Name
                .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,13020,3360,8400,10185,11970,12180,6090,3360})
                .. SilentAim.HitPart
        )
        return closestTarget
    end

    dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({8190,11655,3360,8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,7350,8295,9030,6090,3360}) .. SilentAim.FOV.Radius)
    return nil
end

local function setupHitDetection()
    local function connectHealthMonitor(player)
        if
            (player == Target or player == ForceHitTarget) and player.Character
        then
            local humanoid = player.Character:FindFirstChildOfClass(ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500}))
            if humanoid then
                local lastHealth = humanoid.Health
                local connection
                connection = humanoid.HealthChanged:Connect(function(newHealth)
                    if
                        HitChamsEnabled
                        and (BulletTPEnabled or ForceHitEnabled or SilentAim.Enabled)
                        and not RapidFireEnabled
                        and not TargetAll
                        and (player == Target or player == ForceHitTarget)
                        and newHealth < lastHealth
                        and (tick() - lastShotTime) <= HIT_DETECTION_WINDOW
                    then
                        createHitCham(player.Character)
                        notify(
                            ZuRdGvkgaGnppuwNtpwpGfeN({7560,11025,12180,3360,7035,10920,10185,11445,12075}),
                            ZuRdGvkgaGnppuwNtpwpGfeN({7560,11025,12180,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,11655,11550,3360}) .. player.Name,
                            3
                        )
                        dbgWarn(
                            ZuRdGvkgaGnppuwNtpwpGfeN({7560,11025,12180,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,11655,11550,3360})
                                .. player.Name
                                .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,12180,11970,11025,10815,10815,10605,11970,11025,11550,10815,3360,10395,10920,10185,11445,12075})
                        )
                    end
                    lastHealth = newHealth
                end)
                local cleanup
                cleanup = Players.PlayerRemoving:Connect(function(p)
                    if p == player then
                        connection:Disconnect()
                        cleanup:Disconnect()
                    end
                end)
                local targetChange
                targetChange = RunService.Heartbeat:Connect(function()
                    if Target ~= player and ForceHitTarget ~= player then
                        connection:Disconnect()
                        cleanup:Disconnect()
                        targetChange:Disconnect()
                    end
                end)
            end
        end
    end

    local lastTarget = nil
    local lastForceHitTarget = nil
    RunService.Heartbeat:Connect(function()
        if Target ~= lastTarget or ForceHitTarget ~= lastForceHitTarget then
            if Target then
                connectHealthMonitor(Target)
            end
            if ForceHitTarget then
                connectHealthMonitor(ForceHitTarget)
            end
            lastTarget = Target
            lastForceHitTarget = ForceHitTarget
        end
    end)
end

task.spawn(setupHitDetection)

pcall(function()
    setthreadidentity(7)
end)
notify(ZuRdGvkgaGnppuwNtpwpGfeN({6825,10500,11655,11550,11025,12075,3360,6930,12705,11760,10185,12075,12075}), ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,3360,11340,11655,10185,10500,10605,10500,3360,12075,12285,10395,10395,10605,12075,12075,10710,12285,11340,11340,12705}), 4)
dbgWarn(`Hooked {#h} Adonis functions`)

