-- Notification Function
local function notify(title, text, dur)
	if SEND_NOTIFICATIONS then
		pcall(function()
			StarterGui:SetCore(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10605,11550,10500,8190,11655,12180,11025,10710,11025,10395,10185,12180,11025,11655,11550}), {
				Title = title,
				Text = text,
				Duration = dur or 3,
			})
		end)
	end
end

-- Debug Function
local function dbgWarn(msg)
	if DEBUG_MODE then
		warn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9555,7140,7245,6930,8925,7455,9765,3360}) .. tostring(msg))
	end
end

-- Adonis Bypass
local HookedFunctions = {}
local DetectedFunc, KillFunc

pcall(function()
	setthreadidentity(2)
end)

for _, obj in ipairs(getgc(true)) do
	if typeof(obj) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({12180,10185,10290,11340,10605}) then
		local detect = rawget(obj, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7140,10605,12180,10605,10395,12180,10605,10500}))
		local kill = rawget(obj, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7875,11025,11340,11340}))
		if typeof(detect) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,12285,11550,10395,12180,11025,11655,11550}) and not DetectedFunc then
			DetectedFunc = detect
			local old
			old = hookfunction(detect, function(action, info, noCrash)
				if action ~= YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9975}) then
					dbgWarn(
						`Adonis flag blocked | Method: {action} | Info: {info}`
					)
				end
				return true
			end)
			table.insert(HookedFunctions, DetectedFunc)
		end
		if
			rawget(obj, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9030,10185,11970,11025,10185,10290,11340,10605,12075}))
			and rawget(obj, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8400,11970,11655,10395,10605,12075,12075}))
			and typeof(kill) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,12285,11550,10395,12180,11025,11655,11550})
			and not KillFunc
		then
			KillFunc = kill
			local old
			old = hookfunction(kill, function(info)
				dbgWarn(`Adonis kill attempt blocked | Info: {info}`)
			end)
			table.insert(HookedFunctions, KillFunc)
		end
		local indexInstance = rawget(obj, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11025,11550,10500,10605,12600,7665,11550,12075,12180,10185,11550,10395,10605}))
		if typeof(indexInstance) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({12180,10185,10290,11340,10605}) and indexInstance[1] == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11235,11025,10395,11235}) then
			setreadonly(obj, false)
			setreadonly(indexInstance, false)
			rawset(obj, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,10290,11340,10605}), {
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11235,11025,10395,11235}),
				function()
					coroutine.yield()
				end,
			})
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,12285,12180,10185,12180,10605,10500,3360,11025,11550,10500,10605,12600,7665,11550,12075,12180,10185,11550,10395,10605,3360,12180,11655,3360,10290,11340,11655,10395,11235,3360,11235,11025,10395,11235,3360,10395,11655,11445,11445,12285,11550,11025,10395,10185,12180,11025,11655,11550}))
		end
	end
end

do
	local old
	old = hookfunction(
		getrenv().debug.info,
		newcclosure(function(...)
			local a, b = ...
			if DetectedFunc and a == DetectedFunc then
				dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6930,11340,11655,10395,11235,10605,10500,3360,6825,10500,11655,11550,11025,12075,3360,10500,10605,10290,12285,10815,4830,11025,11550,10710,11655,3360,12075,10185,11550,11025,12180,12705,3360,10395,10920,10605,10395,11235}))
				return coroutine.yield(coroutine.running())
			end
			return old(...)
		end)
	)
end

do
	local old
	old = hookmetamethod(game, YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9975,9975,11550,10185,11445,10605,10395,10185,11340,11340}), function(self, ...)
		local method = getnamecallmethod()
		if typeof(method) == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({12075,12180,11970,11025,11550,10815}) and method:lower() == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11235,11025,10395,11235}) then
			notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,11550,12180,11025,4725,7875,11025,10395,11235}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7875,11025,10395,11235,3360,10185,12180,12180,10605,11445,11760,12180,3360,10290,11340,11655,10395,11235,10605,10500}), 3)
			return nil
		end
		return old(self, ...)
	end)
end

pcall(function()
	setthreadidentity(7)
end)
notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,10500,11655,11550,11025,12075,3360,6930,12705,11760,10185,12075,12075}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10395,11970,11025,11760,12180,3360,11340,11655,10185,10500,10605,10500,3360,12075,12285,10395,10395,10605,12075,12075,10710,12285,11340,11340,12705}), 4)
dbgWarn(`Hooked {#HookedFunctions} Adonis functions`)

-- Utility Functions
local function findPlayer(name)
	name = name and name:lower()
	if not name then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,7245,11445,11760,12180,12705,3360,11550,10185,11445,10605,3360,11760,11970,11655,12390,11025,10500,10605,10500}))
		return nil
	end
	for _, plr in ipairs(Players:GetPlayers()) do
		if
			plr.Name:lower():find(name) or plr.DisplayName:lower():find(name)
		then
			dbgWarn(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,7350,11655,12285,11550,10500,3360,11760,11340,10185,12705,10605,11970,3360}) .. plr.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10710,11655,11970,3360,11025,11550,11760,12285,12180,3360}) .. name
			)
			return plr
		end
	end
	dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,11760,11340,10185,12705,10605,11970,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360,11025,11550,11760,12285,12180,3360}) .. name)
	return nil
end

-- Process Assist Input (called on text change)
local lastProcessedInput = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
local function processAssistInput(value)
	if value == YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({}) then
		notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,12075,12075,11025,12075,12180}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({9345,11655,12285,3360,11445,12285,12075,12180,3360,11760,11970,11655,12390,11025,10500,10605,3360,10185,3360,11760,11340,10185,12705,10605,11970,3360,11550,10185,11445,10605}), 3)
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11760,11970,11655,10395,10605,12075,12075,6825,12075,12075,11025,12075,12180,7665,11550,11760,12285,12180,6090,3360,7245,11445,11760,12180,12705,3360,11025,11550,11760,12285,12180}))
		return false
	end
	if value == lastProcessedInput then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11760,11970,11655,10395,10605,12075,12075,6825,12075,12075,11025,12075,12180,7665,11550,11760,12285,12180,6090,3360,7665,11550,11760,12285,12180,3360,12285,11550,10395,10920,10185,11550,10815,10605,10500,4620,3360,12075,11235,11025,11760,11760,11025,11550,10815,6090,3360}) .. value)
		return false
	end
	local target = findPlayer(value)
	if not target then
		notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,12075,12075,11025,12075,12180}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8400,11340,10185,12705,10605,11970,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,6090,3360}) .. value, 3)
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11760,11970,11655,10395,10605,12075,12075,6825,12075,12075,11025,12075,12180,7665,11550,11760,12285,12180,6090,3360,8400,11340,10185,12705,10605,11970,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360,11025,11550,11760,12285,12180,6090,3360}) .. value)
		return false
	end
	local userId = target.UserId
	-- Check if player is already in the list
	for _, id in ipairs(AssistOthers.AllowedPlayers) do
		if id == userId then
			notify(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,12075,12075,11025,12075,12180}),
				target.DisplayName .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,11025,12075,3360,10185,11340,11970,10605,10185,10500,12705,3360,10290,10605,11025,11550,10815,3360,10185,12075,12075,11025,12075,12180,10605,10500}),
				3
			)
			dbgWarn(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11760,11970,11655,10395,10605,12075,12075,6825,12075,12075,11025,12075,12180,7665,11550,11760,12285,12180,6090,3360,8400,11340,10185,12705,10605,11970,3360,10185,11340,11970,10605,10185,10500,12705,3360,10185,12075,12075,11025,12075,12180,10605,10500,6090,3360})
					.. target.DisplayName
			)
			return false
		end
	end
	-- Add player to assist list and enable assisting
	table.insert(AssistOthers.AllowedPlayers, userId)
	AssistOthers.Enabled = true
	lastProcessedInput = value
	notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,12075,12075,11025,12075,12180}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8190,11655,12495,3360,10185,12075,12075,11025,12075,12180,11025,11550,10815,3360}) .. target.DisplayName, 3)
	dbgWarn(
		YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11760,11970,11655,10395,10605,12075,12075,6825,12075,12075,11025,12075,12180,7665,11550,11760,12285,12180,6090,3360,6825,12075,12075,11025,12075,12180,3360,10605,11550,10185,10290,11340,10605,10500,3360,10710,11655,11970,3360})
			.. target.DisplayName
			.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,4200,8925,12075,10605,11970,7665,10500,6090,3360})
			.. userId
			.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4305})
	)
	return true
end

