		end
		local Module = require(Handler)
		Module.Ignored = {
			workspace:WaitForChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################','#####################################################################################################','########################################################################################################','#########################################################################################################','###################################################################################################','############################################################################################################','#####################################################################################################','###################################################################################################################'})),
