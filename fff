        end
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({7770,10605,12075,12285,12075,3360,7035,11970,11655,12075,12075,3360,11445,10185,12180,10605,11970,11025,10185,11340,3360,12075,10605,12180,3360,12180,11655,6090,3360}) .. value)
