-- Target Lock Keybind
TargetAimSection:AddButton({
	Name = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10605,12180,3360,8820,10185,11970,10815,10605,12180,3360,7980,11655,10395,11235,3360,7875,10605,12705,3360,4200,8400,11970,10605,12075,12075,3360,10185,3360,11235,10605,12705,3360,10185,10710,12180,10605,11970,3360,10395,11340,11025,10395,11235,11025,11550,10815,4305}),
	Callback = function()
		local listening = true
		notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,7980,11655,10395,11235,3360,7875,10605,12705}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8400,11970,10605,12075,12075,3360,10185,3360,11235,10605,12705,3360,12180,11655,3360,12075,10605,12180,4830,4830,4830}), 5)
		local connection
		connection = UserInputService.InputBegan:Connect(
			function(input, processed)
				if
					processed
					or input.UserInputType ~= Enum.UserInputType.Keyboard
					or not listening
				then
					return
				end
				listening = false
				Keybinds.TargetLock = input.KeyCode
				TargetLockKeyLabel.Text = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,7980,11655,10395,11235,3360,7875,10605,12705,6090,3360})
					.. input.KeyCode.Name
				dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,3360,11235,10605,12705,3360,12075,10605,12180,3360,12180,11655,6090,3360}) .. input.KeyCode.Name)
				connection:Disconnect()
			end
		)
	end,
})

