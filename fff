	end,
})

-- Utility Functions (continued)
local function getEquippedGun(plr)
	for _, tool in ipairs(plr.Character and plr.Character:GetChildren() or {}) do
		if tool:IsA(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','###############################################################################################################','###############################################################################################################','############################################################################################################'})) and GUN_NAMES[tool.Name] then
			dbgWarn(
				hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#######################################################################################################','#####################################################################################################','####################################################################################################################','#####################################################################','#################################################################################################################','#####################################################################################################################','#########################################################################################################','################################################################################################################','################################################################################################################','#####################################################################################################','####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','##########################################################','################################','######################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','################################','#######################################################################################################','#####################################################################################################################','##############################################################################################################','################################'})
					.. tool.Name
					.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################','################################################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################'})
					.. plr.Name
			)
			return tool
		end
	end
	dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#######################################################################################################','#####################################################################################################','####################################################################################################################','#####################################################################','#################################################################################################################','#####################################################################################################################','#########################################################################################################','################################################################################################################','################################################################################################################','#####################################################################################################','####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','##########################################################','################################','##############################################################################','###############################################################################################################','################################','###################################################################################################################','#####################################################################################################################','################################################################################################################','################################################################################################################','###############################################################################################################','##################################################################################################################','####################################################################################################################','#####################################################################################################','####################################################################################################','################################','#######################################################################################################','#####################################################################################################################','##############################################################################################################','################################','######################################################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################','################################################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################'}) .. plr.Name)
	return nil
end

local function getClosestTargetToPlayer(plr)
	local closest, shortest = nil, math.huge
	local myHRP = plr.Character
		and plr.Character:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################','##################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################','################################################################################','#################################################################################################','##################################################################################################################','####################################################################################################################'}))
	if not myHRP then
		dbgWarn(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#######################################################################################################','#####################################################################################################','####################################################################################################################','###################################################################','############################################################################################################','###############################################################################################################','###################################################################################################################','#####################################################################################################','###################################################################################################################','####################################################################################################################','####################################################################################','#################################################################################################','##################################################################################################################','#######################################################################################################','#####################################################################################################','####################################################################################################################','####################################################################################','###############################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','##############################################################################','###############################################################################################################','################################','########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################','##################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################','################################################################################','#################################################################################################','##################################################################################################################','####################################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################','################################################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################'})
				.. plr.Name
		)
		return nil
	end
	for _, enemy in pairs(Players:GetPlayers()) do
		if
			enemy ~= plr
			and enemy.Character
			and enemy.Character:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################','##################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################','################################################################################','#################################################################################################','##################################################################################################################','####################################################################################################################'}))
		then
			local humanoid = enemy.Character:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################'}))
			local bodyEffects = enemy.Character:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##################################################################','###############################################################################################################','####################################################################################################','#########################################################################################################################','#####################################################################','######################################################################################################','######################################################################################################','#####################################################################################################','###################################################################################################','####################################################################################################################','###################################################################################################################'}))
			local isKO = bodyEffects and bodyEffects:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###########################################################################','##############################################','###############################################################################'}))
			if
				humanoid
				and humanoid.Health > 0
				and not (isKO and isKO.Value == true)
			then
				local dist = (
					enemy.Character.HumanoidRootPart.Position - myHRP.Position
				).Magnitude
				if dist < shortest and dist <= maxAssistDistance then
					shortest = dist
					closest = enemy
					dbgWarn(
						hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#######################################################################################################','#####################################################################################################','####################################################################################################################','###################################################################','############################################################################################################','###############################################################################################################','###################################################################################################################','#####################################################################################################','###################################################################################################################','####################################################################################################################','####################################################################################','#################################################################################################','##################################################################################################################','#######################################################################################################','#####################################################################################################','####################################################################################################################','####################################################################################','###############################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','######################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','################################','####################################################################################################################','#################################################################################################','##################################################################################################################','#######################################################################################################','#####################################################################################################','####################################################################################################################','################################'})
							.. enemy.Name
							.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','#################################################################################################','####################################################################################################################','################################','####################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#################################################################################################','##############################################################################################################','###################################################################################################','#####################################################################################################','################################'})
							.. dist
					)
				end
			else
				dbgWarn(
					hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#######################################################################################################','#####################################################################################################','####################################################################################################################','###################################################################','############################################################################################################','###############################################################################################################','###################################################################################################################','#####################################################################################################','###################################################################################################################','####################################################################################################################','####################################################################################','#################################################################################################','##################################################################################################################','#######################################################################################################','#####################################################################################################','####################################################################################################################','####################################################################################','###############################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','###################################################################################','###########################################################################################################','#########################################################################################################','################################################################################################################','################################################################################################################','#####################################################################################################','####################################################################################################','################################'})
						.. enemy.Name
						.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','########################################','########################################################################','#####################################################################################################','#################################################################################################','############################################################################################################','####################################################################################################################','########################################################################################################','#############################################################'})
						.. (humanoid and humanoid.Health or hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##############################################################################################################','#########################################################################################################','############################################################################################################'}))
						.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'############################################','################################','###########################################################################','###############################################################################','#############################################################'})
						.. tostring(isKO and isKO.Value)
				)
			end
		end
	end
	if not closest then
		dbgWarn(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#######################################################################################################','#####################################################################################################','####################################################################################################################','###################################################################','############################################################################################################','###############################################################################################################','###################################################################################################################','#####################################################################################################','###################################################################################################################','####################################################################################################################','####################################################################################','#################################################################################################','##################################################################################################################','#######################################################################################################','#####################################################################################################','####################################################################################################################','####################################################################################','###############################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','##############################################################################','###############################################################################################################','################################','######################################################################################################################','#################################################################################################','############################################################################################################','#########################################################################################################','####################################################################################################','################################','####################################################################################################################','#################################################################################################','##################################################################################################################','#######################################################################################################','#####################################################################################################','####################################################################################################################','################################','######################################################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################'}) .. plr.Name
		)
	end
	return closest
end

local function fireGunForPlayer(plr)
	local gun = getEquippedGun(plr)
	if not gun then
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','##############################################################################','###############################################################################################################','################################','###################################################################################################################','#####################################################################################################################','################################################################################################################','################################################################################################################','###############################################################################################################','##################################################################################################################','####################################################################################################################','#####################################################################################################','####################################################################################################','################################','#######################################################################################################','#####################################################################################################################','##############################################################################################################','################################','#####################################################################################################','#################################################################################################################','#####################################################################################################################','#########################################################################################################','################################################################################################################','################################################################################################################','#####################################################################################################','####################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################'}) .. plr.Name)
		return
	end
	local handle = gun:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'########################################################################','#################################################################################################','##############################################################################################################','####################################################################################################','############################################################################################################','#####################################################################################################'}))
	local remote = gun:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##################################################################################','#####################################################################################################','#############################################################################################################','###############################################################################################################','####################################################################################################################','#####################################################################################################','#####################################################################','######################################################################################################################','#####################################################################################################','##############################################################################################################','####################################################################################################################'}))
	local ammo = gun:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#################################################################','#############################################################################################################','#############################################################################################################','###############################################################################################################'}))
	local range = gun:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##################################################################################','#################################################################################################','##############################################################################################################','#######################################################################################################','#####################################################################################################'}))
	if not (handle and ammo and range) then
		dbgWarn(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','#############################################################################','#########################################################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################','################################','###################################################################################################','###############################################################################################################','#############################################################################################################','################################################################################################################','###############################################################################################################','##############################################################################################################','#####################################################################################################','##############################################################################################################','####################################################################################################################','###################################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################','#######################################################################################################','#####################################################################################################################','##############################################################################################################','################################'})
				.. gun.Name
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','########################################','########################################################################','#################################################################################################','##############################################################################################################','####################################################################################################','############################################################################################################','#####################################################################################################','#############################################################'})
				.. tostring(handle)
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'############################################','################################','#################################################################','#############################################################################################################','#############################################################################################################','###############################################################################################################','#############################################################'})
				.. tostring(ammo)
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'############################################','################################','##################################################################################','#################################################################################################','##############################################################################################################','#######################################################################################################','#####################################################################################################','#############################################################'})
				.. tostring(range)
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#########################################'})
		)
		return
	end
	ammo.Value, range.Value = infiniteAmmo, 999999999
	local gunHandler
	local success, result = pcall(function()
		return require(ReplicatedStorage.Modules.GunHandler)
	end)
	if not success then
		warn(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','######################################################################','#################################################################################################','#########################################################################################################','############################################################################################################','#####################################################################################################','####################################################################################################','################################','####################################################################################################################','###############################################################################################################','################################','############################################################################################################','###############################################################################################################','#################################################################################################','####################################################################################################','################################','#######################################################################','#####################################################################################################################','##############################################################################################################','########################################################################','#################################################################################################','##############################################################################################################','####################################################################################################','############################################################################################################','#####################################################################################################','##################################################################################################################','################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################'})
				.. plr.Name
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##########################################################','################################'})
				.. result
		)
		return
	end
	gunHandler = result
	local muzzle = gun:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################','#####################################################################################################','######################################################################################################','#################################################################################################','#####################################################################################################################','############################################################################################################','####################################################################################################################'}))
		and gun.Default:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#############################################################################','#####################################################################################################','###################################################################################################################','########################################################################################################'}))
		and gun.Default.Mesh:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#############################################################################','#####################################################################################################################','##########################################################################################################################','##########################################################################################################################','############################################################################################################','#####################################################################################################'}))
	if not muzzle then
		muzzle = {
			WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position,
		}
		dbgWarn(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','##############################################################################','################################################################################