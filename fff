		else
			Text.Visible = false
			notify(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','**************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','*******************************************************************************************************************'}), vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************','*******************************************************************************','**************************************************************************************','********************************','***************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************','****************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}), 3)
		end
