-- Notification Function
local function notify(title, text, dur)
    if SEND_NOTIFICATIONS then
        pcall(function()
            StarterGui:SetCore(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','*****************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','******************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}), {
                Title = title,
                Text = text,
                Duration = dur or 3,
            })
        end)
    end
end

-- Debug Function
local function dbgWarn(msg)
    if DEBUG_MODE then
        warn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*******************************************************************************************','********************************************************************','*********************************************************************','******************************************************************','*************************************************************************************','***********************************************************************','*********************************************************************************************','********************************'}) .. tostring(msg))
    end
end

-- Tracer Helper for Force Hit
local function createTracer()
    if tracerLine then
        tracerLine:Remove()
    end
    tracerLine = Drawing.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'****************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************'}))
    tracerLine.Color = Color3.new(1, 0, 0)
    tracerLine.Thickness = 2
    tracerLine.Visible = false
end

local function updateTracer()
    if not (ForceHitEnabled and ForceHitTarget and tracerLine) then
        if tracerLine then
            tracerLine.Visible = false
        end
        return
    end

    local localChar = LocalPlayer.Character
    local targetChar = ForceHitTarget.Character
    if not localChar or not targetChar then
        tracerLine.Visible = false
        return
    end

    local cam = workspace.CurrentCamera
    local localHead = localChar:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'}))
    local targetHead = targetChar:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'}))

    if localHead and targetHead then
        local fromPos, onScreen1 = cam:WorldToViewportPoint(localHead.Position)
        local toPos, onScreen2 = cam:WorldToViewportPoint(targetHead.Position)
        if onScreen1 and onScreen2 then
            tracerLine.From = Vector2.new(fromPos.X, fromPos.Y)
            tracerLine.To = Vector2.new(toPos.X, toPos.Y)
            tracerLine.Visible = true
        else
            tracerLine.Visible = false
        end
    else
        tracerLine.Visible = false
    end
end

local function removeTracer()
    if tracerLine then
        tracerLine:Remove()
        tracerLine = nil
    end
end

-- Hit Sound
local function playHitSound()
    local head = LocalPlayer.Character
        and LocalPlayer.Character:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'}))
    if head then
        local sound = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************'}))
        sound.SoundId = hitSoundId
        sound.Volume = 1
        sound.Parent = head
        sound:Play()
        game.Debris:AddItem(sound, 2)
    end
end

-- Hit Chams Creation
local function createHitCham(targetChar)
    if not targetChar or not HitChamsEnabled or not targetChar:IsDescendantOf(workspace) then
        return
    end

    local chamModel = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'}))
    chamModel.Name = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*******************************************************************','********************************************************************************************************','*************************************************************************************************','*************************************************************************************************************'})
    chamModel.Parent = workspace

    for _, partName in ipairs(BodyParts) do
        local part = targetChar:FindFirstChild(partName)
        if part and part:IsA(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'})) then
            local chamPart = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
            chamPart.Size = part.Size
            chamPart.CFrame = part.CFrame
            chamPart.Anchored = true
            chamPart.CanCollide = false
            chamPart.Material = HitChamMaterial
            chamPart.Color = HitChamColor
            chamPart.Transparency = 0.5
            chamPart.Parent = chamModel
        end
    end

    if #chamModel:GetChildren() == 0 then
        chamModel:Destroy()
        return
    end

    local startTime = tick()
    local conn
    conn = RunService.RenderStepped:Connect(function()
        local elapsed = tick() - startTime
        if elapsed >= ChamFadeTime then
            chamModel:Destroy()
            conn:Disconnect()
        else
            local t = elapsed / ChamFadeTime
            for _, p in ipairs(chamModel:GetChildren()) do
                if p:IsA(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'})) then
                    p.Transparency = 0.5 + 0.5 * t
                end
            end
        end
    end)
    dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*******************************************************************','******************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*******************************************************************','********************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. (targetChar.Name or aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*********************************************************************************************************************','**************************************************************************************************************','***********************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'})) .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************','***********************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','********************************************************************************************************','********************************','*************************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','************************************************************************************************************','**********************************************************','********************************'}) .. tostring(HitChamMaterial))
end

-- Fire Gun for Force Hit
local function fireGun()
    if not ForceHitEnabled or not ForceHitTarget then
        return
    end
    local char = LocalPlayer.Character
    local targetChar = ForceHitTarget.Character
    if not (char and targetChar) then
        return
    end
    local tool = char:FindFirstChildWhichIsA(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
    if not tool or not tool:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'})) then
        return
    end

    local handle = tool.Handle
    local muzzle = handle:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
        or { WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position }
    local targetHead = targetChar:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'}))
    if not targetHead or not muzzle.WorldPosition then
        return
    end

    local mainRemotes = ReplicatedStorage:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'}))
    local mainRemoteEvent = mainRemotes
        and mainRemotes:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'}))
    if mainRemoteEvent then
        local offset = Vector3.new(
            math.random(-5, 5) / 100,
            math.random(-5, 5) / 100,
            math.random(-5, 5) / 100
        )
        mainRemoteEvent:FireServer(
            aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
            handle,
            muzzle.WorldPosition,
            targetHead.Position + offset,
            targetHead,
            Vector3.new(0, 0, 0)
        )
        lastShotTime = tick()
        playHitSound()
        if HitChamsEnabled then
            createHitCham(targetChar)
        end
    end
end

-- Adonis Bypass
local HookedFunctions = {}
local DetectedFunc, KillFunc

pcall(function()
    setthreadidentity(2)
end)

for _, obj in ipairs(getgc(true)) do
    if typeof(obj) == aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) then
        local detect = rawget(obj, aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************************************','*****************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
        local kill = rawget(obj, aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***************************************************************************','*********************************************************************************************************','************************************************************************************************************','************************************************************************************************************'}))
        if typeof(detect) == aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}) and not DetectedFunc then
            DetectedFunc = detect
            local old
            old = hookfunction(detect, function(action, info, noCrash)
                if action ~= aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************************'}) then
                    dbgWarn(
                        `Adonis flag blocked | Method: {action} | Info: {info}`
                    )
                end
                return true
            end)
            table.insert(HookedFunctions, DetectedFunc)
        end
        if
            rawget(obj, aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'**************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'}))
            and rawget(obj, aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************'}))
            and typeof(kill) == aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'})
            and not KillFunc
        then
            KillFunc = kill
            local old
            old = hookfunction(kill, function(info)
                dbgWarn(`Adonis kill attempt blocked | Info: {info}`)
            end)
            table.insert(HookedFunctions, KillFunc)
        end
        local indexInstance = rawget(obj, aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','*************************************************************************','**************************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************'}))
        if typeof(indexInstance) == aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) and indexInstance[1] == aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','***********************************************************************************************************'}) then
            setreadonly(obj, false)
            setreadonly(indexInstance, false)
            rawset(obj, aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}), {
                aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','***********************************************************************************************************'}),
                function()
                    coroutine.yield()
