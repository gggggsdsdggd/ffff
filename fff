local labelOffsetY = 0 -- Track vertical offset for stacking labels
local function CreateLabel(section, text)
	local label = Instance.new(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','********************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'}))
	label.Text = text
	label.TextSize = 15
	label.TextColor3 = Color3.new(1, 1, 1)
	label.BackgroundTransparency = 1
	label.Size = UDim2.new(1, 0, 0, 20)
	label.Position = UDim2.new(0, 0, 0, labelOffsetY)
	label.Parent = ScreenGui -- Parent to ScreenGui instead of section
	labelOffsetY = labelOffsetY + 25 -- Increment offset for next label
	return label
end

