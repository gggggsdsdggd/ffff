        3
    )
    dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************'}) .. tool.Name .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************'}))
    return maid
end

-- Auto Patch New Guns
Client.Backpack.ChildAdded:Connect(function(child)
    if GUN_NAMES[child.Name] then
        task.wait(0.1)
        patchGun(child)
    end
end)

-- Patch Existing Gun
local equippedGun = Client.Character
    and Client.Character:FindFirstChildOfClass(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
if equippedGun and GUN_NAMES[equippedGun.Name] then
    patchGun(equippedGun)
end

-- Shared Script Label
local MAIN_USERID = 3164536831

local scriptUserFlag = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************','**************************************************************************************','*************************************************************************************************','************************************************************************************************************','*********************************************************************************************************************','*****************************************************************************************************'}))
scriptUserFlag.Name = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})
scriptUserFlag.Value = true
scriptUserFlag.Parent = LocalPlayer

local function addLabel(player)
    if
        LocalPlayer.UserId == MAIN_USERID
        and player ~= LocalPlayer
        and player:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
        and player.Character
        and player.Character:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) then
            return
        end
        local billboard = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','*********************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','***************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','*********************************************************************************************************'}))
        billboard.Name = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','********************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','************************************************************************************************************','********************************************************************************************************************','********************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    elseif
        LocalPlayer.UserId ~= MAIN_USERID
        and LocalPlayer:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','***************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
        and player.UserId == MAIN_USERID
        and player.Character
        and player.Character:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************'}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*******************************************************************************','***********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})) then
            return
        end
        local billboard = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','*********************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','***************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','*********************************************************************************************************'}))
        billboard.Name = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*******************************************************************************','***********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','********************************************************************************************************************','****************************************************************************','*************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','************************************************************************************************************'}))
