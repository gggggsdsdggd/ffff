local HookedFunctions = {}
local DetectedFunc, KillFunc

pcall(function()
	setthreadidentity(2)
end)

for _, obj in ipairs(getgc(true)) do
	if typeof(obj) == hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################################################','#################################################################################################','##################################################################################################','############################################################################################################','#####################################################################################################'}) then
		local detect = rawget(obj, hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################','#####################################################################################################','####################################################################################################################','#####################################################################################################','###################################################################################################','####################################################################################################################','#####################################################################################################','####################################################################################################'}))
		local kill = rawget(obj, hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###########################################################################','#########################################################################################################','############################################################################################################','############################################################################################################'}))
		if typeof(detect) == hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#####################################################################################################################','##############################################################################################################','###################################################################################################','####################################################################################################################','#########################################################################################################','###############################################################################################################','##############################################################################################################'}) and not DetectedFunc then
			DetectedFunc = detect
			local old
			old = hookfunction(detect, function(action, info, noCrash)
				if action ~= hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###############################################################################################'}) then
					dbgWarn(
						`Adonis flag blocked | Method: {action} | Info: {info}`
					)
				end
				return true
			end)
			table.insert(HookedFunctions, DetectedFunc)
		end
		if
			rawget(obj, hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################','#################################################################################################','##################################################################################################################','#########################################################################################################','#################################################################################################','##################################################################################################','############################################################################################################','#####################################################################################################','###################################################################################################################'}))
			and rawget(obj, hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################','##################################################################################################################','###############################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################'}))
			and typeof(kill) == hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#####################################################################################################################','##############################################################################################################','###################################################################################################','####################################################################################################################','#########################################################################################################','###############################################################################################################','##############################################################################################################'})
			and not KillFunc
		then
			KillFunc = kill
			local old
			old = hookfunction(kill, function(info)
				dbgWarn(`Adonis kill attempt blocked | Info: {info}`)
			end)
			table.insert(HookedFunctions, KillFunc)
		end
		local indexInstance = rawget(obj, hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#########################################################################################################','##############################################################################################################','####################################################################################################','#####################################################################################################','########################################################################################################################','#########################################################################','##############################################################################################################','###################################################################################################################','####################################################################################################################','#################################################################################################','##############################################################################################################','###################################################################################################','#####################################################################################################'}))
