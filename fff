					dbgWarn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'******************************************************************************','***************************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************'}))
				end
			else
				notify(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}), cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'*********************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','********************************','*************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}), 3)
				dbgWarn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','