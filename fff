	BeamConnections,
	workspace.DescendantAdded:Connect(function(descendant)
		if not BeamVisualsEnabled or not descendant:IsA(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'******************************************************************','*****************************************************************************************************','*************************************************************************************************','*************************************************************************************************************'})) then
