-- Notification Function
local function notify(title, text, dur)
	if SEND_NOTIFICATIONS then
		pcall(function()
			StarterGui:SetCore(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***********************************************************************************','*****************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','******************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}), {
				Title = title,
				Text = text,
				Duration = dur or 3,
			})
		end)
	end
end

-- Debug Function
local function dbgWarn(msg)
	if DEBUG_MODE then
		warn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'*******************************************************************************************','********************************************************************','*********************************************************************','******************************************************************','*************************************************************************************','***********************************************************************','*********************************************************************************************','********************************'}) .. tostring(msg))
	end
end

-- Adonis Bypass
local HookedFunctions = {}
local DetectedFunc, KillFunc

pcall(function()
	setthreadidentity(2)
end)

for _, obj in ipairs(getgc(true)) do
	if typeof(obj) == cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) then
		local detect = rawget(obj, cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************************************************','*****************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
		local kill = rawget(obj, cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***************************************************************************','*********************************************************************************************************','************************************************************************************************************','************************************************************************************************************'}))
		if typeof(detect) == cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}) and not DetectedFunc then
			DetectedFunc = detect
			local old
			old = hookfunction(detect, function(action, info, noCrash)
				if action ~= cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***********************************************************************************************'}) then
					dbgWarn(
						`Adonis flag blocked | Method: {action} | Info: {info}`
					)
				end
				return true
			end)
			table.insert(HookedFunctions, DetectedFunc)
		end
		if
			rawget(obj, cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'**************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'}))
			and rawget(obj, cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************'}))
			and typeof(kill) == cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'})
			and not KillFunc
		then
			KillFunc = kill
			local old
			old = hookfunction(kill, function(info)
				dbgWarn(`Adonis kill attempt blocked | Info: {info}`)
			end)
			table.insert(HookedFunctions, KillFunc)
		end
		local indexInstance = rawget(obj, cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','*************************************************************************','**************************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************'}))
		if typeof(indexInstance) == cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) and indexInstance[1] == cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','***********************************************************************************************************'}) then
			setreadonly(obj, false)
			setreadonly(indexInstance, false)
			rawset(obj, cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}), {
