***************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
		end
	end)
	applyWallbang()
	hookGunHandlerShoot()
	dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*********************************************','****************************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************************'}))
end)
    
