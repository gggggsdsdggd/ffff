    elseif LocalPlayer.UserId ~= MAIN_USERID and LocalPlayer:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) and player.UserId == MAIN_USERID and player.Character and player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,10605,10185,10500})) then
