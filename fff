local Circle = Drawing.new(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************','*********************************************************************************************************','******************************************************************************************************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
Circle.Filled = false
local Text = Drawing.new(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','********************************************************************************************************************'}))

-- Notification Function
local function notify(title, text, dur)
	if SEND_NOTIFICATIONS then
		pcall(function()
			StarterGui:SetCore(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','*****************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','******************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}), {
				Title = title,
				Text = text,
				Duration = dur or 3,
			})
		end)
	end
end

-- Debug Function
local function dbgWarn(msg)
	if DEBUG_MODE then
		warn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************','********************************************************************','*********************************************************************','******************************************************************','*************************************************************************************','***********************************************************************','*********************************************************************************************','********************************'}) .. tostring(msg))
	end
end

-- Adonis Bypass
local getinfo = getinfo or debug.getinfo
local DEBUG = false
local Hooked = {}

local Detected, Kill

setthreadidentity(2)

for i, v in getgc(true) do
    if typeof(v) == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) then
        local DetectFunc = rawget(v, dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************','*****************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
        local KillFunc = rawget(v, dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***************************************************************************','*********************************************************************************************************','************************************************************************************************************','************************************************************************************************************'}))
    
        if typeof(DetectFunc) == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}) and not Detected then
            Detected = DetectFunc
            
            local Old; Old = hookfunction(Detected, function(Action, Info, NoCrash)
                if Action ~= dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************************'}) then
                    if DEBUG then
                        warn(`Adonis AntiCheat flagged\nMethod: {Action}\nInfo: {Info}`)
                    end
                end
                
                return true
            end)

            table.insert(Hooked, Detected)
        end

        if rawget(v, dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'})) and rawget(v, dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************'})) and typeof(KillFunc) == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}) and not Kill then
            Kill = KillFunc
            local Old; Old = hookfunction(Kill, function(Info)
                if DEBUG then
                    warn(`Adonis AntiCheat tried to kill (fallback): {Info}`)
                end
            end)

            table.insert(Hooked, Kill)
        end
    end
end

local Old; Old = hookfunction(getrenv().debug.info, newcclosure(function(...)
    local LevelOrFunc, Info = ...

    if Detected and LevelOrFunc == Detected then
        if DEBUG then
            warn(`zins | adonis bypassed`)
        end

        return coroutine.yield(coroutine.running())
    end
    
    return Old(...)
end))
-- setthreadidentity(9)
setthreadidentity(7)
-- Utility Functions
local function findPlayer(name)
	name = name and name:lower()
	if not name then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','*********************************************************************','*************************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************************************','********************************','**************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','********************************','****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','**********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
		return nil
	end
	for _, plr in ipairs(Players:GetPlayers()) do
		if
			plr.Name:lower():find(name) or plr.DisplayName:lower():find(name)
		then
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','**********************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************'}) .. plr.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','********************************'}) .. name
			)
			return plr
		end
	end
	dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','********************************'}) .. name)
	return nil
end

-- Process Assist Input (called on text change)
local lastProcessedInput = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
local function processAssistInput(value)
	if value == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({}) then
		notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','********************************','*************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','********************************','****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','**********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','********************************','*************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','**************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************'}), 3)
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************','*********************************************************************','*************************************************************************************************************','****************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************************************','********************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************'}))
		return false
	end
	if value == lastProcessedInput then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************','*************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************************','********************************','*******************************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','**********************************************************','********************************'}) .. value)
		return false
	end
	local target = findPlayer(value)
	if not target then
		notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. value, 3)
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************'}) .. value)
		return false
	end
	local userId = target.UserId
	-- Check if player is already in the list
	for _, id in ipairs(AssistOthers.AllowedPlayers) do
		if id == userId then
			notify(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************'}),
				target.DisplayName .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*********************************************************************************************************','*******************************************************************************************************************','********************************','*************************************************************************************************','************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','********************************','**************************************************************************************************','*****************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}),
				3
			)
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','*************************************************************************************************','************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','********************************','*************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'})
					.. target.DisplayName
			)
			return false
		end
	end
	-- Add player to assist list and enable assisting
	table.insert(AssistOthers.AllowedPlayers, userId)
	AssistOthers.Enabled = true
	lastProcessedInput = value
	notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************','***************************************************************************************************************','***********************************************************************************************************************','********************************','*************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************'}) .. target.DisplayName, 3)
	dbgWarn(
		dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'****************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************','**************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************','*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
			.. target.DisplayName
			.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','****************************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************************','****************************************************************************************************','**********************************************************','********************************'})
			.. userId
			.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************'})
	)
	return true
end

