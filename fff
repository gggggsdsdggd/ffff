*****************************','*****************************************************************************************************'}))
		end
		local Module = require(Handler)
		Module.Ignored = {
			workspace:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**************************************************************************************','*****************************************************************************************************','********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'})),
			workspace:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*****************************************************************************','*****************************************************************','********************************************************************************'})),
			workspace:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'})),
		}
		dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************','*************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**************************************************************************************','*****************************************************************************************************','********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************','********************************','*****************************************************************************','*****************************************************************','********************************************************************************','********************************************','********************************','*************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
	end)
	if not success then
		dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*****************************************************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************'}) .. tostring(err))
	end
end

-- Hook GunHandler Shoot
local function hookGunHandlerShoot()
	local success, err = pcall(function()
		local gunHandler =
			require(ReplicatedStorage.Modules:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})))
		local oldShoot = gunHandler.shoot
		gunHandler.shoot = function(args)
			if RapidFireEnabled or TargetAll then
				args.Range = RapidFire.InfiniteRange
				dbgWarn(
					vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
						.. RapidFire.InfiniteRange
						.. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','*********************************************************************************************************','**************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'})
				)
			end
			return oldShoot(args)
		end
	end)
	if not success then
		dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************'}) .. tostring(err))
	end
end

task.spawn(function()
	applyWallbang()
	hookGunHandlerShoot()
end)

-- Rapid Fire Logic
local function randomVector3(maxOffset)
	return Vector3.new(
		(math.random() - 0.5) * 2 * maxOffset,
		(math.random() - 0.5) * 2 * maxOffset,
		(math.random() - 0.5) * 2 * maxOffset
	)
end

local function patchGun(tool)
	if not GUN_NAMES[tool.Name] or PatchedGuns[tool] then
		return
	end
	PatchedGuns[tool] = true
	local ammo = tool:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}))
	local handle = tool:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
	local gunHandler =
		require(ReplicatedStorage.Modules:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})))
	local remote = tool:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'}))
	local range = tool:WaitForChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************'}))
	local maid = require(ReplicatedStorage.Modules.Maid).new()
	GunMaids[tool] = maid

	applyWallbang()

	task.spawn(function()
		while tool.Parent and tool.Parent == Client.Character do
			local success, err = pcall(function()
				ammo.Value = RapidFire.InfiniteAmmo
				range.Value = RapidFire.InfiniteRange
				dbgWarn(
					vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************'})
						.. tool.Name
						.. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','*************************************************************'})
						.. ammo.Value
						.. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
						.. range.Value
				)
			end)
			if not success then
				dbgWarn(
					vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*************************************************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','***********************************************','******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
						.. tool.Name
						.. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************','********************************'})
						.. tostring(err)
				)
			end
			task.wait(0.1)
		end
		maid:Destroy()
		GunMaids[tool] = nil
		PatchedGuns[tool] = nil
		dbgWarn(
			vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. tool.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','*********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************************','********************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************************','****************************************************************************************************************','********************************','*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'})
		)
	end)

	maid:GiveTask(RunService.Heartbeat:Connect(function()
		if
			not (RapidFireEnabled or TargetAll)
			or not mouseHeld
			or ammo.Value <= 0
			or tool.Parent ~= Client.Character
		then
			return
		end
		local success, err = pcall(function()
			range.Value = RapidFire.InfiniteRange
			applyWallbang()
			local muzzle = tool:FindFirstChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************','*****************************************************************************************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','********************************************************************************************************************'}))
				and tool.Default:FindFirstChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*****************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************'}))
				and tool.Default.Mesh:FindFirstChild(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*****************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
			local position = muzzle and muzzle.WorldPosition
				or handle.CFrame.Position
			if TargetAll then
				for _, p in pairs(Players:GetPlayers()) do
					if
						p ~= LocalPlayer
						and p.Character
						and p.Character:FindFirstChild(TargetPart)
					then
						local targetPart =
							p.Character:FindFirstChild(TargetPart)
						for i = 1, RapidFire.BulletsPerShot do
							local targetPos =
								PredictPosition(targetPart, BulletSpeed)
							local aimOffset =
								randomVector3(RapidFire.SpreadAmount)
							local finalPos = targetPos + aimOffset
							pcall(function()
								setidentity(2)
							end)
							local vShoot, v2, v3 = gunHandler.shoot({
								Shooter = LocalPlayer.Character,
								Handle = handle,
								ForcedOrigin = position,
								AimPosition = finalPos,
								BeamColor = Color3.new(1, 0.545, 0.149),
								Range = RapidFire.InfiniteRange,
							})
							pcall(function()
								setidentity(8)
							end)
							addShot()
							ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
								vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
								handle,
								position,
								vShoot,
								v2,
								v3
							)
							remote:FireServer(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
							dbgWarn(
								vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','************************************************************************************************************','************************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','*************************************************************************************************','********************************************************************************************************************','********************************'})
									.. p.Name
									.. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
									.. tostring(finalPos)
							)
						end
					end
				end
			elseif
				RapidFireEnabled
				and Target
				and Target.Character
				and Target.Character:FindFirstChild(TargetPart)
			then
				for i = 1, RapidFire.BulletsPerShot do
					local targetPos = PredictPosition(
						Target.Character:FindFirstChild(TargetPart),
						BulletSpeed
					)
					local aimOffset = randomVector3(RapidFire.SpreadAmount)
					local finalPos = targetPos + aimOffset
					pcall(function()
						setidentity(2)
					end)
					local vShoot, v2, v3 = gunHandler.shoot({
						Shooter = LocalPlayer.Character,
						Handle = handle,
						ForcedOrigin = position,
						AimPosition = finalPos,
						BeamColor = Color3.new(1, 0.545, 0.149),
						Range = RapidFire.InfiniteRange,
					})
					pcall(function()
						setidentity(8)
					end)
					addShot()
					ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
						vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
						handle,
						position,
						vShoot,
						v2,
						v3
					)
					remote:FireServer(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
					dbgWarn(
						vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','*************************************************************************************************','********************************************************************************************************************','********************************'})
							.. Target.Name
							.. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
							.. tostring(finalPos)
					)
				end
			elseif RapidFireEnabled then
				for i = 1, RapidFire.BulletsPerShot do
					local aimOffset = randomVector3(RapidFire.SpreadAmount)
					local targetPos = position
						+ gunHandler.getAim(position) * RapidFire.InfiniteRange
						+ aimOffset
					pcall(function()
						setidentity(2)
					end)
					local vShoot, v2, v3 = gunHandler.shoot({
						Shooter = LocalPlayer.Character,
						Handle = handle,
						ForcedOrigin = position,
						AimPosition = targetPos,
						BeamColor = Color3.new(1, 0.545, 0.149),
						Range = RapidFire.InfiniteRange,
					})
					pcall(function()
						setidentity(8)
					end)
					addShot()
					ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
						vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
						handle,
						position,
						vShoot,
						v2,
						v3
					)
					remote:FireServer(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
					dbgWarn(
						vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','********************************','*************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
							.. tostring(targetPos)
					)
				end
			end
		end)
		if not success then
			dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*****************************************************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. tool.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************','********************************'}) .. tostring(err))
		end
	end))

	notify(
		vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************'}),
		vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************'}) .. tool.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','********************************','*******************************************','********************************','*************************************************************************','**************************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','********************************','*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}),
		3
	)
	dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************'}) .. tool.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************'}))
	return maid
end

-- Auto Patch New Guns
Client.Backpack.ChildAdded:Connect(function(child)
	if GUN_NAMES[child.Name] then
		task.wait(0.1)
		patchGun(child)
	end
end)

-- Patch Existing Gun
local equippedGun = Client.Character
	and Client.Character:FindFirstChildOfClass(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
if equippedGun and GUN_NAMES[equippedGun.Name] then
	patchGun(equippedGun)
end

-- Prevent Duplicate Firing
local oldFireServer
oldFireServer = hookmetamethod(game, vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************************','***********************************************************************************************','**************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************'}), function(self, ...)
	local method = getnamecallmethod()
	local args = { ... }
	if
		method == vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})
		and self.Name == vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'})
		and args[1] == vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'})
		and (RapidFireEnabled or TargetAll)
	then
		dbgWarn(
			vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'******************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','****************************************************************************************************','*****************************************************************************************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','********************************************************************************************************************','********************************','**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','**********************************************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','****************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','********************************','****************************************************************************************************','*********************************************************************************************************************','****************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************'})
		)
		return
	end
	return oldFireServer(self, ...)
end)

-- Cleanup and Re-patch on Respawn
Client.CharacterAdded:Connect(function(newCharacter)
	character = newCharacter
	tpwalking = false
	mouseHeld = false
	if
		ViewEnabled
		and Target
		and Target.Character
		and Target.Character:FindFirstChild(TargetPart)
	then
		local targetPart = Target.Character:FindFirstChild(TargetPart)
		if targetPart then
			CurrentCamera.CameraSubject = targetPart
			notify(
				vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}),
				vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*****************************************************************************************************','*********************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','********************************','***************************************************************************************************************','**************************************************************************************************************','********************************'}) .. Target.DisplayName .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','*************************************************************************************************','******************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'}),
				3
			)
		else
			ViewEnabled = false
			CurrentCamera.CameraSubject = newCharacter
				and newCharacter:FindFirstChildWhichIsA(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			notify(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}), vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','****************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','****************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}), 3)
		end
	else
		CurrentCamera.CameraSubject = newCharacter
			and newCharacter:FindFirstChildWhichIsA(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
	end
	Text.Visible = false
	Circle.Visible = false
	currentText = vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({})
	textIndex = 1
	for tool, maid in pairs(GunMaids) do
		maid:Destroy()
		GunMaids[tool] = nil
		PatchedGuns[tool] = nil
		dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*******************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************************','****************************************************************************************************************','********************************','*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. tool.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','***************************************************************************************************************','**************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'}))
	end
	local equippedGun = newCharacter
		and newCharacter:FindFirstChildOfClass(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
	if equippedGun and GUN_NAMES[equippedGun.Name] then
		patchGun(equippedGun)
		dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**********************************************************************************','*****************************************************************************************************','*********************************************','****************************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. equippedGun.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','***************************************************************************************************************','**************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'}))
	end
	local childAddedConnection
	childAddedConnection = newCharacter.ChildAdded:Connect(function(child)
		if child:IsA(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'})) and GUN_NAMES[child.Name] then
			task.wait(0.1)
			patchGun(child)
			dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**************************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. child.Name .. vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************','***************************************************************************************************************','**************************************************************************************************************','********************************','**************************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
		end
	end)
	newCharacter.AncestryChanged:Connect(function()
		if not newCharacter:IsDescendantOf(workspace) then
			childAddedConnection:Disconnect()
			dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************','*********************************************************************************************************','*******************************************************************************************************************','***************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************','********************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','****************************************************************************************************','*****************************************************************','*********************************************************************************