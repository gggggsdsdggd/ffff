        local connection
        connection = UserInputService.InputBegan:Connect(
            function(input, processed)
                if
                    processed
                    or input.UserInputType ~= Enum.UserInputType.Keyboard
                    or not listening
                then
                    return
                end
                listening = false
                Keybinds.TPWalk = input.KeyCode
                TPWalkKeyLabel.Text = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name
                dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name)
                connection:Disconnect()
            end
        )
    end,
})

