-- *** ENHANCED JESUS CROSS FUNCTIONS ***
local function createBackCross()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local torso = char:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8925,11760,11760,10605,11970,8820,11655,11970,12075,11655})) or char:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11970,12075,11655}))

    if char:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,10185,10395,11235,7035,11970,11655,12075,12075})) then
        char.BackCross:Destroy()
    end

    JesusCrossModel = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,11655,10500,10605,11340}))
    JesusCrossModel.Name = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,10185,10395,11235,7035,11970,11655,12075,12075})
    JesusCrossModel.Parent = char

    local verticalLength = 4
    local horizontalLength = 2
    local thickness = 0.4

    local vert = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8400,10185,11970,12180}))
    vert.Size = Vector3.new(thickness, verticalLength, thickness)
    vert.Material = JesusCrossMaterial -- DYNAMIC MATERIAL!
    vert.Color = JesusCrossColor -- DYNAMIC COLOR!
    vert.Transparency = 0.2
    vert.Anchored = false
    vert.CanCollide = false
    vert.Parent = JesusCrossModel

    local horiz = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8400,10185,11970,12180}))
    horiz.Size = Vector3.new(horizontalLength, thickness, thickness)
    horiz.Material = JesusCrossMaterial -- DYNAMIC MATERIAL!
    horiz.Color = JesusCrossColor -- DYNAMIC COLOR!
    horiz.Transparency = 0.2
    horiz.Anchored = false
    horiz.CanCollide = false
    horiz.Parent = JesusCrossModel

    local function weldPart(part, offsetCFrame)
        local weld = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9135,10605,11340,10500}))
        weld.Part0 = torso
        weld.Part1 = part
        weld.C0 = offsetCFrame
        weld.Parent = part
    end

    local offsetZ = 0.7
    weldPart(vert, CFrame.new(0, 0, offsetZ))
    weldPart(horiz, CFrame.new(0, verticalLength/4, offsetZ))

    -- Enhanced Pulse effect
    if JesusPulseConnection then JesusPulseConnection:Disconnect() end
    local pulseDirection = 1
    JesusPulseConnection = RunService.Heartbeat:Connect(function()
        if not JesusCrossModel or not JesusCrossModel.Parent then
            JesusPulseConnection:Disconnect()
            return
        end
        vert.Transparency = math.clamp(vert.Transparency + 0.01 * pulseDirection, 0.1, 0.4)
        horiz.Transparency = vert.Transparency
        if vert.Transparency >= 0.4 then 
            pulseDirection = -1
        elseif vert.Transparency <= 0.1 then 
            pulseDirection = 1 
        end
    end)

    LocalPlayer.CharacterAdded:Connect(function(newChar)
        if JesusCrossEnabled then
            task.wait(1)
            createBackCross()
        end
    end)
end

local function enableJesusCross()
    task.wait(0.1)
    createBackCross()
end

local function disableJesusCross()
    if JesusCrossModel then JesusCrossModel:Destroy() end
    if JesusPulseConnection then JesusPulseConnection:Disconnect() end
    JesusCrossEnabled = false
end

-- Notification Function
local function notify(title, text, dur)
    if SEND_NOTIFICATIONS then
        pcall(function()
            StarterGui:SetCore(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10605,11550,10500,8190,11655,12180,11025,10710,11025,10395,10185,12180,11025,11655,11550}), {
                Title = title,
                Text = text,
                Duration = dur or 3,
            })
        end)
    end
end

-- Debug Function
local function dbgWarn(msg)
    if DEBUG_MODE then
        warn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9555,7140,7245,6930,8925,7455,9765,3360}) .. tostring(msg))
    end
end

-- Hit Sound
local function playHitSound()
    local head = LocalPlayer.Character
        and LocalPlayer.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,10605,10185,10500}))
    if head then
        local sound = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,11655,12285,11550,10500}))
        sound.SoundId = hitSoundId
        sound.Volume = 1
        sound.Parent = head
        sound:Play()
        game.Debris:AddItem(sound, 2)
    end
end

-- Hit Chams Creation
local function createHitCham(targetChar)
    if
        not targetChar
        or not HitChamsEnabled
        or not targetChar:IsDescendantOf(workspace)
    then
        return
    end

    local chamModel = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,11655,10500,10605,11340}))
    chamModel.Name = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180,7035,10920,10185,11445})
    chamModel.Parent = workspace

    for _, partName in ipairs(BodyParts) do
        local part = targetChar:FindFirstChild(partName)
        if part and part:IsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,10185,12075,10605,8400,10185,11970,12180})) then
            local chamPart = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8400,10185,11970,12180}))
            chamPart.Size = part.Size
            chamPart.CFrame = part.CFrame
            chamPart.Anchored = true
            chamPart.CanCollide = false
            chamPart.Material = HitChamMaterial
            chamPart.Color = HitChamColor
            chamPart.Transparency = 0.5
            chamPart.Parent = chamModel
        end
    end

    if #chamModel:GetChildren() == 0 then
        chamModel:Destroy()
        return
    end

    local startTime = tick()
    local conn
    conn = RunService.RenderStepped:Connect(function()
        local elapsed = tick() - startTime
        if elapsed >= ChamFadeTime then
            chamModel:Destroy()
            conn:Disconnect()
        else
            local t = elapsed / ChamFadeTime
            for _, p in ipairs(chamModel:GetChildren()) do
                if p:IsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,10185,12075,10605,8400,10185,11970,12180})) then
                    p.Transparency = 0.5 + 0.5 * t
                end
            end
        end
    end)
    dbgWarn(
        CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7035,11970,10605,10185,12180,10605,10500,3360,7560,11025,12180,7035,10920,10185,11445,3360,10710,11655,11970,3360})
            .. (targetChar.Name or CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({12285,11550,11235,11550,11655,12495,11550}))
            .. CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({3360,12495,11025,12180,10920,3360,11445,10185,12180,10605,11970,11025,10185,11340,6090,3360})
            .. tostring(HitChamMaterial)
    )
end

-- *** RAPID FIRE IMPLEMENTATION ***
--// Services
local ReplicatedStorage = game:GetService(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605}))
local Players = game:GetService(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8400,11340,10185,12705,10605,11970,12075}))
local UserInputService = game:GetService(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8925,12075,10605,11970,7665,11550,11760,12285,12180,8715,10605,11970,12390,11025,10395,10605}))

--// Player setup
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()

--// Helper function to get equipped tool
local function findEquippedTool()
    for _, tool in ipairs(Character:GetChildren()) do
        if tool:IsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11655,11340})) then
            return tool
        end
    end
    return nil
end

--// Vars
local tool
local maid
local customCooldown = 0.01 -- Adjust this for faster/slower rate
local isMouseHeld = false

--// Function to initialize a tool
local function initializeTool(newTool)
    if not newTool:IsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11655,11340})) then return end
    if not RapidFireEnabled then return end -- Only initialize if Rapid Fire is toggled on
    tool = newTool
    notify(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605}), CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11655,11340,3360,11025,11550,11025,12180,11025,10185,11340,11025,12810,10605,10500,6090,3360}) .. tool.Name, 3)
    dbgWarn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11655,11340,3360,11025,11550,11025,12180,11025,10185,11340,11025,12810,10605,10500,6090,3360}) .. tool.Name)

    local v_u_1 = CFrame.new(-1, 0.4, 0)
    local v_u_2 = ReplicatedStorage
    local v_u_3 = LocalPlayer
    local v_u_4 = tool
    local v_u_5 = v_u_4:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,10185,11550,10500,11340,10605}), 10)
    local v_u_6 = Character
    local v_u_7 = require(v_u_2:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,11655,10500,12285,11340,10605,12075}), 10):WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970}), 10))
    local v_u_8 = v_u_4:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11445,11445,11655}), 10)
    local v_u_9 = v_u_4:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,10185,11550,10815,10605}), 10)
    local v_u_10 = v_u_4:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10920,11655,11655,12180,11025,11550,10815,7035,11655,11655,11340,10500,11655,12495,11550}), 10).Value
    local v_u_12 = require(v_u_2:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,11655,10500,12285,11340,10605,12075}), 10):WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,10185,11025,10500}), 10)).new()
    local v_u_13 = v_u_4:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})) or { FireServer = function() end }

    maid = v_u_12 -- track cleanup

    --// Shooting logic
    local function shoot()
        if not v_u_4.Parent or v_u_4.Parent ~= v_u_6 then return end
        v_u_8.Value = 99999
        v_u_13:FireServer(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10920,11655,11655,12180}))

        local v14 = (v_u_5.CFrame * v_u_1).Position
        local muzzle = v_u_4:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7140,10605,10710,10185,12285,11340,12180})) and v_u_4.Default:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,10605,12075,10920})) and v_u_4.Default.Mesh:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,12285,12810,12810,11340,10605}))
        local muzzlePos = muzzle and muzzle.WorldPosition or v14

        --// Add bullet spread (tweak CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({12075,11760,11970,10605,10185,10500,6825,11550,10815,11340,10605}) to adjust)
        local spreadAngle = 2 -- degrees of spread
        local randomX = math.rad(math.random(-spreadAngle, spreadAngle))
        local randomY = math.rad(math.random(-spreadAngle, spreadAngle))
        local spreadCFrame = CFrame.Angles(randomX, randomY, 0)

        -- Apply spread to aim
    -- Determine where to aim depending on Target Aim state
    local aimPos

    if BulletTPEnabled and Target and Target.Character then
        local targetPart = Target.Character:FindFirstChild(TargetPart or CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,10605,10185,10500}))
        if targetPart then
            -- Predict bullet landing position (same math as your Target Aim)
            local predicted = targetPart.Position + (targetPart.Velocity * (Settings.Prediction or LeadStrength or 0.1))
            aimPos = predicted
        else
            aimPos = muzzlePos + aimDirection * 200 -- fallback
        end
    else
        -- Normal free-aim when Target Aim is off
        local aimDirection = (v_u_7.getAim(muzzlePos)).unit
        aimDirection = (CFrame.lookAt(Vector3.new(), aimDirection) * spreadCFrame).LookVector
        aimPos = muzzlePos + aimDirection * 200
    end

    -- Fire bullet to that final position
    local v17, v18, v19 = v_u_7.shoot({
        Shooter = v_u_6,
        Handle = v_u_5,
        ForcedOrigin = muzzlePos,
        AimPosition = aimPos,
        BeamColor = Color3.new(1, 0.545098, 0.14902),
        Range = v_u_9.Value
    })

        local v17, v18, v19 = v_u_7.shoot({
            Shooter = v_u_6,
            Handle = v_u_5,
            ForcedOrigin = muzzlePos,
            AimPosition = aimPos,
            BeamColor = Color3.new(1, 0.545098, 0.14902),
            Range = v_u_9.Value
        })

        local mainRemotes = v_u_2:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,12075}))
        if mainRemotes then
            local evt = mainRemotes:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
            if evt then
                evt:FireServer(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({12600,7140,8715,10920,11655,8295,12180,7455,12285,11550,9450,10605,10605,7560,8295,8295,7140,9240,7140,7140}), v_u_5, muzzlePos, v17, v18, v19)
            end
        end
        v_u_13:FireServer()
    end

    --// Input connections
    v_u_12:GiveTask(UserInputService.InputBegan:Connect(function(input, gp)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isMouseHeld = true
            while isMouseHeld and v_u_4.Parent == v_u_6 and RapidFireEnabled do
                shoot()
                task.wait(customCooldown)
            end
        end
    end))

    v_u_12:GiveTask(UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isMouseHeld = false
        end
    end))

    --// Auto ammo reset
    v_u_12:GiveTask(v_u_8:GetPropertyChangedSignal(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9030,10185,11340,12285,10605})):Connect(function()
        if v_u_8.Value < 99999 then
            v_u_8.Value = 99999
        end
    end))

    --// Cleanup when unequipped
    v_u_12:GiveTask(v_u_4.AncestryChanged:Connect(function()
        if not v_u_4:IsDescendantOf(v_u_3) then
            v_u_12:DoCleaning()
            tool = nil
        end
    end))
end

--// Detect tool equipped automatically
--// Robust tool detection across respawns
local function onCharacterChildAdded(obj)
    if obj:IsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11655,11340})) then
        task.wait(0.1)
        if RapidFireEnabled then
            initializeTool(obj)
        end
    end
end

local characterChildConn
local characterRemovingConn

local function onCharacterAdded(newChar)
    if characterChildConn then
        characterChildConn:Disconnect()
        characterChildConn = nil
    end
    if characterRemovingConn then
        characterRemovingConn:Disconnect()
        characterRemovingConn = nil
    end
    if maid and type(maid.DoCleaning) == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({10710,12285,11550,10395,12180,11025,11655,11550}) then
        pcall(function() maid:DoCleaning() end)
        maid = nil
        tool = nil
    end

    Character = newChar

    characterChildConn = Character.ChildAdded:Connect(function(obj)
        onCharacterChildAdded(obj)
    end)

    task.spawn(function()
        task.wait(0.1)
        for _, c in ipairs(Character:GetChildren()) do
            if c:IsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,11655,11655,11340})) and RapidFireEnabled then
                initializeTool(c)
                break
            end
        end
    end)

    characterRemovingConn = LocalPlayer.CharacterRemoving:Connect(function()
        if characterChildConn then
            pcall(function() characterChildConn:Disconnect() end)
            characterChildConn = nil
        end
        if maid and type(maid.DoCleaning) == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({10710,12285,11550,10395,12180,11025,11655,11550}) then
            pcall(function() maid:DoCleaning() end)
            maid = nil
            tool = nil
        end
    end)
end

