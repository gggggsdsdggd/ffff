    if ForceHitEnabled and ForceHitTarget then
        if tick() - lastShoot >= shootCooldown then
            fireGun()
            lastShoot = tick()
        end
    end
end)

RunService.Heartbeat:Connect(function(delta)
    if tpWalkEnabled and tpwalking then
        local chr = Client.Character
        local hum = chr and chr:FindFirstChildWhichIsA(ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500}))
        local root = chr and chr:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
        if chr and hum and root and hum.MoveDirection.Magnitude > 0 then
            local success, err = pcall(function()
                root.CFrame = root.CFrame
                    + (hum.MoveDirection * tpWalkSpeed * delta * 10)
            end)
            if not success then
                local targetPos = root.Position
                    + (hum.MoveDirection * tpWalkSpeed * delta * 10)
                hum:MoveTo(targetPos)
                dbgWarn(
                    ZuRdGvkgaGnppuwNtpwpGfeN({8820,8400,3360,9135,10185,11340,11235,3360,7035,7350,11970,10185,11445,10605,3360,10710,10185,11025,11340,10605,10500,4620,3360,12285,12075,11025,11550,10815,3360,8085,11655,12390,10605,8820,11655,6090,3360}) .. tostring(err)
                )
            else
                dbgWarn(
                    ZuRdGvkgaGnppuwNtpwpGfeN({8820,8400,3360,9135,10185,11340,11235,3360,10185,11760,11760,11340,11025,10605,10500,6090,3360,8085,11655,12390,10605,10500,3360,10290,12705,3360})
                        .. tostring(
                            hum.MoveDirection * tpWalkSpeed * delta * 10
                        )
                )
            end
        else
            dbgWarn(
                ZuRdGvkgaGnppuwNtpwpGfeN({8820,8400,3360,9135,10185,11340,11235,3360,10710,10185,11025,11340,10605,10500,6090,3360,7035,10920,10185,11970,10185,10395,12180,10605,11970,6405})
                    .. tostring(chr)
                    .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,7560,12285,11445,10185,11550,11655,11025,10500,6405})
                    .. tostring(hum)
                    .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,8610,11655,11655,12180,6405})
                    .. tostring(root)
            )
        end
    end
end)

local function PredictPosition(targetPart, bulletSpeed)
    if not targetPart or not targetPart:IsDescendantOf(workspace) then
        return targetPart and targetPart.Position or Vector3.zero
    end
    local char = Client.Character
    local hrp = char and char:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
    if not hrp then
        return targetPart.Position
    end
    local playerPos = hrp.Position
    local targetPos = targetPart.Position
    local targetVel = targetPart.Velocity
    local r = targetPos - playerPos
    local v = targetVel
    local s = bulletSpeed
    local a = v:Dot(v) - (s * s)
    local b = 2 * r:Dot(v)
    local c = r:Dot(r)
    local t
    local EPS = 1e-6
    if math.abs(a) < EPS then
        if math.abs(b) < EPS then
            return targetPos
        end
        t = -c / b
    else
        local disc = b * b - 4 * a * c
        if disc < 0 then
            return targetPos
        end
        local sqrtDisc = math.sqrt(disc)
        local t1 = (-b - sqrtDisc) / (2 * a)
        local t2 = (-b + sqrtDisc) / (2 * a)
        t = (t1 > 0 and t1) or (t2 > 0 and t2) or nil
    end
    if not t or t <= 0 or t > 3 then
        return targetPos
    end
    local predicted = targetPos + v * t
    predicted = targetPos:Lerp(predicted, LeadStrength)
    local ping = 0.08
    local ok, p = pcall(function()
        return Client:GetNetworkPing()
    end)
    if ok and type(p) == ZuRdGvkgaGnppuwNtpwpGfeN({11550,12285,11445,10290,10605,11970}) then
        ping = p
    end
    ping = math.clamp(ping, 0.05, 0.5)
    predicted += v * ping
    if v.Magnitude > 0 then
        predicted += v.Unit * 0.1
    end
    return predicted
end

local function InstallBulletHook()
    local mt = getrawmetatable(game)
    setreadonly(mt, false)
    local previous = mt.__index
    local function hook(self, key)
        local ok, result = pcall(function()
            if not checkcaller() and self == Mouse and SilentAim.Enabled then
                local TargetPart = GetSilentAimTarget()
                if TargetPart then
                    if key == ZuRdGvkgaGnppuwNtpwpGfeN({7560,11025,12180}) then
                        dbgWarn(
                            ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,10500,11025,11970,10605,10395,12180,11025,11550,10815,3360,8085,11655,12285,12075,10605,4830,7560,11025,12180,3360,12180,11655,6090,3360})
                                .. TargetPart.Parent.Name
                        )
                        return CFrame.new(TargetPart.Position)
                    elseif key == ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180}) then
                        dbgWarn(
                            ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,10500,11025,11970,10605,10395,12180,11025,11550,10815,3360,8085,11655,12285,12075,10605,4830,8820,10185,11970,10815,10605,12180,3360,12180,11655,6090,3360})
                                .. TargetPart.Parent.Name
                        )
                        return TargetPart
                    end
                end
            elseif
                not checkcaller()
                and self == Mouse
                and BulletTPEnabled
                and not RapidFireEnabled
                and not TargetAll
                and Target
                and (
                    key == ZuRdGvkgaGnppuwNtpwpGfeN({7560,11025,12180})
                    or (key == ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180}) and game.PlaceId == 2788229376)
                )
            then
                local part = Target.Character
                    and Target.Character:FindFirstChild(TargetPart)
                if part then
                    local pos = PredictPosition(part, BulletSpeed)
                    if typeof(pos) == ZuRdGvkgaGnppuwNtpwpGfeN({9030,10605,10395,12180,11655,11970,5355}) then
                        return CFrame.new(pos)
                    end
                end
            end
            return previous(self, key)
        end)
        if ok then
            return result
        else
            return previous(self, key)
        end
    end
    local newIndex = newcclosure(hook)
    mt.__index = newIndex
    setreadonly(mt, true)
    return newIndex
end

local currentMouseIndex = InstallBulletHook()
task.spawn(function()
    while true do
        task.wait(0.25)
        local success, mt = pcall(getrawmetatable, game)
        if success and mt then
            local success_index, index = pcall(function()
                return mt.__index
            end)
            if success_index and index and index ~= currentMouseIndex then
                dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({9135,10185,12180,10395,10920,10500,11655,10815,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,9975,9975,11025,11550,10500,10605,12600,3360,10395,10920,10185,11550,10815,10605,4620,3360,11970,10605,11025,11550,12075,12180,10185,11340,11340,11025,11550,10815,3360,10920,11655,11655,11235}))
                currentMouseIndex = InstallBulletHook()
            end
        end
    end
end)

local function applyWallbang()
    local success, err = pcall(function()
        local Handler = ReplicatedStorage:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605}))
        if not Handler then
            error(ZuRdGvkgaGnppuwNtpwpGfeN({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605}))
        end
        local Module = require(Handler)
        Module.Ignored = {
            workspace:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({9030,10605,10920,11025,10395,11340,10605,12075})),
            workspace:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,6825,8400})),
            workspace:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({7665,10815,11550,11655,11970,10605,10500})),
        }
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({9135,10185,11340,11340,10290,10185,11550,10815,3360,10185,11760,11760,11340,11025,10605,10500,6090,3360,7665,10815,11550,11655,11970,10605,10500,3360,9030,10605,10920,11025,10395,11340,10605,12075,4620,3360,8085,6825,8400,4620,3360,7665,10815,11550,11655,11970,10605,10500}))
    end)
    if not success then
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({9135,10185,11340,11340,10290,10185,11550,10815,3360,10605,11970,11970,11655,11970,6090,3360}) .. tostring(err))
    end
end

local function hookGunHandlerShoot()
    local success, err = pcall(function()
        local gunHandler =
            require(ReplicatedStorage.Modules:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970})))
        local oldShoot = gunHandler.shoot
        gunHandler.shoot = function(args)
            if RapidFireEnabled or TargetAll then
                args.Range = RapidFire.InfiniteRange
                dbgWarn(
                    ZuRdGvkgaGnppuwNtpwpGfeN({7350,11655,11970,10395,10605,10500,3360,8610,10185,11550,10815,10605,6405})
                        .. RapidFire.InfiniteRange
                        .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,11025,11550,3360,10815,12285,11550,7560,10185,11550,10500,11340,10605,11970,4830,12075,10920,11655,11655,12180})
                )
            end
            return oldShoot(args)
        end
    end)
    if not success then
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({7245,11970,11970,11655,11970,3360,10920,11655,11655,11235,11025,11550,10815,3360,7455,12285,11550,7560,10185,11550,10500,11340,10605,11970,6090,3360}) .. tostring(err))
    end
end

task.spawn(function()
    applyWallbang()
    hookGunHandlerShoot()
end)

local function randomVector3(maxOffset)
    return Vector3.new(
        (math.random() - 0.5) * 2 * maxOffset,
        (math.random() - 0.5) * 2 * maxOffset,
        (math.random() - 0.5) * 2 * maxOffset
    )
end

local function patchGun(tool)
    if not GUN_NAMES[tool.Name] or PatchedGuns[tool] then
        return
    end
    PatchedGuns[tool] = true
    local ammo = tool:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({6825,11445,11445,11655}))
    local handle = tool:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10185,11550,10500,11340,10605}))
    local gunHandler =
        require(ReplicatedStorage.Modules:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970})))
    local remote = tool:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
    local range = tool:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({8610,10185,11550,10815,10605}))
    local maid = require(ReplicatedStorage.Modules.Maid).new()
    GunMaids[tool] = maid

    applyWallbang()

    task.spawn(function()
        while tool.Parent and tool.Parent == Client.Character do
            local success, err = pcall(function()
                ammo.Value = RapidFire.InfiniteAmmo
                range.Value = RapidFire.InfiniteRange
                dbgWarn(
                    ZuRdGvkgaGnppuwNtpwpGfeN({8715,10605,12180,3360})
                        .. tool.Name
                        .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,6825,11445,11445,11655,6405})
                        .. ammo.Value
                        .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,8610,10185,11550,10815,10605,6405})
                        .. range.Value
                )
            end)
            if not success then
                dbgWarn(
                    ZuRdGvkgaGnppuwNtpwpGfeN({7245,11970,11970,11655,11970,3360,12075,10605,12180,12180,11025,11550,10815,3360,10185,11445,11445,11655,4935,11970,10185,11550,10815,10605,3360,10710,11655,11970,3360})
                        .. tool.Name
                        .. ZuRdGvkgaGnppuwNtpwpGfeN({6090,3360})
                        .. tostring(err)
                )
            end
            task.wait(0.1)
        end
        maid:Destroy()
        GunMaids[tool] = nil
        PatchedGuns[tool] = nil
        dbgWarn(
            ZuRdGvkgaGnppuwNtpwpGfeN({7455,12285,11550,3360}) .. tool.Name .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,12285,11550,10605,11865,12285,11025,11760,11760,10605,10500,3360,11655,11970,3360,11970,10605,11445,11655,12390,10605,10500,4620,3360,10395,11340,10605,10185,11550,10605,10500,3360,12285,11760,3360,8085,10185,11025,10500})
        )
    end)

    maid:GiveTask(RunService.Heartbeat:Connect(function()
        if
            not (RapidFireEnabled or TargetAll)
            or not mouseHeld
            or ammo.Value <= 0
            or tool.Parent ~= Client.Character
        then
            return
        end
        local success, err = pcall(function()
            range.Value = RapidFire.InfiniteRange
            applyWallbang()
            local muzzle = tool:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7140,10605,10710,10185,12285,11340,12180}))
                and tool.Default:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,10605,12075,10920}))
                and tool.Default.Mesh:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8085,12285,12810,12810,11340,10605}))
            local position = muzzle and muzzle.WorldPosition
                or handle.CFrame.Position
            if TargetAll then
                for _, p in pairs(Players:GetPlayers()) do
                    if
                        p ~= LocalPlayer
                        and p.Character
                        and p.Character:FindFirstChild(TargetPart)
                    then
                        local targetPart =
                            p.Character:FindFirstChild(TargetPart)
                        for i = 1, RapidFire.BulletsPerShot do
                            local targetPos =
                                PredictPosition(targetPart, BulletSpeed)
                            local aimOffset =
                                randomVector3(RapidFire.SpreadAmount)
                            local finalPos = targetPos + aimOffset
                            pcall(function()
                                setidentity(2)
                            end)
                            local vShoot, v2, v3 = gunHandler.shoot({
                                Shooter = LocalPlayer.Character,
                                Handle = handle,
                                ForcedOrigin = position,
                                AimPosition = finalPos,
                                BeamColor = Color3.new(1, 0.545, 0.149),
                                Range = RapidFire.InfiniteRange,
                            })
                            pcall(function()
                                setidentity(8)
                            end)
                            addShot()
                            if HitChamsEnabled then
                                createHitCham(p.Character)
                            end
                            ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
                                ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180,7455,12285,11550}),
                                handle,
                                position,
                                vShoot,
                                v2,
                                v3
                            )
                            remote:FireServer(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180}))
                            dbgWarn(
                                ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,6825,11340,11340,3360,12075,10920,11655,12180,3360,10185,12180,3360})
                                    .. p.Name
                                    .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
                                    .. tostring(finalPos)
                            )
                        end
                    end
                end
            elseif RapidFireEnabled and SilentAim.Enabled then
                local targetPart = GetSilentAimTarget()
                if targetPart then
                    for i = 1, RapidFire.BulletsPerShot do
                        local targetPos = targetPart.Position
                        local aimOffset = randomVector3(RapidFire.SpreadAmount)
                        local finalPos = targetPos + aimOffset
                        pcall(function()
                            setidentity(2)
                        end)
                        local vShoot, v2, v3 = gunHandler.shoot({
                            Shooter = LocalPlayer.Character,
                            Handle = handle,
                            ForcedOrigin = position,
                            AimPosition = finalPos,
                            BeamColor = Color3.new(1, 0.545, 0.149),
                            Range = RapidFire.InfiniteRange,
                        })
                        pcall(function()
                            setidentity(8)
                        end)
                        addShot()
                        if HitChamsEnabled then
                            createHitCham(targetPart.Parent)
                        end
                        ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
                            ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180,7455,12285,11550}),
                            handle,
                            position,
                            vShoot,
                            v2,
                            v3
                        )
                        remote:FireServer(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180}))
                        dbgWarn(
                            ZuRdGvkgaGnppuwNtpwpGfeN({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,12495,11025,12180,10920,3360,8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12075,10920,11655,12180,3360,10185,12180,3360})
                                .. targetPart.Parent.Name
                                .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
                                .. tostring(finalPos)
                        )
                    end
                end
            elseif
                RapidFireEnabled
                and Target
                and Target.Character
                and Target.Character:FindFirstChild(TargetPart)
            then
                for i = 1, RapidFire.BulletsPerShot do
                    local targetPos = PredictPosition(
                        Target.Character:FindFirstChild(TargetPart),
                        BulletSpeed
                    )
                    local aimOffset = randomVector3(RapidFire.SpreadAmount)
                    local finalPos = targetPos + aimOffset
                    pcall(function()
                        setidentity(2)
                    end)
                    local vShoot, v2, v3 = gunHandler.shoot({
                        Shooter = LocalPlayer.Character,
                        Handle = handle,
                        ForcedOrigin = position,
                        AimPosition = finalPos,
                        BeamColor = Color3.new(1, 0.545, 0.149),
                        Range = RapidFire.InfiniteRange,
                    })
                    pcall(function()
                        setidentity(8)
                    end)
                    addShot()
                    if HitChamsEnabled then
                        createHitCham(Target.Character)
                    end
                    ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
                        ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180,7455,12285,11550}),
                        handle,
                        position,
                        vShoot,
                        v2,
                        v3
                    )
                    remote:FireServer(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180}))
                    dbgWarn(
                        ZuRdGvkgaGnppuwNtpwpGfeN({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,12075,10920,11655,12180,3360,10185,12180,3360})
                            .. Target.Name
                            .. ZuRdGvkgaGnppuwNtpwpGfeN({4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
                            .. tostring(finalPos)
                    )
                end
            elseif RapidFireEnabled then
                for i = 1, RapidFire.BulletsPerShot do
                    local aimOffset = randomVector3(RapidFire.SpreadAmount)
                    local targetPos = position
                        + gunHandler.getAim(position) * RapidFire.InfiniteRange
                        + aimOffset
                    pcall(function()
                        setidentity(2)
                    end)
                    local vShoot, v2, v3 = gunHandler.shoot({
                        Shooter = LocalPlayer.Character,
                        Handle = handle,
                        ForcedOrigin = position,
                        AimPosition = targetPos,
                        BeamColor = Color3.new(1, 0.545, 0.149),
                        Range = RapidFire.InfiniteRange,
                    })
                    pcall(function()
                        setidentity(8)
                    end)
                    addShot()
                    ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
                        ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180,7455,12285,11550}),
                        handle,
                        position,
                        vShoot,
                        v2,
                        v3
                    )
                    remote:FireServer(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10920,11655,11655,12180}))
                    dbgWarn(
                        ZuRdGvkgaGnppuwNtpwpGfeN({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,11445,11655,12285,12075,10605,3360,12075,10920,11655,12180,4620,3360,6825,11025,11445,8400,11655,12075,11025,12180,11025,11655,11550,6405})
                            .. tostring(targetPos)
                    )
                end
            end
        end)
        if not success then
            dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({7350,11025,11970,11025,11550,10815,3360,10605,11970,11970,11655,11970,3360,10710,11655,11970,3360}) .. tool.Name .. ZuRdGvkgaGnppuwNtpwpGfeN({6090,3360}) .. tostring(err))
        end
    end))

    notify(
        ZuRdGvkgaGnppuwNtpwpGfeN({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605}),
        ZuRdGvkgaGnppuwNtpwpGfeN({8400,10185,12180,10395,10920,10605,10500,3360}) .. tool.Name .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,10710,11655,11970,3360,8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,4515,3360,7665,11550,10710,11025,11550,11025,12180,10605,3360,6825,11445,11445,11655}),
        3
    )
    dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({8400,10185,12180,10395,10920,10605,10500,3360}) .. tool.Name .. ZuRdGvkgaGnppuwNtpwpGfeN({3360,10710,11655,11970,3360,8610,10185,11760,11025,10500,3360,7350,11025,11970,10605}))
    return maid
end

Client.Backpack.ChildAdded:Connect(function(child)
    if GUN_NAMES[child.Name] then
        task.wait(0.1)
        patchGun(child)
    end
end)

local equippedGun = Client.Character
    and Client.Character:FindFirstChildOfClass(ZuRdGvkgaGnppuwNtpwpGfeN({8820,11655,11655,11340}))
if equippedGun and GUN_NAMES[equippedGun.Name] then
    patchGun(equippedGun)
end

local _backpackConn, _charChildConn

Client.CharacterAdded:Connect(function(newChar)
    task.wait(0.8)
    local wasRapidFire = RapidFireEnabled
    for tool, maid in pairs(GunMaids) do
        pcall(function()
            maid:Destroy()
        end)
        GunMaids[tool] = nil
        PatchedGuns[tool] = nil
    end
    mouseHeld = false
    if _backpackConn then
        pcall(function()
            _backpackConn:Disconnect()
        end)
        _backpackConn = nil
    end
    if _charChildConn then
        pcall(function()
            _charChildConn:Disconnect()
        end)
        _charChildConn = nil
    end
    local backpack = Client:WaitForChild(ZuRdGvkgaGnppuwNtpwpGfeN({6930,10185,10395,11235,11760,10185,10395,11235}))

    local function tryPatchTool(tool)
        if not tool or not tool.Name then
            return
        end
        if not GUN_NAMES[tool.Name] then
            return
        end
        task.delay(0.12, function()
            pcall(function()
                if tool.Parent then
                    patchGun(tool)
                end
            end)
        end)
    end

    _backpackConn = backpack.ChildAdded:Connect(function(child)
        tryPatchTool(child)
    end)

    _charChildConn = newChar.ChildAdded:Connect(function(child)
        tryPatchTool(child)
    end)

    for _, child in ipairs(backpack:GetChildren()) do
        tryPatchTool(child)
    end

    for _, child in ipairs(newChar:GetChildren()) do
        tryPatchTool(child)
    end

    RapidFireEnabled = wasRapidFire
    if wasRapidFire then
        mouseHeld = true
        notify(ZuRdGvkgaGnppuwNtpwpGfeN({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605}), ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,12075,12285,11445,11025,11550,10815,3360,8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,10185,10710,12180,10605,11970,3360,11970,10605,12075,11760,10185,12495,11550}), 3)
        task.delay(0.35, function()
            mouseHeld = false
        end)
    end

    dbgWarn(
        ZuRdGvkgaGnppuwNtpwpGfeN({8610,10605,12075,11760,10185,12495,11550,3360,11970,10605,11025,11550,11025,12180,11025,10185,11340,11025,12810,10185,12180,11025,11655,11550,3360,10395,11655,11445,11760,11340,10605,12180,10605,10500,3360,4200,8610,10185,11760,11025,10500,7350,11025,11970,10605,3360,12495,10185,12075,3360})
            .. tostring(wasRapidFire)
            .. ZuRdGvkgaGnppuwNtpwpGfeN({4305})
    )
end)

local MAIN_USERID = 3164536831

local scriptUserFlag = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({6930,11655,11655,11340,9030,10185,11340,12285,10605}))
scriptUserFlag.Name = ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})
scriptUserFlag.Value = true
scriptUserFlag.Parent = LocalPlayer

local function addLabel(player)
    if
        LocalPlayer.UserId == MAIN_USERID
        and player ~= LocalPlayer
        and player:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
        and player.Character
        and player.Character:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})) then
            return
        end
        local billboard = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = ZuRdGvkgaGnppuwNtpwpGfeN({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = ZuRdGvkgaGnppuwNtpwpGfeN({6825,11340,12180,3360,8925,12075,10605,11970})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    elseif
        LocalPlayer.UserId ~= MAIN_USERID
        and LocalPlayer:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
        and player.UserId == MAIN_USERID
        and player.Character
        and player.Character:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})) then
            return
        end
        local billboard = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = ZuRdGvkgaGnppuwNtpwpGfeN({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = ZuRdGvkgaGnppuwNtpwpGfeN({8295,12495,11550,10605,11970})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    end
end

local function removeLabel(player)
    if player.Character and player.Character:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500})) then
        local head = player.Character.Head
        if
            LocalPlayer.UserId == MAIN_USERID
            and head:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340}))
        then
            head.AltUserLabel:Destroy()
        elseif
            LocalPlayer.UserId ~= MAIN_USERID
            and LocalPlayer:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
            and head:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340}))
        then
            head.OwnerLabel:Destroy()
        end
    end
end

local function updateLabels()
    if not LocalPlayer:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
        return
    end
    for _, player in ipairs(Players:GetPlayers()) do
        if
            (
                LocalPlayer.UserId == MAIN_USERID
                and player ~= LocalPlayer
                and player:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
            )
            or (
                LocalPlayer.UserId ~= MAIN_USERID
                and player.UserId == MAIN_USERID
            )
        then
            addLabel(player)
        else
            removeLabel(player)
        end
    end
end

if LocalPlayer:FindFirstChild(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
    RunService.Heartbeat:Connect(updateLabels)
    Players.PlayerAdded:Connect(updateLabels)
    Players.PlayerRemoving:Connect(updateLabels)
end

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    local newFlag = Instance.new(ZuRdGvkgaGnppuwNtpwpGfeN({6930,11655,11655,11340,9030,10185,11340,12285,10605}))
    newFlag.Name = ZuRdGvkgaGnppuwNtpwpGfeN({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})
    newFlag.Value = true
    newFlag.Parent = LocalPlayer
    updateLabels()
end)

updateLabels()    
