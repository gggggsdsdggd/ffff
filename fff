			end
		end
	end
	-- TP Walk Key
	if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
		tpwalking = not tpwalking
		notify(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'####################################################################################','################################################################################','################################','#######################################################################################','#################################################################################################','############################################################################################################','###########################################################################################################'}),
			tpwalking and hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#####################################################################','##############################################################################################################','#################################################################################################','##################################################################################################','############################################################################################################','#####################################################################################################','####################################################################################################','################################','######################################################################################################################','#########################################################################################################','#################################################################################################','################################','###########################################################################################################','#####################################################################################################','#########################################################################################################################','##################################################################################################','#########################################################################################################','##############################################################################################################','####################################################################################################'}) or hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#