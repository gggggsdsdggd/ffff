    Name = ZuRdGvkgaGnppuwNtpwpGfeN({8715,10605,12180,3360,7350,11655,11970,10395,10605,3360,7560,11025,12180,3360,7875,10605,12705,3360,4200,8400,11970,10605,12075,12075,3360,10185,3360,11235,10605,12705,3360,10185,10710,12180,10605,11970,3360,10395,11340,11025,10395,11235,11025,11550,10815,4305}),
    Callback = function()
        local listening = true
        notify(ZuRdGvkgaGnppuwNtpwpGfeN({7350,11655,11970,10395,10605,3360,7560,11025,12180,3360,7875,10605,12705}), ZuRdGvkgaGnppuwNtpwpGfeN({8400,11970,10605,12075,12075,3360,10185,3360,11235,10605,12705,3360,12180,11655,3360,12075,10605,12180,4830,4830,4830}), 5)
        local connection
        connection = UserInputService.InputBegan:Connect(
            function(input, processed)
                if
                    processed
                    or input.UserInputType ~= Enum.UserInputType.Keyboard
                    or not listening
                then
                    return
                end
                listening = false
                Keybinds.ForceHit = input.KeyCode
                ForceHitKeyLabel.Text = ZuRdGvkgaGnppuwNtpwpGfeN({7350,11655,11970,10395,10605,3360,7560,11025,12180,3360,7875,10605,12705,6090,3360}) .. input.KeyCode.Name
                dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({7350,11655,11970,10395,10605,3360,7560,11025,12180,3360,11235,10605,12705,3360,12075,10605,12180,3360,12180,11655,6090,3360}) .. input.KeyCode.Name)
                connection:Disconnect()
            end
        )
    end,
})

TargetAimSection:AddToggle({
    Name = ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}),
    Default = false,
    Callback = function(state)
        BulletTPEnabled = state
        Target = nil
        TargetAll = false
        if not state then
            Line.Visible = false
            Circle.Visible = false
            Text.Visible = false
            ViewEnabled = false
            CurrentCamera.CameraSubject = Client.Character
                and Client.Character:FindFirstChildWhichIsA(ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500}))
            notify(ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), ZuRdGvkgaGnppuwNtpwpGfeN({7140,11025,12075,10185,10290,11340,10605,10500}), 3)
        else
            notify(ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), ZuRdGvkgaGnppuwNtpwpGfeN({7245,11550,10185,10290,11340,10605,10500}), 3)
            if Target then
                notify(ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName, 3)
            end
        end
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,12180,11655,10815,10815,11340,10605,10500,6090,3360}) .. tostring(state))
    end,
})

TargetAimSection:AddDropdown({
    Name = ZuRdGvkgaGnppuwNtpwpGfeN({8820,10185,11970,10815,10605,12180,3360,8400,10185,11970,12180}),
    Default = ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}),
    Values = { ZuRdGvkgaGnppuwNtpwpGfeN({7560,10605,10185,10500}), ZuRdGvkgaGnppuwNtpwpGfeN({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}) },
    Callback = function(value)
        TargetPart = value
        Settings.Aimpart = value
        if ViewEnabled and Target and Target.Character then
            local targetPart = Target.Character:FindFirstChild(TargetPart)
            if targetPart then
                CurrentCamera.CameraSubject = targetPart
            end
        end
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({8715,10605,11340,10605,10395,12180,10605,10500,3360,12180,10185,11970,10815,10605,12180,3360,11760,10185,11970,12180,6090,3360}) .. tostring(value))
    end,
})

