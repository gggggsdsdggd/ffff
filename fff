-- Notification Function
local function notify(title, text, dur)
	if SEND_NOTIFICATIONS then
		pcall(function()
			StarterGui:SetCore(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************','*****************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','******************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}), {
				Title = title,
				Text = text,
				Duration = dur or 3,
			})
		end)
	end
end

-- Debug Function
local function dbgWarn(msg)
	if DEBUG_MODE then
		warn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'*******************************************************************************************','********************************************************************','*********************************************************************','******************************************************************','*************************************************************************************','***********************************************************************','*********************************************************************************************','********************************'}) .. tostring(msg))
	end
end

-- Adonis Bypass
local getinfo = getinfo or debug.getinfo
local DEBUG = false
local Hooked = {}

local Detected, Kill

setthreadidentity(2)

for i, v in getgc(true) do
    if typeof(v) == vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}) then
        local DetectFunc = rawget(v, vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************','*****************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
        local KillFunc = rawget(v, vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***************************************************************************','*********************************************************************************************************','************************************************************************************************************','************************************************************************************************************'}))
    
        if typeof(DetectFunc) == vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}) and not Detected then
            Detected = DetectFunc
            
            local Old; Old = hookfunction(Detected, function(Action, Info, NoCrash)
                if Action ~= vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'***********************************************************************************************'}) then
                    if DEBUG then
                        warn(`Adonis AntiCheat flagged\nMethod: {Action}\nInfo: {Info}`)
                    end
                end
                
                return true
            end)

            table.insert(Hooked, Detected)
        end

        if rawget(v, vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'**************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'})) and rawget(v, vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************************************************************','******************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************'})) and typeof(KillFunc) == vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}) and not Kill then
            Kill = KillFunc
            local Old; Old = hookfunction(Kill, function(Info)
                if DEBUG then
                    warn(`Adonis AntiCheat tried to kill (fallback): {Info}`)
                end
            end)

            table.insert(Hooked, Kill)
        end
    end
end

local Old; Old = hookfunction(getrenv().debug.info, newcclosure(function(...)
    local LevelOrFunc, Info = ...

    if Detected and LevelOrFunc == Detected then
        if DEBUG then
            warn(`zins | adonis bypassed`)
        end

        return coroutine.yield(coroutine.running())
    end
    
    return Old(...)
end))
