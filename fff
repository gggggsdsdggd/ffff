local Settings = {
    Aimpart = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}),
    FOVRadius = 60,
    Prediction = 0.1,
    Color = Color3.fromRGB(255, 255, 255),
}

-- Function to create and weld a part
local function createWeldedPart(rootPart, size, positionOffset, color, material)
    local part = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8400,10185,11970,12180}))
    part.Size = size
    part.Color = color
    part.Material = material
    part.Anchored = false
    part.CanCollide = false
    part.CFrame = rootPart.CFrame * CFrame.new(positionOffset)
    part.Parent = workspace

    local weld = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9135,10605,11340,10500}))
    weld.Part0 = rootPart
    weld.Part1 = part
    weld.C0 = CFrame.new(positionOffset)
    weld.Parent = part

    return part, weld
end

-- Function to set up structure and spin it
local function setupStructure(character)
    local rootPart = character:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))

    local parts = {
        {Vector3.new(0.3, 4, 0.3), Vector3.new(0, 6, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(-1, 4, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(1, 8, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(1, 6, 0)},
        {Vector3.new(0.3, 2, 0.3), Vector3.new(2, 5, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(-1, 6, 0)},
        {Vector3.new(0.3, 2, 0.3), Vector3.new(-2, 7, 0)},
    }

    for _, p in ipairs(parts) do
        local part, weld = createWeldedPart(
            rootPart,
            p[1],
            p[2],
            SpinStructureColor,
            Enum.Material.Neon
        )
        table.insert(PartsAndWelds, {weld = weld, offset = p[2], part = part})
    end

    local time = 0

    SpinConnection = game:GetService(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,12285,11550,8715,10605,11970,12390,11025,10395,10605})).Heartbeat:Connect(function(dt)
        if not SpinStructureEnabled or not rootPart.Parent then
            SpinConnection:Disconnect()
            SpinConnection = nil
            return
        end
        time += dt
        local angle = time * SpinSpeed
        for _, partData in ipairs(PartsAndWelds) do
            local offset = partData.offset
            local rotatedOffset = CFrame.new(0, offset.Y, 0)
                * CFrame.Angles(0, angle, 0)
                * CFrame.new(offset.X, 0, offset.Z)
            partData.weld.C0 = rotatedOffset
        end
    end)
end

-- Cleanup
local function removeStructure()
    for _, data in ipairs(PartsAndWelds) do
        if data.part then
            data.part:Destroy()
        end
    end
    PartsAndWelds = {}
    if SpinConnection then
        SpinConnection:Disconnect()
        SpinConnection = nil
    end
end

-- Spinning Swastika Controls
SpinStructureSection:AddToggle({
    Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,12495,10185,12075,12180,11025,11235,10185}),
    Default = false,
    Callback = function(state)
        SpinStructureEnabled = state
        if state then
            if game.Players.LocalPlayer.Character then
                setupStructure(game.Players.LocalPlayer.Character)
            end
            game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
                if SpinStructureEnabled then
                    task.wait(1)
                    setupStructure(char)
                end
            end)
        else
            removeStructure()
        end
    end,
})

SpinStructureSection:AddColorPicker({
    Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,12495,10185,12075,12180,11025,11235,10185,3360,7035,11655,11340,11655,11970}),
    Default = SpinStructureColor,
    Callback = function(value)
        SpinStructureColor = value
        for _, data in ipairs(PartsAndWelds) do
            if data.part then
                data.part.Color = value
            end
        end
    end,
})

SpinStructureSection:AddSlider({
    Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,11760,10605,10605,10500}),
    Min = 1,
    Max = 10,
    Default = SpinSpeed,
    Callback = function(value)
        SpinSpeed = value
    end,
})

