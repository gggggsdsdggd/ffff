-- Notification Function
local function notify(title, text, dur)
	if SEND_NOTIFICATIONS then
		pcall(function()
			StarterGui:SetCore(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###################################################################################','#####################################################################################################','##############################################################################################################','####################################################################################################','##############################################################################','###############################################################################################################','####################################################################################################################','#########################################################################################################','######################################################################################################','#########################################################################################################','###################################################################################################','#################################################################################################','####################################################################################################################','#########################################################################################################','###############################################################################################################','##############################################################################################################'}), {
				Title = title,
				Text = text,
				Duration = dur or 3,
			})
		end)
	end
end

-- Debug Function
local function dbgWarn(msg)
	if DEBUG_MODE then
		warn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###########################################################################################','####################################################################','#####################################################################','##################################################################','#####################################################################################','#######################################################################','#############################################################################################','################################'}) .. tostring(msg))
	end
end

