end)

-- Mouse Input for Rapid Fire
UserInputService.InputBegan:Connect(function(input, gpe)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		local equippedGun = Client.Character
			and Client.Character:FindFirstChildOfClass(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
		if equippedGun and GUN_NAMES[equippedGun.Name] then
			mouseHeld = true
			dbgWarn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'*****************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','********************************************************************************************************','*****************************************************************************************************','************************************************************************************************************','****************************************************************************************************','********************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. equippedGun.Name)
		end
	end
end)

UserInputService.InputEnded:Connect(function(input, gpe)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		mouseHeld = false
		dbgWarn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'*****************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
	end
end)

-- Detect Gun Unequip
Client.Character.ChildRemoved:Connect(function(child)
	if child:IsA(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'})) and GUN_NAMES[child.Name] then
		mouseHeld = false
		if GunMaids[child] then
			GunMaids[child]:Destroy()
			GunMaids[child] = nil
			PatchedGuns[child] = nil
			dbgWarn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. child.Name .. cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************************','********************************','***************************************************************************************************','*********************************************************