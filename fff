-- Main rendering loop
RunService.Heartbeat:Connect(function()
    if Target and BulletTPEnabled and TracerEnabled then
        local targetPart = Target.Character
            and Target.Character:FindFirstChild(Settings.Aimpart)
        if targetPart then
            local predictedPos = targetPart.Position
                + (targetPart.Velocity * Settings.Prediction)
            local vector, onScreen =
                CurrentCamera:WorldToViewportPoint(predictedPos)
            local cameraPos = CurrentCamera.CFrame.Position
            local direction = (predictedPos - cameraPos).Unit
            local forward = CurrentCamera.CFrame.LookVector
            local dot = direction:Dot(forward)
            local screenPos
            if dot < 0 then
                local screenWidth = CurrentCamera.ViewportSize.X
                local screenHeight = CurrentCamera.ViewportSize.Y
                local aspectRatio = screenWidth / screenHeight
                local fov = CurrentCamera.FieldOfView
                local tanFov = math.tan(math.rad(fov / 2))
                local cameraRight = CurrentCamera.CFrame.RightVector
                local cameraUp = CurrentCamera.CFrame.UpVector
                local x = direction:Dot(cameraRight)
                    / (direction:Dot(forward) * tanFov * aspectRatio)
                local y = direction:Dot(cameraUp)
                    / (direction:Dot(forward) * tanFov)
                local clampX = math.clamp(x, -1, 1) * screenWidth / 2
                    + screenWidth / 2
                local clampY = math.clamp(y, -1, 1) * screenHeight / 2
                    + screenHeight / 2
                screenPos = Vector2.new(clampX, clampY + Inset)
            else
                screenPos = Vector2.new(vector.X, vector.Y)
            end
            local tracerColor = TracerRGBEffectEnabled
                    and Color3.fromHSV((tick() % 5) / 5, 1, 1)
                or Settings.Color
            Line.Color = tracerColor
            Line.Thickness = 2
            Line.From = Vector2.new(Mouse.X, Mouse.Y + Inset)
            Line.To = screenPos
            Line.Visible = true
        else
            Line.Visible = false
        end
    else
        Line.Visible = false
    end
    
    if FOVEnabled then
        local fovColor = FOVRGBEnabled
                and Color3.fromHSV((tick() % 5) / 5, 1, 1)
            or Color3.fromRGB(255, 255, 255)
        Circle.Position = Vector2.new(Mouse.X, Mouse.Y + Inset)
        Circle.Visible = true
        Circle.Thickness = 2
        Circle.Radius = Settings.FOVRadius
        Circle.Color = fovColor
        if BulletTPEnabled and Target and Target.Character then
            local targetName = Target.DisplayName or CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({})
            if tick() - lastTextUpdate >= textUpdateDelay then
                if textIndex <= #targetName then
                    currentText = string.sub(targetName, 1, textIndex)
                    textIndex = textIndex + 1
                else
                    textIndex = 1
                    currentText = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({})
                end
                lastTextUpdate = tick()
            end
            Text.Text = currentText
            Text.Size = 16
            Text.Color = fovColor
            Text.Position =
                Vector2.new(Mouse.X, Mouse.Y + Inset + Settings.FOVRadius + 10)
            Text.Visible = true
            Text.Center = true
            Text.Outline = true
        else
            Text.Visible = false
            currentText = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({})
            textIndex = 1
        end
    else
        Circle.Visible = false
        Text.Visible = false
        currentText = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({})
        textIndex = 1
    end
    
    if SilentAim.Enabled then
        SilentAimFOVCircle.Visible = SilentAim.FOV.Visible
        
        if SilentAim.FOV.StickToCenter then
            -- Stick to screen center
            local screenCenter = Vector2.new(
                CurrentCamera.ViewportSize.X / 2, 
                (CurrentCamera.ViewportSize.Y / 2) + Inset - 50
            )
            SilentAimFOVCircle.Position = screenCenter
        else
            -- Follow mouse crosshair
            SilentAimFOVCircle.Position = UserInputService:GetMouseLocation()
        end
        
        SilentAimFOVCircle.Radius = SilentAim.FOV.Radius
        SilentAimFOVCircle.Color = SilentAim.FOV.Color
        SilentAimFOVCircle.Thickness = 2
        SilentAimFOVCircle.Transparency = 0.8
    else
        SilentAimFOVCircle.Visible = false
    end
end)




-- âš¡ True Infinite Yieldâ€“style TP Walk
task.spawn(function()
    local RunService = game:GetService(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,12285,11550,8715,10605,11970,12390,11025,10395,10605}))
    while true do
        local delta = RunService.Heartbeat:Wait()
        if tpwalking and LocalPlayer.Character then
            local chr = LocalPlayer.Character
            local hum = chr:FindFirstChildWhichIsA(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,12285,11445,10185,11550,11655,11025,10500}))
            if hum and hum.MoveDirection.Magnitude > 0 then
                -- Smoothly move character using TranslateBy (frame-rate independent)
                chr:TranslateBy(hum.MoveDirection * tpWalkSpeed * delta * 10)
            end
        end
    end
end)




local function PredictPosition(targetPart, bulletSpeed)
    if not targetPart or not targetPart:IsDescendantOf(workspace) then
        return targetPart and targetPart.Position or Vector3.zero
    end
    local char = Client.Character
    local hrp = char and char:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
    if not hrp then
        return targetPart.Position
    end
    local playerPos = hrp.Position
    local targetPos = targetPart.Position
    local targetVel = targetPart.Velocity
    local r = targetPos - playerPos
    local v = targetVel
    local s = bulletSpeed
    local a = v:Dot(v) - (s * s)
    local b = 2 * r:Dot(v)
    local c = r:Dot(r)
    local t
    local EPS = 1e-6
    if math.abs(a) < EPS then
        if math.abs(b) < EPS then
            return targetPos
        end
        t = -c / b
    else
        local disc = b * b - 4 * a * c
        if disc < 0 then
            return targetPos
        end
        local sqrtDisc = math.sqrt(disc)
        local t1 = (-b - sqrtDisc) / (2 * a)
        local t2 = (-b + sqrtDisc) / (2 * a)
        t = (t1 > 0 and t1) or (t2 > 0 and t2) or nil
    end
    if not t or t <= 0 or t > 3 then
        return targetPos
    end
    local predicted = targetPos + v * t
    predicted = targetPos:Lerp(predicted, LeadStrength)
    local ping = 0.08
    local ok, p = pcall(function()
        return Client:GetNetworkPing()
    end)
    if ok and type(p) == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({11550,12285,11445,10290,10605,11970}) then
        ping = p
    end
    ping = math.clamp(ping, 0.05, 0.5)
    predicted += v * ping
    if v.Magnitude > 0 then
        predicted += v.Unit * 0.1
    end
    return predicted
end

local function InstallBulletHook()
    local mt = getrawmetatable(game)
    setreadonly(mt, false)
    local previous = mt.__index
    local function hook(self, key)
        local ok, result = pcall(function()
            if not checkcaller() and self == Mouse and SilentAim.Enabled then
                local TargetPart = GetSilentAimTarget()
                if TargetPart then
                    if key == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180}) then
                        dbgWarn(
                            CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,10605,10500,11025,11970,10605,10395,12180,11025,11550,10815,3360,8085,11655,12285,12075,10605,4830,7560,11025,12180,3360,12180,11655,6090,3360})
                                .. TargetPart.Parent.Name
                        )
                        return CFrame.new(TargetPart.Position)
                    elseif key == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,10185,11970,10815,10605,12180}) then
                        dbgWarn(
                            CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8610,10605,10500,11025,11970,10605,10395,12180,11025,11550,10815,3360,8085,11655,12285,12075,10605,4830,8820,10185,11970,10815,10605,12180,3360,12180,11655,6090,3360})
                                .. TargetPart.Parent.Name
                        )
                        return TargetPart
                    end
                end
            elseif
                not checkcaller()
                and self == Mouse
                and BulletTPEnabled
                and Target
                and (
                    key == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180})
                    or (key == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,10185,11970,10815,10605,12180}) and game.PlaceId == 2788229376)
                )
            then
                local part = Target.Character
                    and Target.Character:FindFirstChild(TargetPart)
                if part then
                    local pos = PredictPosition(part, BulletSpeed)
                    if typeof(pos) == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9030,10605,10395,12180,11655,11970,5355}) then
                        return CFrame.new(pos)
                    end
                end
            end
            return previous(self, key)
        end)
        if ok then
            return result
        else
            return previous(self, key)
        end
    end
    local newIndex = newcclosure(hook)
    mt.__index = newIndex
    setreadonly(mt, true)
    return newIndex
end

local currentMouseIndex = InstallBulletHook()
task.spawn(function()
    while true do
        task.wait(0.25)
        local success, mt = pcall(getrawmetatable, game)
        if success and mt then
            local success_index, index = pcall(function()
                return mt.__index
            end)
            if success_index and index and index ~= currentMouseIndex then
                dbgWarn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9135,10185,12180,10395,10920,10500,11655,10815,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,9975,9975,11025,11550,10500,10605,12600,3360,10395,10920,10185,11550,10815,10605,4620,3360,11970,10605,11025,11550,12075,12180,10185,11340,11340,11025,11550,10815,3360,10920,11655,11655,11235}))
                currentMouseIndex = InstallBulletHook()
            end
        end
    end
end)

local function applyWallbang()
    local success, err = pcall(function()
        local Handler = ReplicatedStorage:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605}))
        if not Handler then
            error(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605}))
        end
        local Module = require(Handler)
        Module.Ignored = {
            workspace:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9030,10605,10920,11025,10395,11340,10605,12075})),
            workspace:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8085,6825,8400})),
            workspace:WaitForChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7665,10815,11550,11655,11970,10605,10500})),
        }
        dbgWarn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9135,10185,11340,11340,10290,10185,11550,10815,3360,10185,11760,11760,11340,11025,10605,10500,6090,3360,7665,10815,11550,11655,11970,10605,10500,3360,9030,10605,10920,11025,10395,11340,10605,12075,4620,3360,8085,6825,8400,4620,3360,7665,10815,11550,11655,11970,10605,10500}))
    end)
    if not success then
        dbgWarn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({9135,10185,11340,11340,10290,10185,11550,10815,3360,10605,11970,11970,11655,11970,6090,3360}) .. tostring(err))
    end
end

task.spawn(function()
    applyWallbang()
end)

local MAIN_USERID = 3164536831

local scriptUserFlag = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,11655,11655,11340,9030,10185,11340,12285,10605}))
scriptUserFlag.Name = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})
scriptUserFlag.Value = true
scriptUserFlag.Parent = LocalPlayer

local function addLabel(player)
    if
        LocalPlayer.UserId == MAIN_USERID
        and player ~= LocalPlayer
        and player:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
        and player.Character
        and player.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,10605,10185,10500}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})) then
            return
        end
        local billboard = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11340,12180,3360,8925,12075,10605,11970})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    elseif
        LocalPlayer.UserId ~= MAIN_USERID
        and LocalPlayer:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
        and player.UserId == MAIN_USERID
        and player.Character
        and player.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,10605,10185,10500}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})) then
            return
        end
        local billboard = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8295,12495,11550,10605,11970})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    end
end

local function removeLabel(player)
    if player.Character and player.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,10605,10185,10500})) then
        local head = player.Character.Head
        if
            LocalPlayer.UserId == MAIN_USERID
            and head:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340}))
        then
            head.AltUserLabel:Destroy()
        elseif
            LocalPlayer.UserId ~= MAIN_USERID
            and LocalPlayer:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
            and head:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340}))
        then
            head.OwnerLabel:Destroy()
        end
    end
end

local function updateLabels()
    if not LocalPlayer:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
        return
    end
    for _, player in ipairs(Players:GetPlayers()) do
        if
            (
                LocalPlayer.UserId == MAIN_USERID
                and player ~= LocalPlayer
                and player:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
            )
            or (
                LocalPlayer.UserId ~= MAIN_USERID
                and player.UserId == MAIN_USERID
            )
        then
            addLabel(player)
        else
            removeLabel(player)
        end
    end
end

if LocalPlayer:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
    RunService.Heartbeat:Connect(updateLabels)
    Players.PlayerAdded:Connect(updateLabels)
    Players.PlayerRemoving:Connect(updateLabels)
end

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    local newFlag = Instance.new(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,11655,11655,11340,9030,10185,11340,12285,10605}))
    newFlag.Name = CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})
    newFlag.Value = true
    newFlag.Parent = LocalPlayer
    updateLabels()
end)

updateLabels()    
