				.. gun.Name
				.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'############################################','################################','#####################################################################################################################','###################################################################################################################','#########################################################################################################','##############################################################################################################','#######################################################################################################','################################','######################################################################################################','#################################################################################################','############################################################################################################','############################################################################################################','##################################################################################################','#################################################################################################','###################################################################################################','###########################################################################################################','################################','################################################################################################################','###############################################################################################################','###################################################################################################################','#########################################################################################################','####################################################################################################################','#########################################################################################################','###############################################################################################################','##############################################################################################################'})
		)
	end
	local target = getClosestTargetToPlayer(plr)
	if
		not target
		or not target.Character
		or not target.Character:FindFirstChild(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'########################################################################','#####################################################################################################################','#############################################################################################################','#################################################################################################','##############################################################################################################','###############################################################################################################','#########################################################################################################','####################################################################################################','##################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################','################################################################################','#################################################################################################','##################################################################################################################','####################################################################################################################'}))
	then
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','##############################################################################','#########################################################################