        and args[1] == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10920,11655,11655,12180})
        and BulletTPEnabled
        and Target
        and Target.Character
    then
        lastShotTime = tick()
        dbgWarn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10920,11655,12180,3360,10710,11025,11970,10605,10500,3360,11025,11550,3360,8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,11655,11550,3360}) .. Target.Name)

        -- ðŸŸ£ Trigger HitCham on Target Aim shots
        if HitChamsEnabled and Target and Target.Character then
            createHitCham(Target.Character)
            notify(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180,3360,7035,10920,10185,11445,12075}), CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11760,11760,11340,11025,10605,10500,3360,12180,11655,3360}) .. Target.Name, 2)
        end
    end

        if typeof(method) == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({12075,12180,11970,11025,11550,10815}) and method:lower() == CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({11235,11025,10395,11235}) then
            notify(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,11550,12180,11025,4725,7875,11025,10395,11235}), CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7875,11025,10395,11235,3360,10185,12180,12180,10605,11445,11760,12180,3360,10290,11340,11655,10395,11235,10605,10500}), 3)
            return nil
        end
        return old(self, ...)
