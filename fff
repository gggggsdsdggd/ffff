				dbgWarn(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name)
				connection:Disconnect()
			end
		)
	end,
})

