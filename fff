					.. plr.Name
					.. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'##########################################################','################################'})
					.. tostring(vShoot)
			)
			return
		end
		ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###################################################################################','########################################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################'}),
			handle,
			muzzle.WorldPosition,
			vShoot,
			v2,
			v3
		)
		if remote then
			remote:FireServer(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'###################################################################################','########################################################################################################','###############################################################################################################','###############################################################################################################','####################################################################################################################'}))
		end
		dbgWarn(
			hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','######################################################################','########################################################################################