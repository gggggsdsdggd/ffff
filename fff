	return nil
end

-- Process Assist Input (called on text change)
local lastProcessedInput = hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({})
local function processAssistInput(value)
	if value == hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({}) then
		notify(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################'}), hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'#########################################################################################','###############################################################################################################','#####################################################################################################################','################################','#############################################################################################################','#####################################################################################################################','###################################################################################################################','####################################################################################################################','################################','################################################################################################################','##################################################################################################################','###############################################################################################################','######################################################################################################################','#########################################################################################################','####################################################################################################','#####################################################################################################','################################','#################################################################################################','################################','################################################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################','##############################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################'}), 3)
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################################################','##################################################################################################################','###############################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#########################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','####################################################################################################################','##########################################################','################################','#####################################################################','#############################################################################################################','################################################################################################################','####################################################################################################################','#########################################################################################################################','################################','#########################################################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','####################################################################################################################'}))
		return false
	end
	if value == lastProcessedInput then
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################################################################################################','##################################################################################################################','###############################################################################################################','###################################################################################################','#####################################################################################################','###################################################################################################################','###################################################################################################################','#################################################################','###################################################################################################################','###################################################################################################################','#########################################################################################################','###################################################################################################################','####################################################################################################################','#########################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','#########################################################################