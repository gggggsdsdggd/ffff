			notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,9030,11025,12075,12285,10185,11340,12075}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7350,8295,9030,3360,10395,11025,11970,10395,11340,10605,3360,10500,11025,12075,10185,10290,11340,10605,10500}), 3)
		end
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7350,8295,9030,3360,10395,11025,11970,10395,11340,10605,3360,12180,11655,10815,10815,11340,10605,10500,6090,3360}) .. tostring(state))
	end,
})

