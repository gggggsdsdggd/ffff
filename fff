-- FIXED WALLBANG
local function applyWallbang()
    task.spawn(function()
        pcall(function()
            local Handler = ReplicatedStorage:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605}), 10)
            if Handler then
                local Module = require(Handler)
                Module.Ignored = {
                    workspace:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({9030,10605,10920,11025,10395,11340,10605,12075})) or Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7350,11655,11340,10500,10605,11970})),
                    workspace:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8085,6825,8400})) or Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7350,11655,11340,10500,10605,11970})),
                    workspace:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7665,10815,11550,11655,11970,10605,10500})) or Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7350,11655,11340,10500,10605,11970}))
                }
            end
        end)
    end)
end
applyWallbang()

-- MODIFIED SHOOT FUNCTION (Removed Target All and Rapid Fire logic)
local function shoot()
    if not tool or not Character or not tool.Parent or tool.Parent ~= Character then
        return
    end

    local success = pcall(function()
        v_u_1 = CFrame.new(-1, 0.4, 0)
        v_u_2 = ReplicatedStorage
        v_u_3 = LocalPlayer
        v_u_4 = tool
        v_u_5 = v_u_4:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,10185,11550,10500,11340,10605}), 1)
        v_u_6 = Character
        v_u_7 = require(v_u_2:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8085,11655,10500,12285,11340,10605,12075}), 5):WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970}), 5))
        v_u_8 = v_u_4:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6825,11445,11445,11655}), 1)
        v_u_9 = v_u_4:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8610,10185,11550,10815,10605}), 1)
        v_u_13 = v_u_4:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})) or { FireServer = function() end }
        mainRemotes = v_u_2:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,12075}))
    end)

    if not success or not v_u_5 or not v_u_7 or not v_u_8 or not v_u_9 then
        return
    end

    v_u_8.Value = 99999
    v_u_9.Value = 999999
    
    pcall(function() v_u_13:FireServer(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10920,11655,11655,12180})) end)

    local v14 = (v_u_5.CFrame * v_u_1).Position
    local muzzlePos = v14

    local aimPos
    if BulletTPEnabled and Target and Target.Character then
        local targetPart = Target.Character:FindFirstChild(TargetPart)
        if targetPart then
            aimPos = targetPart.Position + (targetPart.Velocity * LeadStrength)
        end
    else
        local mouseHit = Mouse.Hit.Position
        aimPos = mouseHit
    end

    if not aimPos then
        aimPos = Mouse.Hit.Position
    end

    local shootSuccess, v17, v18, v19 = pcall(function()
        return v_u_7.shoot({
            Shooter = v_u_6,
            Handle = v_u_5,
            ForcedOrigin = muzzlePos,
            AimPosition = aimPos,
            BeamColor = Color3.new(1, 0.545098, 0.14902),
            Range = 999999
        })
    end)

    if shootSuccess and mainRemotes then
        local evt = mainRemotes:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
        if evt and v17 then
            pcall(function()
                evt:FireServer(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({12600,7140,8715,10920,11655,8295,12180,7455,12285,11550,9450,10605,10605,7560,8295,8295,7140,9240,7140,7140}), v_u_5, muzzlePos, v17, v18, v19)
            end)
        end
    end
    
    pcall(function() v_u_13:FireServer() end)
end

-- MODIFIED TOOL INITIALIZATION (Removed Rapid Fire logic)
local function initializeTool(newTool)
    if not newTool:IsA(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8820,11655,11655,11340})) then return end
    tool = newTool
end

-- CHARACTER HANDLING
local function onCharacterChildAdded(obj)
    if obj:IsA(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8820,11655,11655,11340})) then
        task.wait(0.1)
        initializeTool(obj)
    end
end

local characterChildConn
local function onCharacterAdded(newChar)
    Character = newChar
    if characterChildConn then characterChildConn:Disconnect() end
    characterChildConn = Character.ChildAdded:Connect(onCharacterChildAdded)
    
    task.spawn(function()
        task.wait(0.1)
        local equippedTool = nil
        for _, child in ipairs(Character:GetChildren()) do
            if child:IsA(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8820,11655,11655,11340})) then
                equippedTool = child
                break
            end
        end
        if equippedTool then
            initializeTool(equippedTool)
        end
    end)
end

if LocalPlayer.Character then onCharacterAdded(LocalPlayer.Character) end
LocalPlayer.CharacterAdded:Connect(onCharacterAdded)

LocalPlayer:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6930,10185,10395,11235,11760,10185,10395,11235}))
LocalPlayer.Backpack.ChildAdded:Connect(function(obj)
    if obj:IsA(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8820,11655,11655,11340})) then
        obj.Equipped:Connect(function()
            task.wait(0.05)
            initializeTool(obj)
        end)
    end
end)

-- GetClosestTarget
local function GetClosestTarget()
    local Closest, MinDist = nil, math.huge
    local MousePos = Vector2.new(Mouse.X, Mouse.Y)
    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= Client and Player.Character then
            local Root = Player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
            local Humanoid = Player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,12285,11445,10185,11550,11655,11025,10500}))
            local ISNTKNOCKED = Player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075})) and Player.Character.BodyEffects[HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7875,4830,8295})].Value ~= true
            local ISNTGRABBED = Player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,8190,8715,8820,8610,6825,7665,8190,8820})) == nil
            if Root and Humanoid and Humanoid.Health > 1 and ISNTKNOCKED and ISNTGRABBED then
                local screenPos, onScreen = CurrentCamera:WorldToViewportPoint(Root.Position)
                if onScreen then
                    local Dist = (Vector2.new(screenPos.X, screenPos.Y) - MousePos).Magnitude
                    if Dist < MinDist then
                        MinDist = Dist
                        Closest = Player
                    end
                end
            end
        end
    end
    return Closest
end

-- UpdateViewTarget
local function UpdateViewTarget()
    if ViewEnabled then
        if Target and Target.Character then
            local targetPart = Target.Character:FindFirstChild(TargetPart)
            if targetPart then
                CurrentCamera.CameraSubject = targetPart
                return
            end
        end
    end
    CurrentCamera.CameraSubject = Client.Character and Client.Character:FindFirstChildWhichIsA(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,12285,11445,10185,11550,11655,11025,10500}))
end

-- Function to create and weld a part
local function createWeldedPart(rootPart, size, positionOffset, color, material)
    local part = Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8400,10185,11970,12180}))
    part.Size = size
    part.Color = color
    part.Material = material
    part.Anchored = false
    part.CanCollide = false
    part.CFrame = rootPart.CFrame * CFrame.new(positionOffset)
    part.Parent = workspace

    local weld = Instance.new(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({9135,10605,11340,10500}))
    weld.Part0 = rootPart
    weld.Part1 = part
    weld.C0 = CFrame.new(positionOffset)
    weld.Parent = part

    return part, weld
end

-- Function to set up structure and spin it
local function setupStructure(character)
    local rootPart = character:WaitForChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))

    local parts = {
        {Vector3.new(0.3, 4, 0.3), Vector3.new(0, 6, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(-1, 4, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(1, 8, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(1, 6, 0)},
        {Vector3.new(0.3, 2, 0.3), Vector3.new(2, 5, 0)},
        {Vector3.new(2, 0.3, 0.3), Vector3.new(-1, 6, 0)},
        {Vector3.new(0.3, 2, 0.3), Vector3.new(-2, 7, 0)},
    }

    for _, p in ipairs(parts) do
        local part, weld = createWeldedPart(
            rootPart,
            p[1],
            p[2],
            SpinStructureColor,
            Enum.Material.Neon
        )
        table.insert(PartsAndWelds, {weld = weld, offset = p[2], part = part})
    end

    local time = 0

    SpinConnection = game:GetService(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8610,12285,11550,8715,10605,11970,12390,11025,10395,10605})).Heartbeat:Connect(function(dt)
        if not SpinStructureEnabled or not rootPart.Parent then
            SpinConnection:Disconnect()
            SpinConnection = nil
            return
        end
        time += dt
        local angle = time * SpinSpeed
        for _, partData in ipairs(PartsAndWelds) do
            local offset = partData.offset
            local rotatedOffset = CFrame.new(0, offset.Y, 0)
                * CFrame.Angles(0, angle, 0)
                * CFrame.new(offset.X, 0, offset.Z)
            partData.weld.C0 = rotatedOffset
        end
    end)
end

-- Cleanup
local function removeStructure()
    for _, data in ipairs(PartsAndWelds) do
        if data.part then
            data.part:Destroy()
        end
    end
    PartsAndWelds = {}
    if SpinConnection then
        SpinConnection:Disconnect()
        SpinConnection = nil
    end
end

