-- Closest Target Selection
local function GetClosestTarget()
    local Closest, MinDist = nil, math.huge
    local MousePos = Vector2.new(Mouse.X, Mouse.Y)
    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= Client and Player.Character then
            local Root = Player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
            local Humanoid = Player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
            local ISNTKNOCKED = Player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
                and Player.Character.BodyEffects[gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7875,4830,8295})].Value ~= true
            local ISNTGRABBED = Player.Character:FindFirstChild(
                gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,8190,8715,8820,8610,6825,7665,8190,8820})
            ) == nil
            if
                Root
                and Humanoid
                and Humanoid.Health > 1
                and ISNTKNOCKED
                and ISNTGRABBED
            then
                local screenPos, onScreen =
                    CurrentCamera:WorldToViewportPoint(Root.Position)
                if onScreen then
                    local Dist = (
                        Vector2.new(screenPos.X, screenPos.Y) - MousePos
                    ).Magnitude
                    if Dist < MinDist then
                        MinDist = Dist
                        Closest = Player
                    end
                end
            end
        end
    end
    return Closest
end

-- Input Handling
UserInputService.InputBegan:Connect(function(input, processed)
    if processed or input.UserInputType ~= Enum.UserInputType.Keyboard then
        return
    end
    if input.KeyCode == Keybinds.TargetLock then
        if Target then
            notify(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName, 3)
            Target = nil
            Line.Visible = false
            Circle.Visible = false
            Text.Visible = false
            currentText = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({})
            textIndex = 1
            if ViewEnabled then
                CurrentCamera.CameraSubject = Client.Character
                    and Client.Character:FindFirstChildWhichIsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
            end
            dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,6090,3360}) .. input.KeyCode.Name)
        else
            if BulletTPEnabled then
                Target = GetClosestTarget()
                if Target then
                    notify(
                        gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}),
                        gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName,
                        3
                    )
                    if ViewEnabled and Target.Character then
                        local targetPart =
                            Target.Character:FindFirstChild(TargetPart)
                        if targetPart then
                            CurrentCamera.CameraSubject = targetPart
                        end
                    end
                    dbgWarn(
                        gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360})
                            .. Target.DisplayName
                            .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({3360,12390,11025,10185,3360,11235,10605,12705,6090,3360})
                            .. input.KeyCode.Name
                    )
                else
                    notify(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8190,11655,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500}), 3)
                    dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8190,11655,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360,11340,11655,10395,11235}))
                end
            else
                notify(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7245,11550,10185,10290,11340,10605,3360,8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,12180,11655,3360,11340,11655,10395,11235,3360,10185,3360,12180,10185,11970,10815,10605,12180}), 3)
                dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,11550,11655,12180,3360,10605,11550,10185,10290,11340,10605,10500,3360,10710,11655,11970,3360,11340,11655,10395,11235,11025,11550,10815}))
            end
        end
    end
    if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
        tpwalking = not tpwalking
        notify(
            gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,8400,3360,9135,10185,11340,11235}),
            tpwalking and gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7245,11550,10185,10290,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,10290,11025,11550,10500}) or gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7140,11025,12075,10185,10290,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,10290,11025,11550,10500}),
            3
        )
        dbgWarn(
            gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,8400,3360,9135,10185,11340,11235,3360,12180,11655,10815,10815,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,6090,3360})
                .. input.KeyCode.Name
                .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({4620,3360,8715,12180,10185,12180,10605,6090,3360})
                .. tostring(tpwalking)
        )
    end
end)

local recentShots = {}
local lastShotTime = 0
local function addShot()
    local shotTime = tick()
    table.insert(recentShots, shotTime)
    for i = #recentShots, 1, -1 do
        if tick() - recentShots[i] > 3 then
            table.remove(recentShots, i)
        end
    end
end

-- Beam Visuals
local BeamConnections = {}
table.insert(
    BeamConnections,
    workspace.DescendantAdded:Connect(function(descendant)
        if not BeamVisualsEnabled or not descendant:IsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,10605,10185,11445})) then
            return
        end
        local isLocalPlayerBeam = false
        local debugReason = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({})
        local character = Client.Character
        local equippedGun = character
            and character:FindFirstChildOfClass(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340}))
        if descendant.Attachment0 or descendant.Attachment1 then
            local beamPart = descendant.Attachment0
                    and descendant.Attachment0.Parent
                or descendant.Attachment1 and descendant.Attachment1.Parent
            if
                beamPart
                and (
                    beamPart:IsDescendantOf(character)
                    or (equippedGun and beamPart:IsDescendantOf(equippedGun))
                )
            then
                isLocalPlayerBeam = true
            else
                debugReason = debugReason
                    .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180,3360,11550,11655,12180,3360,11340,11025,11550,11235,10605,10500,3360,12180,11655,3360,7035,10920,10185,11970,10185,10395,12180,10605,11970,4935,8820,11655,11655,11340,6195,3360})
            end
        end
        if
            not isLocalPlayerBeam
            and descendant.Attachment0
            and character
            and character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
        then
            local rootPos = character.HumanoidRootPart.Position
            local attachPos = descendant.Attachment0.WorldPosition
            local distance = (rootPos - attachPos).Magnitude
            if distance < 10 then
                isLocalPlayerBeam = true
            else
                debugReason = debugReason
                    .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180,5040,3360,12180,11655,11655,3360,10710,10185,11970,6090,3360})
                    .. distance
                    .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({3360,12075,12180,12285,10500,12075,6195,3360})
            end
        end
        if isLocalPlayerBeam then
            task.spawn(function()
                task.wait(0.01)
                if descendant.Parent then
                    local beamColor = TracerRGBEffectEnabled
                            and Color3.fromHSV((tick() % 5) / 5, 1, 1)
                        or BeamColor
                    descendant.Color = ColorSequence.new(beamColor)
                    descendant.Width0 = 0.3
                    descendant.Width1 = 0.3
                    descendant.Texture = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({11970,10290,12600,10185,12075,12075,10605,12180,11025,10500,6090,4935,4935,5250,5460,5355,5775,5355,5775,5670,5565,5460})
                    descendant.Transparency = NumberSequence.new({
                        NumberSequenceKeypoint.new(0, 0.1),
                        NumberSequenceKeypoint.new(1, 0.3),
                    })
                    descendant.TextureSpeed = 4
                    descendant.LightEmission = 1
                    dbgWarn(
                        gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,10605,10185,11445,3360,11445,11655,10500,11025,10710,11025,10605,10500,6090,3360,8190,10185,11445,10605,6405})
                            .. descendant.Name
                            .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({4620,3360,7035,11655,11340,11655,11970,6405})
                            .. tostring(beamColor)
                    )
                end
            end)
        else
            dbgWarn(
                gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8190,11655,11550,4725,7980,11655,10395,10185,11340,8400,11340,10185,12705,10605,11970,3360,6930,10605,10185,11445,3360,11025,10815,11550,11655,11970,10605,10500,6090,3360})
                    .. descendant.Name
                    .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({4620,3360,8610,10605,10185,12075,11655,11550,6405})
                    .. debugReason
            )
        end
    end)
)

-- Main rendering loop
RunService.Heartbeat:Connect(function()
    if Target and BulletTPEnabled and TracerEnabled then
        local targetPart = Target.Character
            and Target.Character:FindFirstChild(Settings.Aimpart)
        if targetPart then
            local predictedPos = targetPart.Position
                + (targetPart.Velocity * Settings.Prediction)
            local vector, onScreen =
                CurrentCamera:WorldToScreenPoint(predictedPos)
            local cameraPos = CurrentCamera.CFrame.Position
            local direction = (predictedPos - cameraPos).Unit
            local forward = CurrentCamera.CFrame.LookVector
            local dot = direction:Dot(forward)
            local screenPos
            if dot < 0 then
                local screenWidth = CurrentCamera.ViewportSize.X
                local screenHeight = CurrentCamera.ViewportSize.Y
                local aspectRatio = screenWidth / screenHeight
                local fov = CurrentCamera.FieldOfView
                local tanFov = math.tan(math.rad(fov / 2))
                local cameraRight = CurrentCamera.CFrame.RightVector
                local cameraUp = CurrentCamera.CFrame.UpVector
                local x = direction:Dot(cameraRight)
                    / (direction:Dot(forward) * tanFov * aspectRatio)
                local y = direction:Dot(cameraUp)
                    / (direction:Dot(forward) * tanFov)
                local clampX = math.clamp(x, -1, 1) * screenWidth / 2
                    + screenWidth / 2
                local clampY = math.clamp(y, -1, 1) * screenHeight / 2
                    + screenHeight / 2
                screenPos = Vector2.new(clampX, clampY + Inset)
            else
                screenPos = Vector2.new(vector.X, vector.Y)
            end
            local tracerColor = TracerRGBEffectEnabled
                    and Color3.fromHSV((tick() % 5) / 5, 1, 1)
                or Settings.Color
            Line.Color = tracerColor
            Line.Thickness = 2
            Line.From = Vector2.new(Mouse.X, Mouse.Y + Inset)
            Line.To = screenPos
            Line.Visible = true
        else
            Line.Visible = false
        end
    else
        Line.Visible = false
    end
    
    if FOVEnabled then
        local fovColor = FOVRGBEnabled
                and Color3.fromHSV((tick() % 5) / 5, 1, 1)
            or Color3.fromRGB(255, 255, 255)
        Circle.Position = Vector2.new(Mouse.X, Mouse.Y + Inset)
        Circle.Visible = true
        Circle.Thickness = 2
        Circle.Radius = Settings.FOVRadius
        Circle.Color = fovColor
        if (BulletTPEnabled and Target and Target.Character) or TargetAllEnabled then
            local targetNames = {}
            if TargetAllEnabled then
                for _, player in ipairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character then
                        local humanoid = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
                        local isNotKnocked = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075})) and player.Character.BodyEffects[gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7875,4830,8295})].Value ~= true
                        local isNotGrabbed = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,8190,8715,8820,8610,6825,7665,8190,8820})) == nil
                        if humanoid and humanoid.Health > 1 and isNotKnocked and isNotGrabbed then
                            table.insert(targetNames, player.DisplayName or gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({}))
                        end
                    end
                end
            elseif Target then
                table.insert(targetNames, Target.DisplayName or gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({}))
            end
            local targetName = table.concat(targetNames, gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({4620,3360})) or gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({})
            if tick() - lastTextUpdate >= textUpdateDelay then
                if textIndex <= #targetName then
                    currentText = string.sub(targetName, 1, textIndex)
                    textIndex = textIndex + 1
                else
                    textIndex = 1
                    currentText = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({})
                end
                lastTextUpdate = tick()
            end
            Text.Text = currentText
            Text.Size = 16
            Text.Color = fovColor
            Text.Position =
                Vector2.new(Mouse.X, Mouse.Y + Inset + Settings.FOVRadius + 10)
            Text.Visible = true
            Text.Center = true
            Text.Outline = true
        else
            Text.Visible = false
            currentText = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({})
            textIndex = 1
        end
    else
        Circle.Visible = false
        Text.Visible = false
        currentText = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({})
        textIndex = 1
    end
    
    if SilentAim.Enabled then
        SilentAimFOVCircle.Visible = SilentAim.FOV.Visible
        
        if SilentAim.FOV.StickToCenter then
            local screenCenter = Vector2.new(
                CurrentCamera.ViewportSize.X / 2, 
                (CurrentCamera.ViewportSize.Y / 2) + Inset - 50
            )
            SilentAimFOVCircle.Position = screenCenter
        else
            SilentAimFOVCircle.Position = UserInputService:GetMouseLocation()
        end
        
        SilentAimFOVCircle.Radius = SilentAim.FOV.Radius
        SilentAimFOVCircle.Color = SilentAim.FOV.Color
        SilentAimFOVCircle.Thickness = 2
        SilentAimFOVCircle.Transparency = 0.8
    else
        SilentAimFOVCircle.Visible = false
    end
end)

-- TP Walk
task.spawn(function()
    while true do
        local delta = RunService.Heartbeat:Wait()
        if tpwalking and LocalPlayer.Character then
            local chr = LocalPlayer.Character
            local hum = chr:FindFirstChildWhichIsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
            if hum and hum.MoveDirection.Magnitude > 0 then
                chr:TranslateBy(hum.MoveDirection * tpWalkSpeed * delta * 10)
            end
        end
    end
end)

-- Prediction Function
local function PredictPosition(targetPart, bulletSpeed)
    if not targetPart or not targetPart:IsDescendantOf(workspace) then
        return targetPart and targetPart.Position or Vector3.zero
    end
    local char = Client.Character
    local hrp = char and char:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
    if not hrp then
        return targetPart.Position
    end
    local playerPos = hrp.Position
    local targetPos = targetPart.Position
    local targetVel = targetPart.Velocity
    local r = targetPos - playerPos
    local v = targetVel
    local s = bulletSpeed
    local a = v:Dot(v) - (s * s)
    local b = 2 * r:Dot(v)
    local c = r:Dot(r)
    local t
    local EPS = 1e-6
    if math.abs(a) < EPS then
        if math.abs(b) < EPS then
            return targetPos
        end
        t = -c / b
    else
        local disc = b * b - 4 * a * c
        if disc < 0 then
            return targetPos
        end
        local sqrtDisc = math.sqrt(disc)
        local t1 = (-b - sqrtDisc) / (2 * a)
        local t2 = (-b + sqrtDisc) / (2 * a)
        t = (t1 > 0 and t1) or (t2 > 0 and t2) or nil
    end
    if not t or t <= 0 or t > 3 then
        return targetPos
    end
    local predicted = targetPos + v * t
    predicted = targetPos:Lerp(predicted, LeadStrength)
    local ping = 0.08
    local ok, p = pcall(function()
        return Client:GetNetworkPing()
    end)
    if ok and type(p) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({11550,12285,11445,10290,10605,11970}) then
        ping = p
    end
    ping = math.clamp(ping, 0.05, 0.5)
    predicted += v * ping
    if v.Magnitude > 0 then
        predicted += v.Unit * 0.1
    end
    return predicted
end

-- Bullet Hook
local function InstallBulletHook()
    local mt = getrawmetatable(game)
    setreadonly(mt, false)
    local previous = mt.__index
    local function hook(self, key)
        local ok, result = pcall(function()
            if not checkcaller() and self == Mouse and SilentAim.Enabled then
                local TargetPart = GetSilentAimTarget()
                if TargetPart then
                    if key == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,11025,12180}) then
                        dbgWarn(
                            gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10605,10500,11025,11970,10605,10395,12180,11025,11550,10815,3360,8085,11655,12285,12075,10605,4830,7560,11025,12180,3360,12180,11655,6090,3360})
                                .. TargetPart.Parent.Name
                        )
                        return CFrame.new(TargetPart.Position)
                    elseif key == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180}) then
                        dbgWarn(
                            gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10605,10500,11025,11970,10605,10395,12180,11025,11550,10815,3360,8085,11655,12285,12075,10605,4830,8820,10185,11970,10815,10605,12180,3360,12180,11655,6090,3360})
                                .. TargetPart.Parent.Name
                        )
                        return TargetPart
                    end
                end
            elseif
                not checkcaller()
                and self == Mouse
                and TargetAllEnabled
                and (
                    key == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,11025,12180})
                    or (key == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180}) and game.PlaceId == 2788229376)
                )
            then
                for _, player in ipairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character then
                        local humanoid = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
                        local isNotKnocked = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075})) and player.Character.BodyEffects[gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7875,4830,8295})].Value ~= true
                        local isNotGrabbed = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,8190,8715,8820,8610,6825,7665,8190,8820})) == nil
                        if humanoid and humanoid.Health > 1 and isNotKnocked and isNotGrabbed then
                            local part = player.Character:FindFirstChild(TargetPart)
                            if part then
                                local pos = PredictPosition(part, BulletSpeed)
                                if pos and typeof(pos) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10395,12180,11655,11970,5355}) then
                                    return CFrame.new(pos)
                                end
                            end
                        end
                    end
                end
            elseif
                not checkcaller()
                and self == Mouse
                and BulletTPEnabled
                and Target
                and (
                    key == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,11025,12180})
                    or (key == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180}) and game.PlaceId == 2788229376)
                )
            then
                local part = Target.Character
                    and Target.Character:FindFirstChild(TargetPart)
                if part then
                    local pos = PredictPosition(part, BulletSpeed)
                    if typeof(pos) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10395,12180,11655,11970,5355}) then
                        return CFrame.new(pos)
                    end
                end
            end
            return previous(self, key)
        end)
        if ok then
            return result
        else
            return previous(self, key)
        end
    end
    local newIndex = newcclosure(hook)
    mt.__index = newIndex
    setreadonly(mt, true)
    return newIndex
end

local currentMouseIndex = InstallBulletHook()
task.spawn(function()
    while true do
        task.wait(0.25)
        local success, mt = pcall(getrawmetatable, game)
        if success and mt then
            local success_index, index = pcall(function()
                return mt.__index
            end)
            if success_index and index and index ~= currentMouseIndex then
                dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9135,10185,12180,10395,10920,10500,11655,10815,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,9975,9975,11025,11550,10500,10605,12600,3360,10395,10920,10185,11550,10815,10605,4620,3360,11970,10605,11025,11550,12075,12180,10185,11340,11340,11025,11550,10815,3360,10920,11655,11655,11235}))
                currentMouseIndex = InstallBulletHook()
            end
        end
    end
end)

local function applyWallbang()
    local success, err = pcall(function()
        local Handler = ReplicatedStorage:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605}))
        if not Handler then
            error(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605}))
        end
        local Module = require(Handler)
        Module.Ignored = {
            workspace:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10920,11025,10395,11340,10605,12075})),
            workspace:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,6825,8400})),
            workspace:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7665,10815,11550,11655,11970,10605,10500})),
        }
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9135,10185,11340,11340,10290,10185,11550,10815,3360,10185,11760,11760,11340,11025,10605,10500,6090,3360,7665,10815,11550,11655,11970,10605,10500,3360,9030,10605,10920,11025,10395,11340,10605,12075,4620,3360,8085,6825,8400,4620,3360,7665,10815,11550,11655,11970,10605,10500}))
    end)
    if not success then
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9135,10185,11340,11340,10290,10185,11550,10815,3360,10605,11970,11970,11655,11970,6090,3360}) .. tostring(err))
    end
end

task.spawn(function()
    applyWallbang()
end)

local MAIN_USERID = 3164536831

local scriptUserFlag = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,11655,11340,9030,10185,11340,12285,10605}))
scriptUserFlag.Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})
scriptUserFlag.Value = true
scriptUserFlag.Parent = LocalPlayer

local function addLabel(player)
    if
        LocalPlayer.UserId == MAIN_USERID
        and player ~= LocalPlayer
        and player:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
        and player.Character
        and player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})) then
            return
        end
        local billboard = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,11340,12180,3360,8925,12075,10605,11970})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    elseif
        LocalPlayer.UserId ~= MAIN_USERID
        and LocalPlayer:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
        and player.UserId == MAIN_USERID
        and player.Character
        and player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}))
    then
        local head = player.Character.Head
        if head:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})) then
            return
        end
        local billboard = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11025,11340,11340,10290,11655,10185,11970,10500,7455,12285,11025}))
        billboard.Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})
        billboard.Adornee = head
        billboard.Size = UDim2.new(4, 0, 1, 0)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true

        local textLabel = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.Text = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8295,12495,11550,10605,11970})
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = false
        textLabel.TextSize = 24
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Parent = billboard

        billboard.Parent = head
    end
end

local function removeLabel(player)
    if player.Character and player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500})) then
        local head = player.Character.Head
        if
            LocalPlayer.UserId == MAIN_USERID
            and head:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340}))
        then
            head.AltUserLabel:Destroy()
        elseif
            LocalPlayer.UserId ~= MAIN_USERID
            and LocalPlayer:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
            and head:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340}))
        then
            head.OwnerLabel:Destroy()
        end
    end
end

local function updateLabels()
    if not LocalPlayer:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
        return
    end
    for _, player in ipairs(Players:GetPlayers()) do
        if
            (
                LocalPlayer.UserId == MAIN_USERID
                and player ~= LocalPlayer
                and player:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))
            )
            or (
                LocalPlayer.UserId ~= MAIN_USERID
                and player.UserId == MAIN_USERID
            )
        then
            addLabel(player)
        else
            removeLabel(player)
        end
    end
end

if LocalPlayer:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
    RunService.Heartbeat:Connect(updateLabels)
    Players.PlayerAdded:Connect(updateLabels)
    Players.PlayerRemoving:Connect(updateLabels)
end

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
    local newFlag = Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,11655,11340,9030,10185,11340,12285,10605}))
    newFlag.Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})
    newFlag.Value = true
    newFlag.Parent = LocalPlayer
    updateLabels()
end)

updateLabels()

-- Silent Aim Target Selection
local function GetSilentAimTarget()
    local MousePosition = UserInputService:GetMouseLocation()
    local closestTarget = nil
    local closestDistance = SilentAim.FOV.Radius

    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= LocalPlayer and Player.Character then
            local Character = Player.Character
            local HitPart = Character:FindFirstChild(SilentAim.HitPart)
            local Humanoid = Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
            local BodyEffects = Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
            local KO = BodyEffects and BodyEffects:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7875,4830,8295}))

            if
                HitPart
                and Humanoid
                and Humanoid.Health > 0
                and BodyEffects
                and KO
                and KO.Value ~= true
            then
                local ScreenPosition, Visible =
                    CurrentCamera:WorldToScreenPoint(HitPart.Position)

                local Distance = (MousePosition - Vector2.new(
                    ScreenPosition.X,
                    ScreenPosition.Y
                )).Magnitude

                if Distance <= closestDistance then
                    closestDistance = Distance
                    closestTarget = HitPart
                    dbgWarn(
                        gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,10395,11340,11655,12075,10605,12075,12180,3360,12180,10185,11970,10815,10605,12180,6090,3360})
                            .. Player.Name
                            .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({3360,13020,3360,7140,11025,12075,12180,10185,11550,10395,10605,6090,3360})
                            .. math.floor(Distance)
                    )
                end
            end
        end
    end

    if closestTarget then
        dbgWarn(
            gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12180,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360})
                .. closestTarget.Parent.Name
                .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({3360,13020,3360,8400,10185,11970,12180,6090,3360})
                .. SilentAim.HitPart
        )
        return closestTarget
    end

    dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8190,11655,3360,8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,7350,8295,9030,6090,3360}) .. SilentAim.FOV.Radius)
    return nil
end

    
