        end
        dbgWarn(ZuRdGvkgaGnppuwNtpwpGfeN({7770,10605,12075,12285,12075,3360,7035,11970,11655,12075,12075,3360,10395,11655,11340,11655,11970,3360,12075,10605,12180,3360,12180,11655,6090,3360}) .. tostring(value))
    end,
})

JesusCrossSection:AddDropdown({
    Name = ZuRdGvkgaGnppuwNtpwpGfeN({7035,11970,11655,12075,12075,3360,8085,10185,12180,10605,11970,11025,10185,11340}),
    Default = ZuRdGvkgaGnppuwNtpwpGfeN({7350,11655,11970,10395,10605,7350,11025,10605,11340,10500}),
