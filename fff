local function GetSilentAimTarget()
    local MousePosition = UserInputService:GetMouseLocation()
    local closestTarget = nil
    local closestDistance = SilentAim.FOV.Radius

    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= LocalPlayer and Player.Character then
            local Character = Player.Character
            local HitPart = Character:FindFirstChild(SilentAim.HitPart)
            local Humanoid = Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,12285,11445,10185,11550,11655,11025,10500}))
            local BodyEffects = Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
            local KO = BodyEffects and BodyEffects:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7875,4830,8295}))

            if
                HitPart
                and Humanoid
                and Humanoid.Health > 0
                and BodyEffects
                and KO
                and KO.Value ~= true
            then
                local ScreenPosition, Visible =
                    CurrentCamera:WorldToScreenPoint(HitPart.Position)

                local Distance = (MousePosition - Vector2.new(
                    ScreenPosition.X,
                    ScreenPosition.Y
                )).Magnitude

                if Distance <= closestDistance then
                    closestDistance = Distance
                    closestTarget = HitPart
                    dbgWarn(
                        CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,10395,11340,11655,12075,10605,12075,12180,3360,12180,10185,11970,10815,10605,12180,6090,3360})
                            .. Player.Name
                            .. CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({3360,13020,3360,7140,11025,12075,12180,10185,11550,10395,10605,6090,3360})
                            .. math.floor(Distance)
                    )
                end
            end
        end
    end

    if closestTarget then
        dbgWarn(
            CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12180,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360})
                .. closestTarget.Parent.Name
                .. CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({3360,13020,3360,8400,10185,11970,12180,6090,3360})
                .. SilentAim.HitPart
        )
        return closestTarget
    end

    dbgWarn(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8190,11655,3360,8715,11025,11340,10605,11550,12180,3360,6825,11025,11445,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,7350,8295,9030,6090,3360}) .. SilentAim.FOV.Radius)
    return nil
end

local function setupHitDetection()
    local function connectHealthMonitor(player)
        if player == Target and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,12285,11445,10185,11550,11655,11025,10500}))
            if humanoid then
                local lastHealth = humanoid.Health
                local connection
                connection = humanoid.HealthChanged:Connect(function(newHealth)
                    if
                        HitChamsEnabled
                        and BulletTPEnabled
                        and player == Target
                        and newHealth < lastHealth
                        and (tick() - lastShotTime) <= HIT_DETECTION_WINDOW
                    then
                        createHitCham(player.Character)
                        notify(
                            CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180,3360,7035,10920,10185,11445,12075}),
                            CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,11655,11550,3360}) .. player.Name,
                            3
                        )
                        dbgWarn(
                            CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,11025,12180,3360,10500,10605,12180,10605,10395,12180,10605,10500,3360,11655,11550,3360})
                                .. player.Name
                                .. CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({4620,3360,12180,11970,11025,10815,10815,10605,11970,11025,11550,10815,3360,10395,10920,10185,11445,12075})
                        )
                    end
                    lastHealth = newHealth
                end)
                local cleanup
                cleanup = Players.PlayerRemoving:Connect(function(p)
                    if p == player then
                        connection:Disconnect()
                        cleanup:Disconnect()
                    end
                end)
                local targetChange
                targetChange = RunService.Heartbeat:Connect(function()
                    if Target ~= player then
                        connection:Disconnect()
                        cleanup:Disconnect()
                        targetChange:Disconnect()
                    end
                end)
            end
        end
    end

    local lastTarget = nil
    RunService.Heartbeat:Connect(function()
        if Target ~= lastTarget then
            if Target then
                connectHealthMonitor(Target)
            end
            lastTarget = Target
        end
    end)
end

task.spawn(setupHitDetection)

pcall(function()
    setthreadidentity(7)
end)
notify(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6825,10500,11655,11550,11025,12075,3360,6930,12705,11760,10185,12075,12075}), CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({8715,10395,11970,11025,11760,12180,3360,11340,11655,10185,10500,10605,10500,3360,12075,12285,10395,10395,10605,12075,12075,10710,12285,11340,11340,12705}), 4)
