-- Get Closest Target
local function GetClosestTarget()
	local Closest, MinDist = nil, math.huge
	local MousePos = Mouse.Hit.Position
	for _, Player in ipairs(Players:GetPlayers()) do
		if Player ~= Client and Player.Character then
			local Root = Player.Character:FindFirstChild(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
			local Humanoid = Player.Character:FindFirstChild(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			local ISNTKNOCKED = Player.Character:FindFirstChild(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','*********************************************************************','******************************************************************************************************','******************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************'}))
				and Player.Character.BodyEffects[cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***************************************************************************','**********************************************','*******************************************************************************'})].Value ~= true
			local ISNTGRABBED = Player.Character:FindFirstChild(
				cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'***********************************************************************','**********************************************************************************','*****************************************************************','******************************************************************','******************************************************************','*************************************************************************','******************************************************************************','***********************************************************************','***********************************************************************************************','*******************************************************************','*******************************************************************************','*************************************************************************','******************************************************************************','***********************************************************************************','************************************************************************************','**********************************************************************************','*****************************************************************','*************************************************************************','******************************************************************************','************************************************************************************'})
			) == nil
			if
				Root
				and Humanoid
				and Humanoid.Health > 1
				and ISNTKNOCKED
				and ISNTGRABBED
			then
				local Dist = (Root.Position - MousePos).Magnitude
				if Dist < MinDist then
					MinDist = Dist
					Closest = Player
				end
			end
		end
	end
	return Closest
end

-- Keybind Handler
UserInputService.InputBegan:Connect(function(input, processed)
	if processed or input.UserInputType ~= Enum.UserInputType.Keyboard then
		return
	end
	-- Target Lock Key
	if input.KeyCode == Keybinds.TargetLock then
		if Target then
			notify(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}), cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. Target.DisplayName, 3)
			Target = nil
			Line.Visible = false
			Circle.Visible = false
			Text.Visible = false
			currentText = cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({})
			textIndex = 1
			if ViewEnabled then
				CurrentCamera.CameraSubject = Client.Character
					and Client.Character:FindFirstChildWhichIsA(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			end
			dbgWarn(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name)
		else
			if BulletTPEnabled then
				Target = GetClosestTarget()
				if Target then
					notify(
						cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}),
						cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. Target.DisplayName,
						3
					)
					if ViewEnabled and Target.Character then
						local targetPart =
							Target.Character:FindFirstChild(TargetPart)
						if targetPart then
							CurrentCamera.CameraSubject = targetPart
						end
					end
					dbgWarn(
						cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'})
							.. Target.DisplayName
							.. cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'})
							.. input.KeyCode.Name
					)
				else
					notify(cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}), cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'******************************************************************************','***************************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************