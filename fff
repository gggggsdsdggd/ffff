-- Utility Functions (continued)
local function getEquippedGun(plr)
	for _, tool in ipairs(plr.Character and plr.Character:GetChildren() or {}) do
		if tool:IsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340})) and GUN_NAMES[tool.Name] then
			dbgWarn(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10815,10605,12180,7245,11865,12285,11025,11760,11760,10605,10500,7455,12285,11550,6090,3360,7350,11655,12285,11550,10500,3360,10815,12285,11550,3360})
					.. tool.Name
					.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10710,11655,11970,3360,11760,11340,10185,12705,10605,11970,3360})
					.. plr.Name
			)
			return tool
		end
	end
	dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10815,10605,12180,7245,11865,12285,11025,11760,11760,10605,10500,7455,12285,11550,6090,3360,8190,11655,3360,12075,12285,11760,11760,11655,11970,12180,10605,10500,3360,10815,12285,11550,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360,11760,11340,10185,12705,10605,11970,3360}) .. plr.Name)
	return nil
end

local function getClosestTargetToPlayer(plr)
	local closest, shortest = nil, math.huge
	local myHRP = plr.Character
		and plr.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
	if not myHRP then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10815,10605,12180,7035,11340,11655,12075,10605,12075,12180,8820,10185,11970,10815,10605,12180,8820,11655,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180,3360,10710,11655,11970,3360,11760,11340,10185,12705,10605,11970,3360})
				.. plr.Name
		)
		return nil
	end
	for _, enemy in pairs(Players:GetPlayers()) do
		if
			enemy ~= plr
			and enemy.Character
			and enemy.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
		then
			local humanoid = enemy.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500}))
			local bodyEffects = enemy.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
			local isKO = bodyEffects and bodyEffects:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7875,4830,8295}))
			if
				humanoid
				and humanoid.Health > 0
				and not (isKO and isKO.Value == true)
			then
				local dist = (
					enemy.Character.HumanoidRootPart.Position - myHRP.Position
				).Magnitude
				if dist < shortest and dist <= maxAssistDistance then
					shortest = dist
					closest = enemy
					dbgWarn(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10815,10605,12180,7035,11340,11655,12075,10605,12075,12180,8820,10185,11970,10815,10605,12180,8820,11655,8400,11340,10185,12705,10605,11970,6090,3360,7350,11655,12285,11550,10500,3360,12180,10185,11970,10815,10605,12180,3360})
							.. enemy.Name
							.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10185,12180,3360,10500,11025,12075,12180,10185,11550,10395,10605,3360})
							.. dist
					)
				end
			else
				dbgWarn(
					YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10815,10605,12180,7035,11340,11655,12075,10605,12075,12180,8820,10185,11970,10815,10605,12180,8820,11655,8400,11340,10185,12705,10605,11970,6090,3360,8715,11235,11025,11760,11760,10605,10500,3360})
						.. enemy.Name
						.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,4200,7560,10605,10185,11340,12180,10920,6405})
						.. (humanoid and humanoid.Health or YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11550,11025,11340}))
						.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,7875,8295,6405})
						.. tostring(isKO and isKO.Value)
				)
			end
		end
	end
	if not closest then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10815,10605,12180,7035,11340,11655,12075,10605,12075,12180,8820,10185,11970,10815,10605,12180,8820,11655,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,12390,10185,11340,11025,10500,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360}) .. plr.Name
		)
	end
	return closest
end

local function fireGunForPlayer(plr)
	local gun = getEquippedGun(plr)
	if not gun then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,12075,12285,11760,11760,11655,11970,12180,10605,10500,3360,10815,12285,11550,3360,10605,11865,12285,11025,11760,11760,10605,10500,3360,10710,11655,11970,3360}) .. plr.Name)
		return
	end
	local handle = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,10185,11550,10500,11340,10605}))
	local remote = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
	local ammo = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,11445,11445,11655}))
	local range = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10185,11550,10815,10605}))
	if not (handle and ammo and range) then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,8085,11025,12075,12075,11025,11550,10815,3360,10395,11655,11445,11760,11655,11550,10605,11550,12180,12075,3360,10710,11655,11970,3360,10815,12285,11550,3360})
				.. gun.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,4200,7560,10185,11550,10500,11340,10605,6405})
				.. tostring(handle)
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,6825,11445,11445,11655,6405})
				.. tostring(ammo)
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,8610,10185,11550,10815,10605,6405})
				.. tostring(range)
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4305})
		)
		return
	end
	ammo.Value, range.Value = infiniteAmmo, 999999999
	local gunHandler
	local success, result = pcall(function()
		return require(ReplicatedStorage.Modules.GunHandler)
	end)
	if not success then
		warn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,7350,10185,11025,11340,10605,10500,3360,12180,11655,3360,11340,11655,10185,10500,3360,7455,12285,11550,7560,10185,11550,10500,11340,10605,11970,3360,10710,11655,11970,3360})
				.. plr.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360})
				.. result
		)
		return
	end
	gunHandler = result
	local muzzle = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7140,10605,10710,10185,12285,11340,12180}))
		and gun.Default:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,10605,12075,10920}))
		and gun.Default.Mesh:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,12285,12810,12810,11340,10605}))
	if not muzzle then
		muzzle = {
			WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position,
		}
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,11445,12285,12810,12810,11340,10605,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360})
				.. gun.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,12285,12075,11025,11550,10815,3360,10710,10185,11340,11340,10290,10185,10395,11235,3360,11760,11655,12075,11025,12180,11025,11655,11550})
		)
	end
	local target = getClosestTargetToPlayer(plr)
	if
		not target
		or not target.Character
		or not target.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
	then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,12390,10185,11340,11025,10500,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,11970,3360}) .. plr.Name)
		return
	end
	if target == LocalPlayer then
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,8715,11235,11025,11760,11760,10605,10500,3360,10185,12075,12075,11025,12075,12180,11025,11550,10815,3360,11340,11655,10395,10185,11340,3360,11760,11340,10185,12705,10605,11970,3360}) .. plr.Name)
		return
	end
	local targetPos = target.Character.HumanoidRootPart.Position
	for i = 1, assistBulletsPerShot do
		local aimOffset = Vector3.new(
			math.random(-assistSpread * 100, assistSpread * 100) / 100,
			math.random(-assistSpread * 100, assistSpread * 100) / 100,
			math.random(-assistSpread * 100, assistSpread * 100) / 100
		)
		local shootPos = targetPos + aimOffset
		local success, vShoot, v2, v3 = pcall(function()
			setidentity(2)
			local result = {
				gunHandler.shoot({
					Shooter = plr.Character,
					Handle = handle,
					ForcedOrigin = muzzle.WorldPosition,
					AimPosition = shootPos,
					BeamColor = Color3.new(1, 0.545, 0.149),
					Range = range.Value,
				}),
			}
			setidentity(8)
			return unpack(result)
		end)
		if not success then
			dbgWarn(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,7245,11970,11970,11655,11970,3360,12075,10920,11655,11655,12180,11025,11550,10815,3360,10710,11655,11970,3360})
					.. plr.Name
					.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360})
					.. tostring(vShoot)
			)
			return
		end
		ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180,7455,12285,11550}),
			handle,
			muzzle.WorldPosition,
			vShoot,
			v2,
			v3
		)
		if remote then
			remote:FireServer(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180}))
		end
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7455,12285,11550,7350,11655,11970,8400,11340,10185,12705,10605,11970,6090,3360,7350,11025,11970,10605,10500,3360,12075,10920,11655,12180,3360})
				.. i
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10710,11655,11970,3360})
				.. plr.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10185,12180,3360})
				.. target.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,8400,11655,12075,11025,12180,11025,11655,11550,6405})
				.. tostring(shootPos)
		)
		task.wait(assistShotDelay)
	end
end

local function fireLagBulletsAroundPlayer(plr)
	if
		not plr
		or not plr.Character
		or not plr.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
	then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,12390,10185,11340,11025,10500,3360,10395,10920,10185,11970,10185,10395,12180,10605,11970,3360,10710,11655,11970,3360})
				.. (plr and plr.Name or YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({11550,11025,11340}))
		)
		return
	end
	local gun = getEquippedGun(plr)
	if not gun then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,12075,12285,11760,11760,11655,11970,12180,10605,10500,3360,10815,12285,11550,3360,10605,11865,12285,11025,11760,11760,10605,10500,3360,10710,11655,11970,3360})
				.. plr.Name
		)
		return
	end
	local handle = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,10185,11550,10500,11340,10605}))
	local remote = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
	local ammo = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6825,11445,11445,11655}))
	local range = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8610,10185,11550,10815,10605}))
	if not (handle and ammo and range) then
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,8085,11025,12075,12075,11025,11550,10815,3360,10395,11655,11445,11760,11655,11550,10605,11550,12180,12075,3360,10710,11655,11970,3360,10815,12285,11550,3360})
				.. gun.Name
		)
		return
	end
	ammo.Value, range.Value = infiniteAmmo, 999999999
	local gunHandler
	local success, result = pcall(function()
		return require(ReplicatedStorage.Modules.GunHandler)
	end)
	if not success then
		warn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,7350,10185,11025,11340,10605,10500,3360,12180,11655,3360,11340,11655,10185,10500,3360,7455,12285,11550,7560,10185,11550,10500,11340,10605,11970,3360,10710,11655,11970,3360})
				.. plr.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360})
				.. result
		)
		return
	end
	gunHandler = result
	local muzzle = gun:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7140,10605,10710,10185,12285,11340,12180}))
		and gun.Default:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,10605,12075,10920}))
		and gun.Default.Mesh:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,12285,12810,12810,11340,10605}))
	if not muzzle then
		muzzle = {
			WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position,
		}
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,8190,11655,3360,11445,12285,12810,12810,11340,10605,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360})
				.. gun.Name
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,12285,12075,11025,11550,10815,3360,10710,10185,11340,11340,10290,10185,10395,11235,3360,11760,11655,12075,11025,12180,11025,11655,11550})
		)
	end
	local basePos = muzzle.WorldPosition
	local centerPos = plr.Character.HumanoidRootPart.Position
	for i = 1, lagBulletsPerShot do
		local aimOffset = Vector3.new(
			math.random(-lagSpread * 100, lagSpread * 100) / 100,
			math.random(-lagSpread * 100, lagSpread * 100) / 100,
			math.random(-lagSpread * 100, lagSpread * 100) / 100
		)
		local aimPos = centerPos + aimOffset
		local success, shootResult, hitPos, target = pcall(function()
			setidentity(2)
			local result = {
				gunHandler.shoot({
					Shooter = plr.Character,
					Handle = handle,
					ForcedOrigin = basePos,
					AimPosition = aimPos,
					BeamColor = Color3.new(1, 0, 0),
					Range = 999999999,
				}),
			}
			setidentity(8)
			return unpack(result)
		end)
		if not success then
			dbgWarn(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,7245,11970,11970,11655,11970,3360,12075,10920,11655,11655,12180,11025,11550,10815,3360,10710,11655,11970,3360})
					.. plr.Name
					.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6090,3360})
					.. tostring(shootResult)
			)
			return
		end
		ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180,7455,12285,11550}),
			handle,
			basePos,
			shootResult,
			hitPos,
			target
		)
		if remote then
			remote:FireServer(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8715,10920,11655,11655,12180}))
		end
		dbgWarn(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({10710,11025,11970,10605,7980,10185,10815,6930,12285,11340,11340,10605,12180,12075,6825,11970,11655,12285,11550,10500,8400,11340,10185,12705,10605,11970,6090,3360,7350,11025,11970,10605,10500,3360,11340,10185,10815,3360,12075,10920,11655,12180,3360})
				.. i
				.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,10710,11655,11970,3360})
				.. plr.Name
		)
		task.wait(lagShotDelay)
	end
end

-- Get Closest Target
local function GetClosestTarget()
	local Closest, MinDist = nil, math.huge
	local MousePos = Mouse.Hit.Position
	for _, Player in ipairs(Players:GetPlayers()) do
		if Player ~= Client and Player.Character then
			local Root = Player.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
			local Humanoid = Player.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500}))
			local ISNTKNOCKED = Player.Character:FindFirstChild(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
				and Player.Character.BodyEffects[YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7875,4830,8295})].Value ~= true
			local ISNTGRABBED = Player.Character:FindFirstChild(
				YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,7665,8190,8715,8820,8610,6825,7665,8190,8820})
			) == nil
			if
				Root
				and Humanoid
				and Humanoid.Health > 1
				and ISNTKNOCKED
				and ISNTGRABBED
			then
				local Dist = (Root.Position - MousePos).Magnitude
				if Dist < MinDist then
					MinDist = Dist
					Closest = Player
				end
			end
		end
	end
	return Closest
end

-- Keybind Handler
UserInputService.InputBegan:Connect(function(input, processed)
	if processed or input.UserInputType ~= Enum.UserInputType.Keyboard then
		return
	end
	-- Target Lock Key
	if input.KeyCode == Keybinds.TargetLock then
		if Target then
			notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName, 3)
			Target = nil
			Line.Visible = false
			Circle.Visible = false
			Text.Visible = false
			currentText = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({})
			textIndex = 1
			if ViewEnabled then
				CurrentCamera.CameraSubject = Client.Character
					and Client.Character:FindFirstChildWhichIsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7560,12285,11445,10185,11550,11655,11025,10500}))
			end
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,6090,3360}) .. input.KeyCode.Name)
		else
			if BulletTPEnabled then
				Target = GetClosestTarget()
				if Target then
					notify(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}),
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName,
						3
					)
					if ViewEnabled and Target.Character then
						local targetPart =
							Target.Character:FindFirstChild(TargetPart)
						if targetPart then
							CurrentCamera.CameraSubject = targetPart
						end
					end
					dbgWarn(
						YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360})
							.. Target.DisplayName
							.. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({3360,12390,11025,10185,3360,11235,10605,12705,6090,3360})
							.. input.KeyCode.Name
					)
				else
					notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8190,11655,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500}), 3)
					dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8190,11655,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360,11340,11655,10395,11235}))
				end
			else
				notify(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7245,11550,10185,10290,11340,10605,3360,8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,12180,11655,3360,11340,11655,10395,11235,3360,10185,3360,12180,10185,11970,10815,10605,12180}), 3)
				dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,11550,11655,12180,3360,10605,11550,10185,10290,11340,10605,10500,3360,10710,11655,11970,3360,11340,11655,10395,11235,11025,11550,10815}))
			end
		end
	end
	-- TP Walk Key
	if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
		tpwalking = not tpwalking
		notify(
			YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,8400,3360,9135,10185,11340,11235}),
			tpwalking and YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7245,11550,10185,10290,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,10290,11025,11550,10500}) or YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7140,11025,12075,10185,10290,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,10290,11025,11550,10500}),
