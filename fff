-- *** FIXED SHOOT FUNCTION - ONLY TARGET ALL FIXED ***
local function shoot(targetPlayer)
    if not tool or not tool.Parent or tool.Parent ~= Character then
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10920,11655,11655,12180,3360,10185,10290,11655,11970,12180,10605,10500,6090,3360,8820,11655,11655,11340,3360,11550,11655,12180,3360,10605,11865,12285,11025,11760,11760,10605,10500}))
        return
    end
    
    local v_u_4 = tool
    local v_u_5 = v_u_4:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10185,11550,10500,11340,10605}), 10)
    local v_u_8 = v_u_4:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,11445,11445,11655}), 10)
    local v_u_9 = v_u_4:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10185,11550,10815,10605}), 10)
    local v_u_13 = v_u_4:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})) or { FireServer = function() end }
    local v_u_2 = ReplicatedStorage
    local v_u_7 = require(v_u_2:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,11655,10500,12285,11340,10605,12075}), 10):WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970}), 10))
    local v_u_1 = CFrame.new(-1, 0.4, 0)
    local mainRemotes = v_u_2:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,12075}))

    v_u_8.Value = 99999
    v_u_9.Value = INFINITE_RANGE
    v_u_13:FireServer(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10920,11655,11655,12180}))

    local v14 = (v_u_5.CFrame * v_u_1).Position
    local muzzle = v_u_4:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7140,10605,10710,10185,12285,11340,12180})) and v_u_4.Default:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10605,12075,10920})) and v_u_4.Default.Mesh:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,12285,12810,12810,11340,10605}))
    local muzzlePos = muzzle and muzzle.WorldPosition or v14
    if not muzzlePos or typeof(muzzlePos) ~= gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10395,12180,11655,11970,5355}) then
        muzzlePos = CurrentCamera.CFrame.Position
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,12285,12810,12810,11340,10605,3360,11760,11655,12075,11025,12180,11025,11655,11550,3360,11025,11550,12390,10185,11340,11025,10500,4620,3360,12285,12075,11025,11550,10815,3360,10395,10185,11445,10605,11970,10185,3360,11760,11655,12075,11025,12180,11025,11655,11550,6090,3360}) .. tostring(muzzlePos))
    end

    local spreadAngle = 1
    local randomX = math.rad(math.random(-spreadAngle, spreadAngle))
    local randomY = math.rad(math.random(-spreadAngle, spreadAngle))
    local spreadCFrame = CFrame.Angles(randomX, randomY, 0)

    local aimPos
    local raycastParams = RaycastParams.new()
    raycastParams.FilterType = Enum.RaycastFilterType.Blacklist
    local filter = { Character }
    for _, obj in ipairs(ignoreList) do
        if obj and obj:IsDescendantOf(workspace) then
            table.insert(filter, obj)
        end
    end
    raycastParams.FilterDescendantsInstances = filter
    raycastParams.IgnoreWater = true

    -- *** FIXED TARGET ALL LOGIC ***
    if RapidFireOnlyEnabled and BulletTPEnabled and Target and Target.Character then
        -- Rapid Fire + Target Aim: Use locked target
        local targetPart = Target.Character:FindFirstChild(TargetPart or gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}))
        if targetPart and targetPart:IsDescendantOf(workspace) then
            local predicted = targetPart.Position + (targetPart.Velocity * (Settings.Prediction or LeadStrength or 0.1))
            if predicted and typeof(predicted) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10395,12180,11655,11970,5355}) then
                aimPos = predicted
                dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,6825,8400,7665,7140,3360,7350,7665,8610,7245,3360,4515,3360,8820,6825,8610,7455,7245,8820,3360,6825,7665,8085,3360,4725,3360,8400,11340,10185,12705,10605,11970,6090,3360}) .. Target.Name)
            else
                aimPos = targetPart.Position
            end
        end
    elseif TargetAllEnabled and targetPlayer and targetPlayer.Character then
        -- *** FIXED: Target All mode - SAFE PLAYER CHECK ***
        local targetPart = targetPlayer.Character:FindFirstChild(TargetPart or gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}))
        if targetPart and targetPart:IsDescendantOf(workspace) then
            local predicted = targetPart.Position + (targetPart.Velocity * (Settings.Prediction or LeadStrength or 0.1))
            if predicted and typeof(predicted) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10395,12180,11655,11970,5355}) then
                aimPos = predicted
                dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,6825,8610,7455,7245,8820,3360,6825,7980,7980,3360,4725,3360,8400,11340,10185,12705,10605,11970,6090,3360}) .. targetPlayer.Name)
            else
                aimPos = targetPart.Position
            end
        end
    else
        -- Fallback: Normal aim direction
        local aimDirection = (v_u_7.getAim and v_u_7.getAim(muzzlePos)) or CurrentCamera.CFrame.LookVector
        aimDirection = (CFrame.lookAt(Vector3.new(), aimDirection) * spreadCFrame).LookVector
        local raycastResult = workspace:Raycast(muzzlePos, aimDirection * INFINITE_RANGE, raycastParams)
        aimPos = raycastResult and raycastResult.Position or (muzzlePos + aimDirection * INFINITE_RANGE)
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,8295,11550,11340,12705,3360,4200,8190,11655,3360,8820,10185,11970,10815,10605,12180,4305}))
    end

    -- FALLBACK SAFETY
    if not aimPos or typeof(aimPos) ~= gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10395,12180,11655,11970,5355}) then
        aimPos = muzzlePos + (CurrentCamera.CFrame.LookVector * INFINITE_RANGE)
    end

    if not (Character and v_u_5 and muzzlePos and aimPos) then
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7665,11550,12390,10185,11340,11025,10500,3360,12075,10920,11655,11655,12180,3360,11760,10185,11970,10185,11445,10605,12180,10605,11970,12075}))
        return
    end

    local success, v17, v18, v19 = pcall(function()
        return v_u_7.shoot({
            Shooter = Character,
            Handle = v_u_5,
            ForcedOrigin = muzzlePos,
            AimPosition = aimPos,
            BeamColor = Color3.new(1, 0.545098, 0.14902),
            Range = INFINITE_RANGE
        })
    end)
    
    if success and v17 and v18 and v19 and mainRemotes then
        local evt = mainRemotes:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
        if evt then
            evt:FireServer(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({12600,7140,8715,10920,11655,8295,12180,7455,12285,11550,9450,10605,10605,7560,8295,8295,7140,9240,7140,7140}), v_u_5, muzzlePos, v17, v18, v19)
        end
    end
    
    v_u_13:FireServer()
end

local function initializeTool(newTool)
    if not newTool:IsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340})) then return end
    if not RapidFireEnabled then return end
    tool = newTool
    if TargetAllEnabled then
        notify(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,3990,3360,8820,10185,11970,10815,10605,12180,3360,6825,11340,11340}), gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340,3360,11025,11550,11025,12180,11025,10185,11340,11025,12810,10605,10500,6090,3360}) .. tool.Name .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({3360,12495,11025,12180,10920,3360,7665,11550,10710,11025,11550,11025,12180,10605,3360,8610,10185,11550,10815,10605,3360,10185,11550,10500,3360,7665,10815,11550,11655,11970,10605,3360,7980,11025,12075,12180}), 3)
    else
        notify(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,8295,11550,11340,12705}), gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340,3360,11025,11550,11025,12180,11025,10185,11340,11025,12810,10605,10500,6090,3360}) .. tool.Name .. gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({3360,12495,11025,12180,10920,3360,7665,11550,10710,11025,11550,11025,12180,10605,3360,8610,10185,11550,10815,10605}), 3)
    end
    dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340,3360,11025,11550,11025,12180,11025,10185,11340,11025,12810,10605,10500,6090,3360}) .. tool.Name)

    local v_u_1 = CFrame.new(-1, 0.4, 0)
    local v_u_2 = ReplicatedStorage
    local v_u_3 = LocalPlayer
    local v_u_4 = tool
    local v_u_5 = v_u_4:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10185,11550,10500,11340,10605}), 10)
    local v_u_6 = Character
    local v_u_7 = require(v_u_2:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,11655,10500,12285,11340,10605,12075}), 10):WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,12285,11550,7560,10185,11550,10500,11340,10605,11970}), 10))
    local v_u_8 = v_u_4:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6825,11445,11445,11655}), 10)
    local v_u_9 = v_u_4:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10185,11550,10815,10605}), 10)
    local v_u_12 = require(v_u_2:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,11655,10500,12285,11340,10605,12075}), 10):WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,10500}), 10)).new()
    local v_u_13 = v_u_4:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180})) or { FireServer = function() end }

    maid = v_u_12

    v_u_9.Value = INFINITE_RANGE

    -- *** YOUR ORIGINAL MOUSE INPUT - FIXED FOR TARGET ALL ***
    v_u_12:GiveTask(UserInputService.InputBegan:Connect(function(input, gp)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            mouseHeld = true
            while mouseHeld and v_u_4.Parent == v_u_6 and RapidFireEnabled do
                if TargetAllEnabled then
                    -- *** FIXED: Target All loops SAFELY ***
                    for _, player in ipairs(Players:GetPlayers()) do
                        if player ~= LocalPlayer and player.Character then
                            local humanoid = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
                            local bodyEffects = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
                            local ko = bodyEffects and bodyEffects:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7875,4830,8295}))
                            local isNotKnocked = not ko or ko.Value ~= true
                            local isNotGrabbed = not player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,8190,8715,8820,8610,6825,7665,8190,8820}))
                            if humanoid and humanoid.Health > 1 and isNotKnocked and isNotGrabbed then
                                shoot(player) -- *** FIXED: PASS PLAYER CORRECTLY ***
                            end
                        end
                    end
                elseif RapidFireOnlyEnabled and BulletTPEnabled and Target then
                    shoot(Target)
                else
                    shoot()
                end
                task.wait(customCooldown)
            end
        end
    end))

    v_u_12:GiveTask(UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            mouseHeld = false
        end
    end))

    v_u_12:GiveTask(v_u_8:GetPropertyChangedSignal(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10185,11340,12285,10605})):Connect(function()
        if v_u_8.Value < 99999 then
            v_u_8.Value = 99999
        end
    end))

    v_u_12:GiveTask(v_u_9:GetPropertyChangedSignal(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10185,11340,12285,10605})):Connect(function()
        if RapidFireEnabled and v_u_9.Value < INFINITE_RANGE then
            v_u_9.Value = INFINITE_RANGE
        end
    end))

    v_u_12:GiveTask(v_u_4.AncestryChanged:Connect(function()
        if not v_u_4:IsDescendantOf(v_u_3) then
            v_u_12:DoCleaning()
            tool = nil
        end
    end))

    v_u_12:GiveTask(RunService.Heartbeat:Connect(function()
        if RapidFireEnabled and v_u_4:IsDescendantOf(v_u_3) and v_u_9.Value < INFINITE_RANGE then
            v_u_9.Value = INFINITE_RANGE
        end
    end))
end

local function onCharacterChildAdded(obj)
    if obj:IsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340})) then
        task.wait(0.1)
        if RapidFireEnabled then
            initializeTool(obj)
        end
    end
end

local characterChildConn
local characterRemovingConn

local function onCharacterAdded(newChar)
    if characterChildConn then
        characterChildConn:Disconnect()
        characterChildConn = nil
    end
    if characterRemovingConn then
        characterRemovingConn:Disconnect()
        characterRemovingConn = nil
    end
    if maid and type(maid.DoCleaning) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({10710,12285,11550,10395,12180,11025,11655,11550}) then
        pcall(function() maid:DoCleaning() end)
        maid = nil
        tool = nil
    end

    Character = newChar

    characterChildConn = Character.ChildAdded:Connect(function(obj)
        onCharacterChildAdded(obj)
    end)

    task.spawn(function()
        task.wait(0.1)
        for _, c in ipairs(Character:GetChildren()) do
            if c:IsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340})) and RapidFireEnabled then
                initializeTool(c)
                break
            end
        end
    end)

    characterRemovingConn = LocalPlayer.CharacterRemoving:Connect(function()
        if characterChildConn then
            pcall(function() characterChildConn:Disconnect() end)
            characterChildConn = nil
        end
        if maid and type(maid.DoCleaning) == gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({10710,12285,11550,10395,12180,11025,11655,11550}) then
            pcall(function() maid:DoCleaning() end)
            maid = nil
            tool = nil
        end
    end)
end

if LocalPlayer.Character then
    onCharacterAdded(LocalPlayer.Character)
end

LocalPlayer.CharacterAdded:Connect(onCharacterAdded)

LocalPlayer:WaitForChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,10185,10395,11235,11760,10185,10395,11235}))
LocalPlayer.Backpack.ChildAdded:Connect(function(obj)
    if obj:IsA(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11655,11655,11340})) then
        obj.Equipped:Connect(function()
            task.wait(0.05)
            if RapidFireEnabled then
                initializeTool(obj)
            end
        end)
    end
end)

