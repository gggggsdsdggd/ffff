-- Mouse Input for Rapid Fire
UserInputService.InputBegan:Connect(function(input, gpe)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        local equippedGun = Client.Character
            and Client.Character:FindFirstChildOfClass(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,11655,11655,11340}))
        if equippedGun and GUN_NAMES[equippedGun.Name] then
            mouseHeld = true
            dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8085,11655,12285,12075,10605,3360,10920,10605,11340,10500,4620,3360,10815,12285,11550,3360,10605,11865,12285,11025,11760,11760,10605,10500,6090,3360}) .. equippedGun.Name)
        end
    end
end)

UserInputService.InputEnded:Connect(function(input, gpe)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        mouseHeld = false
        dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8085,11655,12285,12075,10605,3360,11970,10605,11340,10605,10185,12075,10605,10500}))
    end
end)

-- Detect Gun Unequip
Client.Character.ChildRemoved:Connect(function(child)
    if child:IsA(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,11655,11655,11340})) and GUN_NAMES[child.Name] then
        mouseHeld = false
        if GunMaids[child] then
            GunMaids[child]:Destroy()
            GunMaids[child] = nil
            PatchedGuns[child] = nil
            dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7455,12285,11550,3360,12285,11550,10605,11865,12285,11025,11760,11760,10605,10500,6090,3360}) .. child.Name .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({4620,3360,10395,11340,10605,10185,11550,10605,10500,3360,12285,11760,3360,8085,10185,11025,10500}))
        end
    end
end)

-- Beam Modification Logic
local recentShots = {}
local function addShot()
    local shotTime = tick()
    table.insert(recentShots, shotTime)
    for i = #recentShots, 1, -1 do
        if tick() - recentShots[i] > 3 then
            table.remove(recentShots, i)
        end
    end
end

local BeamConnections = {}
table.insert(
    BeamConnections,
    workspace.DescendantAdded:Connect(function(descendant)
        if not BeamVisualsEnabled or not descendant:IsA(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6930,10605,10185,11445})) then
            return
        end
        local isLocalPlayerBeam = false
        local debugReason = vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({})
        local character = Client.Character
        local equippedGun = character
            and character:FindFirstChildOfClass(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,11655,11655,11340}))
        if descendant.Attachment0 or descendant.Attachment1 then
            local beamPart = descendant.Attachment0
                    and descendant.Attachment0.Parent
                or descendant.Attachment1 and descendant.Attachment1.Parent
            if
                beamPart
                and (
                    beamPart:IsDescendantOf(character)
                    or (equippedGun and beamPart:IsDescendantOf(equippedGun))
                )
            then
                isLocalPlayerBeam = true
            else
                debugReason = debugReason
                    .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180,3360,11550,11655,12180,3360,11340,11025,11550,11235,10605,10500,3360,12180,11655,3360,7035,10920,10185,11970,10185,10395,12180,10605,11970,4935,8820,11655,11655,11340,6195,3360})
            end
        end
        if
            not isLocalPlayerBeam
            and descendant.Attachment0
            and character
            and character:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
        then
            local rootPos = character.HumanoidRootPart.Position
            local attachPos = descendant.Attachment0.WorldPosition
            local distance = (rootPos - attachPos).Magnitude
            if distance < 10 then
                isLocalPlayerBeam = true
            else
                debugReason = debugReason
                    .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6825,12180,12180,10185,10395,10920,11445,10605,11550,12180,5040,3360,12180,11655,11655,3360,10710,10185,11970,6090,3360})
                    .. distance
                    .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({3360,12075,12180,12285,10500,12075,6195,3360})
            end
        end
        if isLocalPlayerBeam then
            task.spawn(function()
                task.wait(0.01)
                if descendant.Parent then
                    local beamColor = TracerRGBEffectEnabled
                            and Color3.fromHSV((tick() % 5) / 5, 1, 1)
                        or BeamColor
                    descendant.Color = ColorSequence.new(beamColor)
                    descendant.Width0 = 0.3
                    descendant.Width1 = 0.3
                    descendant.Texture = vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({11970,10290,12600,10185,12075,12075,10605,12180,11025,10500,6090,4935,4935,5250,5460,5355,5775,5355,5775,5670,5565,5460})
                    descendant.Transparency = NumberSequence.new({
                        NumberSequenceKeypoint.new(0, 0.1),
                        NumberSequenceKeypoint.new(1, 0.3),
                    })
                    descendant.TextureSpeed = 4
                    descendant.LightEmission = 1
                    dbgWarn(
                        vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6930,10605,10185,11445,3360,11445,11655,10500,11025,10710,11025,10605,10500,6090,3360,8190,10185,11445,10605,6405})
                            .. descendant.Name
                            .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({4620,3360,7035,11655,11340,11655,11970,6405})
                            .. tostring(beamColor)
