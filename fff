local ScreenGui = Instance.new(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8715,10395,11970,10605,10605,11550,7455,12285,11025}))
ScreenGui.Name = vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({9450,10605,10605,7560,11655,11655,10500,8715,10605,12180,12180,11025,11550,10815,12075})
ScreenGui.Parent = PlayerGui
ScreenGui.ResetOnSpawn = false

local labelOffsetY = 0
local function CreateLabel(section, text)
    local label = Instance.new(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10605,12600,12180,7980,10185,10290,10605,11340}))
    label.Text = text
    label.TextSize = 15
    label.TextColor3 = Color3.new(1, 1, 1)
    label.BackgroundTransparency = 1
    label.Size = UDim2.new(1, 0, 0, 20)
    label.Position = UDim2.new(0, 0, 0, labelOffsetY)
    label.Parent = ScreenGui
    labelOffsetY = labelOffsetY + 25
    return label
end

