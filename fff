		return
	end
	if target == LocalPlayer then
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##################################################################################################################','#####################################################################################################','#######################################################################','#####################################################################################################################','##############################################################################################################','######################################################################','###############################################################################################################','##################################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','###################################################################################','####################################################################