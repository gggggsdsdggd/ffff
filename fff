-- Mouse Input for Rapid Fire
UserInputService.InputBegan:Connect(function(input, gpe)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        local equippedGun = Client.Character
            and Client.Character:FindFirstChildOfClass(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
        if equippedGun and GUN_NAMES[equippedGun.Name] then
            mouseHeld = true
            dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','********************************************************************************************************','*****************************************************************************************************','************************************************************************************************************','****************************************************************************************************','********************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. equippedGun.Name)
        end
    end
end)

UserInputService.InputEnded:Connect(function(input, gpe)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        mouseHeld = false
        dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
    end
end)

-- Detect Gun Unequip
Client.Character.ChildRemoved:Connect(function(child)
    if child:IsA(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'})) and GUN_NAMES[child.Name] then
        mouseHeld = false
        if GunMaids[child] then
            GunMaids[child]:Destroy()
            GunMaids[child] = nil
            PatchedGuns[child] = nil
            dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. child.Name .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************','********************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************************','****************************************************************************************************************','********************************','*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
        end
    end
end)

-- Beam Modification Logic
local recentShots = {}
local function addShot()
    local shotTime = tick()
    table.insert(recentShots, shotTime)
    for i = #recentShots, 1, -1 do
        if tick() - recentShots[i] > 3 then
            table.remove(recentShots, i)
        end
    end
end

local BeamConnections = {}
table.insert(
    BeamConnections,
    workspace.DescendantAdded:Connect(function(descendant)
        if not BeamVisualsEnabled or not descendant:IsA(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'******************************************************************','*****************************************************************************************************','*************************************************************************************************','*************************************************************************************************************'})) then
            return
        end
        local isLocalPlayerBeam = false
        local debugReason = aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({})
        local character = Client.Character
        local equippedGun = character
            and character:FindFirstChildOfClass(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
        if descendant.Attachment0 or descendant.Attachment1 then
            local beamPart = descendant.Attachment0
                    and descendant.Attachment0.Parent
                or descendant.Attachment1 and descendant.Attachment1.Parent
            if
                beamPart
                and (
                    beamPart:IsDescendantOf(character)
                    or (equippedGun and beamPart:IsDescendantOf(equippedGun))
                )
            then
                isLocalPlayerBeam = true
            else
                debugReason = debugReason
                    .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','********************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','***********************************************','************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************','***********************************************************','********************************'})
            end
        end
        if
            not isLocalPlayerBeam
            and descendant.Attachment0
            and character
            and character:FindFirstChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
        then
            local rootPos = character.HumanoidRootPart.Position
            local attachPos = descendant.Attachment0.WorldPosition
            local distance = (rootPos - attachPos).Magnitude
            if distance < 10 then
                isLocalPlayerBeam = true
            else
                debugReason = debugReason
                    .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','********************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************','******************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','**************************