				TPWalkKeyLabel.Text = YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,8400,3360,9135,10185,11340,11235,3360,7875,10605,12705,6090,3360}) .. input.KeyCode.Name
				dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,8400,3360,9135,10185,11340,11235,3360,11235,10605,12705,3360,12075,10605,12180,3360,12180,11655,6090,3360}) .. input.KeyCode.Name)
				connection:Disconnect()
