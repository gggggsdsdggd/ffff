-- Get Closest Target
local function GetClosestTarget()
    local Closest, MinDist = nil, math.huge
    local MousePos = Vector2.new(Mouse.X, Mouse.Y)
    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= Client and Player.Character then
            local Root = Player.Character:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
            local Humanoid = Player.Character:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,12285,11445,10185,11550,11655,11025,10500}))
            local ISNTKNOCKED = Player.Character:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
                and Player.Character.BodyEffects[vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7875,4830,8295})].Value ~= true
            local ISNTGRABBED = Player.Character:FindFirstChild(
                vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,7665,8190,8715,8820,8610,6825,7665,8190,8820})
            ) == nil
            if
                Root
                and Humanoid
                and Humanoid.Health > 1
                and ISNTKNOCKED
                and ISNTGRABBED
            then
                local screenPos, onScreen =
                    CurrentCamera:WorldToViewportPoint(Root.Position)
                if onScreen then
                    local Dist = (
                        Vector2.new(screenPos.X, screenPos.Y) - MousePos
                    ).Magnitude
                    if Dist < MinDist then
                        MinDist = Dist
                        Closest = Player
                    end
                end
            end
        end
    end
    return Closest
end

-- Keybind Handler
UserInputService.InputBegan:Connect(function(input, processed)
    if processed or input.UserInputType ~= Enum.UserInputType.Keyboard then
        return
    end
    -- Target Lock Key
    if input.KeyCode == Keybinds.TargetLock then
        if Target then
            notify(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName, 3)
            Target = nil
            Line.Visible = false
            Circle.Visible = false
            Text.Visible = false
            currentText = vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({})
            textIndex = 1
            if ViewEnabled then
                CurrentCamera.CameraSubject = Client.Character
                    and Client.Character:FindFirstChildWhichIsA(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,12285,11445,10185,11550,11655,11025,10500}))
            end
            dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,6090,3360}) .. input.KeyCode.Name)
        else
            if BulletTPEnabled then
                Target = GetClosestTarget()
                if Target then
                    notify(
                        vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}),
                        vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360}) .. Target.DisplayName,
                        3
                    )
                    if ViewEnabled and Target.Character then
                        local targetPart =
                            Target.Character:FindFirstChild(TargetPart)
                        if targetPart then
                            CurrentCamera.CameraSubject = targetPart
                        end
                    end
                    dbgWarn(
                        vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,11340,11655,10395,11235,10605,10500,6090,3360})
                            .. Target.DisplayName
                            .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({3360,12390,11025,10185,3360,11235,10605,12705,6090,3360})
                            .. input.KeyCode.Name
                    )
                else
                    notify(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8190,11655,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500}), 3)
                    dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8190,11655,3360,12180,10185,11970,10815,10605,12180,3360,10710,11655,12285,11550,10500,3360,10710,11655,11970,3360,11340,11655,10395,11235}))
                end
            else
                notify(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445}), vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7245,11550,10185,10290,11340,10605,3360,8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,12180,11655,3360,11340,11655,10395,11235,3360,10185,3360,12180,10185,11970,10815,10605,12180}), 3)
                dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,6825,11025,11445,3360,11550,11655,12180,3360,10605,11550,10185,10290,11340,10605,10500,3360,10710,11655,11970,3360,11340,11655,10395,11235,11025,11550,10815}))
            end
        end
    end
    -- Force Hit Key
    if input.KeyCode == Keybinds.ForceHit and ForceHitEnabled then
        if ForceHitTarget then
            notify(
                vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7350,11655,11970,10395,10605,3360,7560,11025,12180}),
                vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,6090,3360}) .. ForceHitTarget.DisplayName,
                3
            )
            ForceHitTarget = nil
            removeTracer()
            dbgWarn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7350,11655,11970,10395,10605,3360,7560,11025,12180,3360,12180,10185,11970,10815,10605,12180,3360,12285,11550,11340,11655,10395,11235,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,6090,3360}) .. input.KeyCode.Name)
        else
