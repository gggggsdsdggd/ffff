-- Target Visuals Controls
TargetVisualsSection:AddToggle({
    Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,11970,10185,10395,10605,11970,3360,7245,11550,10185,10290,11340,10605,10500}),
