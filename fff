			3
		)
		dbgWarn(
			cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************','*******************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','*************************************************************************************************