TargetAimSection:AddDropdown({
    Name = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8820,10185,11970,10815,10605,12180,3360,8400,10185,11970,12180}),
    Default = gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}),
    Values = { gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,10605,10185,10500}), gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}) },
    Callback = function(value)
        TargetPart = value
        Settings.Aimpart = value
        if ViewEnabled and (Target or TargetAllEnabled) then
            if TargetAllEnabled then
                for _, player in ipairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character then
                        local humanoid = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7560,12285,11445,10185,11550,11655,11025,10500}))
                        local isNotKnocked = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075})) and player.Character.BodyEffects[gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7875,4830,8295})].Value ~= true
                        local isNotGrabbed = player.Character:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,8190,8715,8820,8610,6825,7665,8190,8820})) == nil
                        if humanoid and humanoid.Health > 1 and isNotKnocked and isNotGrabbed then
                            local targetPart = player.Character:FindFirstChild(TargetPart)
                            if targetPart then
                                CurrentCamera.CameraSubject = targetPart
                                break
                            end
                        end
                    end
                end
            elseif Target and Target.Character then
                local targetPart = Target.Character:FindFirstChild(TargetPart)
                if targetPart then
                    CurrentCamera.CameraSubject = targetPart
                end
            end
        end
        dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8715,10605,11340,10605,10395,12180,10605,10500,3360,12180,10185,11970,10815,10605,12180,3360,11760,10185,11970,12180,6090,3360}) .. tostring(value))
    end,
})

