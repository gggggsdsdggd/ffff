	SettingsSection:AddColorPicker({
		Name = v[1],
		Default = v[2],
		Callback = function(val)
			Compkiller.Colors[v[1]:gsub(vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({'********************************'}), vWlwIUwCKdjKCgGcuayCkzTYwluhLYsI({}))] = val
			Compkiller:RefreshCurrentColor(val)
		end,
	})
end

