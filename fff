-- Notification Function
local function notify(title, text, dur)
    if SEND_NOTIFICATIONS then
        pcall(function()
            StarterGui:SetCore(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8715,10605,11550,10500,8190,11655,12180,11025,10710,11025,10395,10185,12180,11025,11655,11550}), {
                Title = title,
                Text = text,
                Duration = dur or 3,
            })
        end)
    end
end

-- Debug Function
local function dbgWarn(msg)
    if DEBUG_MODE then
        warn(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({9555,7140,7245,6930,8925,7455,9765,3360}) .. tostring(msg))
    end
end

-- Tracer Helper for Force Hit
local function createTracer()
    if tracerLine then
        tracerLine:Remove()
    end
    tracerLine = Drawing.new(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7980,11025,11550,10605}))
    tracerLine.Color = Color3.new(1, 0, 0)
    tracerLine.Thickness = 2
    tracerLine.Visible = false
end

local function updateTracer()
    if not (ForceHitEnabled and ForceHitTarget and tracerLine) then
        if tracerLine then
            tracerLine.Visible = false
        end
        return
    end

    local localChar = LocalPlayer.Character
    local targetChar = ForceHitTarget.Character
    if not localChar or not targetChar then
        tracerLine.Visible = false
        return
    end

    local cam = workspace.CurrentCamera
    local localHead = localChar:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,10605,10185,10500}))
    local targetHead = targetChar:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,10605,10185,10500}))

    if localHead and targetHead then
        local fromPos, onScreen1 = cam:WorldToViewportPoint(localHead.Position)
        local toPos, onScreen2 = cam:WorldToViewportPoint(targetHead.Position)
        if onScreen1 and onScreen2 then
            tracerLine.From = Vector2.new(fromPos.X, fromPos.Y)
            tracerLine.To = Vector2.new(toPos.X, toPos.Y)
            tracerLine.Visible = true
        else
            tracerLine.Visible = false
        end
    else
        tracerLine.Visible = false
    end
end

local function removeTracer()
    if tracerLine then
        tracerLine:Remove()
        tracerLine = nil
    end
end

-- Hit Sound
local function playHitSound()
    local head = LocalPlayer.Character
        and LocalPlayer.Character:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,10605,10185,10500}))
    if head then
        local sound = Instance.new(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8715,11655,12285,11550,10500}))
        sound.SoundId = hitSoundId
        sound.Volume = 1
        sound.Parent = head
        sound:Play()
        game.Debris:AddItem(sound, 2)
    end
end

-- Hit Chams Creation
local function createHitCham(targetChar)
    if
        not targetChar
        or not HitChamsEnabled
        or not targetChar:IsDescendantOf(workspace)
    then
        return
    end

    local chamModel = Instance.new(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8085,11655,10500,10605,11340}))
    chamModel.Name = vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,11025,12180,7035,10920,10185,11445})
    chamModel.Parent = workspace

    for _, partName in ipairs(BodyParts) do
        local part = targetChar:FindFirstChild(partName)
        if part and part:IsA(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6930,10185,12075,10605,8400,10185,11970,12180})) then
            local chamPart = Instance.new(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8400,10185,11970,12180}))
            chamPart.Size = part.Size
            chamPart.CFrame = part.CFrame
            chamPart.Anchored = true
            chamPart.CanCollide = false
            chamPart.Material = HitChamMaterial
            chamPart.Color = HitChamColor
            chamPart.Transparency = 0.5
            chamPart.Parent = chamModel
        end
    end

    if #chamModel:GetChildren() == 0 then
        chamModel:Destroy()
        return
    end

    local startTime = tick()
    local conn
    conn = RunService.RenderStepped:Connect(function()
        local elapsed = tick() - startTime
        if elapsed >= ChamFadeTime then
            chamModel:Destroy()
            conn:Disconnect()
        else
            local t = elapsed / ChamFadeTime
            for _, p in ipairs(chamModel:GetChildren()) do
                if p:IsA(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({6930,10185,12075,10605,8400,10185,11970,12180})) then
                    p.Transparency = 0.5 + 0.5 * t
                end
            end
        end
    end)
    dbgWarn(
        vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7035,11970,10605,10185,12180,10605,10500,3360,7560,11025,12180,7035,10920,10185,11445,3360,10710,11655,11970,3360})
            .. (targetChar.Name or vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({12285,11550,11235,11550,11655,12495,11550}))
            .. vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({3360,12495,11025,12180,10920,3360,11445,10185,12180,10605,11970,11025,10185,11340,6090,3360})
            .. tostring(HitChamMaterial)
    )
end

-- Fire Gun for Force Hit
local function fireGun()
    if not ForceHitEnabled or not ForceHitTarget then
        return
    end
    local char = LocalPlayer.Character
    local targetChar = ForceHitTarget.Character
    if not (char and targetChar) then
        return
    end
    local tool = char:FindFirstChildWhichIsA(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,11655,11655,11340}))
    if not tool or not tool:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,10185,11550,10500,11340,10605})) then
        return
    end

    local handle = tool.Handle
    local muzzle = handle:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8085,12285,12810,12810,11340,10605}))
        or { WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position }
    local targetHead = targetChar:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7560,10605,10185,10500}))
    if not targetHead or not muzzle.WorldPosition then
        return
    end

    local mainRemotes = ReplicatedStorage:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,12075}))
    local mainRemoteEvent = mainRemotes
        and mainRemotes:FindFirstChild(vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8085,10185,11025,11550,8610,10605,11445,11655,12180,10605,7245,12390,10605,11550,12180}))
    if mainRemoteEvent then
        local offset = Vector3.new(
            math.random(-5, 5) / 100,
            math.random(-5, 5) / 100,
            math.random(-5, 5) / 100
        )
        mainRemoteEvent:FireServer(
            vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8715,10920,11655,11655,12180,7455,12285,11550}),
            handle,
            muzzle.WorldPosition,
            targetHead.Position + offset,
            targetHead,
            Vector3.new(0, 0, 0)
        )
        lastShotTime = tick()
        playHitSound()
        if HitChamsEnabled then
            createHitCham(targetChar)
        end
    end
end

