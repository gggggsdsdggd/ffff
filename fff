    -- TP Walk Key
    if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
        tpwalking = not tpwalking
        notify(
            vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,8400,3360,9135,10185,11340,11235}),
            tpwalking and vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7245,11550,10185,10290,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,10290,11025,11550,10500}) or vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({7140,11025,12075,10185,10290,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,10290,11025,11550,10500}),
            3
        )
        dbgWarn(
            vTIuHxhAuMIfygbRZjvIyZqrcpsaCYTpULkXxvqhTyGNWYDzhtUtecjtFbrgHcZJsGTBijiMYAXmVznDEPfvReDURBZWztQPE({8820,8400,3360,9135,10185,11340,11235,3360,12180,11655,10815,10815,11340,10605,10500,3360,12390,11025,10185,3360,11235,10605,12705,6090,3360})
