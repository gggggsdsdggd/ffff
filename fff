-- Hook GunHandler Shoot
local function hookGunHandlerShoot()
    local success, err = pcall(function()
        local gunHandler =
            require(ReplicatedStorage.Modules:WaitForChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})))
        local oldShoot = gunHandler.shoot
        gunHandler.shoot = function(args)
            if RapidFireEnabled or TargetAll then
                args.Range = RapidFire.InfiniteRange
                dbgWarn(
                    aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
                        .. RapidFire.InfiniteRange
                        .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************','*********************************************************************************************************','**************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'})
                )
            end
            return oldShoot(args)
        end
    end)
    if not success then
        dbgWarn(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************'}) .. tostring(err))
    end
end

task.spawn(function()
    applyWallbang()
    hookGunHandlerShoot()
end)

-- Rapid Fire Logic
local function randomVector3(maxOffset)
    return Vector3.new(
        (math.random() - 0.5) * 2 * maxOffset,
        (math.random() - 0.5) * 2 * maxOffset,
        (math.random() - 0.5) * 2 * maxOffset
    )
end

local function patchGun(tool)
    if not GUN_NAMES[tool.Name] or PatchedGuns[tool] then
        return
    end
    PatchedGuns[tool] = true
    local ammo = tool:WaitForChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}))
    local handle = tool:WaitForChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
    local gunHandler =
        require(ReplicatedStorage.Modules:WaitForChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})))
    local remote = tool:WaitForChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'}))
    local range = tool:WaitForChild(aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************'}))
    local maid = require(ReplicatedStorage.Modules.Maid).new()
    GunMaids[tool] = maid

    applyWallbang()

    task.spawn(function()
        while tool.Parent and tool.Parent == Client.Character do
            local success, err = pcall(function()
                ammo.Value = RapidFire.InfiniteAmmo
                range.Value = RapidFire.InfiniteRange
                dbgWarn(
                    aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************'})
                        .. tool.Name
                        .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************','*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','*************************************************************'})
                        .. ammo.Value
                        .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
                        .. range.Value
                )
            end)
            if not success then
                dbgWarn(
                    aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*************************************************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','***********************************************','******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
                        .. tool.Name
                        .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'**********************************************************','********************************'})
                        .. tostring(err)
                )
            end
            task.wait(0.1)
        end
        maid:Destroy()
        GunMaids[tool] = nil
        PatchedGuns[tool] = nil
        dbgWarn(
            aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. tool.Name .. aATxfIATWZxSDZDJFEhZCqLiRnPxUIulNyGsUAgeAWauziLRRKLyqCWBPxdsMAXuSTTZXGJshROCYaQRMfRXpKrxtNGipuf({'********************************','*********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************************','******************************************************************************************************************','**************