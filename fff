local function removeLabel(player)
    if player.Character and player.Character:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({7560,10605,10185,10500})) then
        local head = player.Character.Head
        if LocalPlayer.UserId == MAIN_USERID and head:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({6825,11340,12180,8925,12075,10605,11970,7980,10185,10290,10605,11340})) then
            head.AltUserLabel:Destroy()
        elseif LocalPlayer.UserId ~= MAIN_USERID and LocalPlayer:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) and head:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8295,12495,11550,10605,11970,7980,10185,10290,10605,11340})) then
            head.OwnerLabel:Destroy()
        end
    end
end

local function updateLabels()
    if not LocalPlayer:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then return end
    for _, player in ipairs(Players:GetPlayers()) do
        if (LocalPlayer.UserId == MAIN_USERID and player ~= LocalPlayer and player:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970}))) or (LocalPlayer.UserId ~= MAIN_USERID and player.UserId == MAIN_USERID) then
            addLabel(player)
        else
            removeLabel(player)
        end
    end
end

if LocalPlayer:FindFirstChild(HmWzEFozgRHBjqYhbxETuNeOGQJojqZatUKQITxDKZAQlQJvHGnhlaXhvARUekfLDfOzbLfHsSFxUXvJCnlSVcmOao({8715,10395,11970,11025,11760,12180,8925,12075,10605,11970})) then
    RunService.Heartbeat:Connect(updateLabels)
    Players.PlayerAdded:Connect(updateLabels)
    Players.PlayerRemoving:Connect(updateLabels)
end

LocalPlayer.CharacterAdded:Connect(function()
    task.wait(1)
