-- TP Walk Keybind
MovementSection:AddButton({
	Name = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','****************************************','********************************************************************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','********************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','*************************************************************************************************','******************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','***************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************'}),
	Callback = function()
		local listening = true
		notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','********************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','**********************************************','**********************************************','**********************************************'}), 5)
		local connection
		connection = UserInputService.InputBegan:Connect(
			function(input, processed)
				if
					processed
					or input.UserInputType ~= Enum.UserInputType.Keyboard
					or not listening
				then
					return
				end
				listening = false
				Keybinds.TPWalk = input.KeyCode
				TPWalkKeyLabel.Text = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name
				dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name)
				connection:Disconnect()
			end
		)
	end,
})

-- TP Walk Speed Slider
MovementSection:AddSlider({
	Name = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***********************************************************************************','****************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}),
	Min = 0,
	Max = 20,
	Default = 10,
	Callback = function(value)
		tpWalkSpeed = value
		notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','****************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','**********************************************************','********************************'}) .. tostring(value), 3)
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','***********************************************************************************','****************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','**********************************************************','********************************'}) .. tostring(value))
	end,
})

-- Utility Functions (continued)
local function getEquippedGun(plr)
	for _, tool in ipairs(plr.Character and plr.Character:GetChildren() or {}) do
		if tool:IsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'})) and GUN_NAMES[tool.Name] then
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*********************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************','********************************','**********************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'})
					.. tool.Name
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************'})
					.. plr.Name
			)
			return tool
		end
	end
	dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*********************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','*******************************************************************************************************************','*********************************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************'}) .. plr.Name)
	return nil
end

local function getClosestTargetToPlayer(plr)
	local closest, shortest = nil, math.huge
	local myHRP = plr.Character
		and plr.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
	if not myHRP then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************','************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','************************************************************************************','***************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************'})
				.. plr.Name
		)
		return nil
	end
	for _, enemy in pairs(Players:GetPlayers()) do
		if
			enemy ~= plr
			and enemy.Character
			and enemy.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
		then
			local humanoid = enemy.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			local bodyEffects = enemy.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','*********************************************************************','******************************************************************************************************','******************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************'}))
			local isKO = bodyEffects and bodyEffects:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***************************************************************************','**********************************************','*******************************************************************************'}))
			if
				humanoid
				and humanoid.Health > 0
				and not (isKO and isKO.Value == true)
			then
				local dist = (
					enemy.Character.HumanoidRootPart.Position - myHRP.Position
				).Magnitude
				if dist < shortest and dist <= maxAssistDistance then
					shortest = dist
					closest = enemy
					dbgWarn(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************','************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','************************************************************************************','***************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','**********************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************'})
							.. enemy.Name
							.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*************************************************************************************************','********************************************************************************************************************','********************************','****************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','********************************'})
							.. dist
					)
				end
			else
				dbgWarn(
					dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************','************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','************************************************************************************','***************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','***********************************************************************************','***********************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************'})
						.. enemy.Name
						.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','****************************************','************************************************************************','*****************************************************************************************************','*************************************************************************************************','************************************************************************************************************','********************************************************************************************************************','********************************************************************************************************','*************************************************************'})
						.. (humanoid and humanoid.Health or dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************************************','*********************************************************************************************************','************************************************************************************************************'}))
						.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','***************************************************************************','*******************************************************************************','*************************************************************'})
						.. tostring(isKO and isKO.Value)
				)
			end
		end
	end
	if not closest then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************','************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','************************************************************************************','***************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','**********************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. plr.Name
		)
	end
	return closest
end

local function fireGunForPlayer(plr)
	local gun = getEquippedGun(plr)
	if not gun then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','*******************************************************************************************************************','*********************************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. plr.Name)
		return
	end
	local handle = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
	local remote = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'}))
	local ammo = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}))
	local range = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************'}))
	if not (handle and ammo and range) then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','*****************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','***************************************************************************************************','***************************************************************************************************************','*************************************************************************************************************','****************************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'})
				.. gun.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','****************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************'})
				.. tostring(handle)
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','*************************************************************'})
				.. tostring(ammo)
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
				.. tostring(range)
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************'})
		)
		return
	end
	ammo.Value, range.Value = infiniteAmmo, 999999999
	local gunHandler
	local success, result = pcall(function()
		return require(ReplicatedStorage.Modules.GunHandler)
	end)
	if not success then
		warn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','**********************************************************************','*************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','*************************************************************************************************','****************************************************************************************************','********************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. plr.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************'})
				.. result
		)
		return
	end
	gunHandler = result
	local muzzle = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************','*****************************************************************************************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','********************************************************************************************************************'}))
		and gun.Default:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************'}))
		and gun.Default.Mesh:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
	if not muzzle then
		muzzle = {
			WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position,
		}
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','*************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. gun.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*********************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','******************************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','*************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','********************************','****************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'})
		)
	end
	local target = getClosestTargetToPlayer(plr)
	if
		not target
		or not target.Character
		or not target.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
	then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','**********************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. plr.Name)
		return
	end
	if target == LocalPlayer then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','***********************************************************************************','***********************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','*************************************************************************************************','************************************************************************************************************','********************************','****************************************************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************'}) .. plr.Name)
		return
	end
	local targetPos = target.Character.HumanoidRootPart.Position
	for i = 1, assistBulletsPerShot do
		local aimOffset = Vector3.new(
			math.random(-assistSpread * 100, assistSpread * 100) / 100,
			math.random(-assistSpread * 100, assistSpread * 100) / 100,
			math.random(-assistSpread * 100, assistSpread * 100) / 100
		)
		local shootPos = targetPos + aimOffset
		local success, vShoot, v2, v3 = pcall(function()
			setidentity(2)
			local result = {
				gunHandler.shoot({
					Shooter = plr.Character,
					Handle = handle,
					ForcedOrigin = muzzle.WorldPosition,
					AimPosition = shootPos,
					BeamColor = Color3.new(1, 0.545, 0.149),
					Range = range.Value,
				}),
			}
			setidentity(8)
			return unpack(result)
		end)
		if not success then
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
					.. plr.Name
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************'})
					.. tostring(vShoot)
			)
			return
		end
		ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
			handle,
			muzzle.WorldPosition,
			vShoot,
			v2,
			v3
		)
		if remote then
			remote:FireServer(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
		end
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************'})
				.. i
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. plr.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*************************************************************************************************','********************************************************************************************************************','********************************'})
				.. target.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
				.. tostring(shootPos)
		)
		task.wait(assistShotDelay)
	end
end

local function fireLagBulletsAroundPlayer(plr)
	if
		not plr
		or not plr.Character
		or not plr.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
	then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','**********************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. (plr and plr.Name or dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************************************','*********************************************************************************************************','************************************************************************************************************'}))
		)
		return
	end
	local gun = getEquippedGun(plr)
	if not gun then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','*******************************************************************************************************************','*********************************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. plr.Name
		)
		return
	end
	local handle = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
	local remote = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'}))
	local ammo = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}))
	local range = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************'}))
	if not (handle and ammo and range) then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','*****************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','***************************************************************************************************','***************************************************************************************************************','*************************************************************************************************************','****************************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'})
				.. gun.Name
		)
		return
	end
	ammo.Value, range.Value = infiniteAmmo, 999999999
	local gunHandler
	local success, result = pcall(function()
		return require(ReplicatedStorage.Modules.GunHandler)
	end)
	if not success then
		warn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','**********************************************************************','*************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','*************************************************************************************************','****************************************************************************************************','********************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. plr.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************'})
				.. result
		)
		return
	end
	gunHandler = result
	local muzzle = gun:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************','*****************************************************************************************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','********************************************************************************************************************'}))
		and gun.Default:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************'}))
		and gun.Default.Mesh:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
	if not muzzle then
		muzzle = {
			WorldPosition = (handle.CFrame * CFrame.new(-1, 0.4, 0)).Position,
		}
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','******************************************************************************','***************************************************************************************************************','********************************','*************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. gun.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*********************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','******************************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','*************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','********************************','****************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'})
		)
	end
	local basePos = muzzle.WorldPosition
	local centerPos = plr.Character.HumanoidRootPart.Position
	for i = 1, lagBulletsPerShot do
		local aimOffset = Vector3.new(
			math.random(-lagSpread * 100, lagSpread * 100) / 100,
			math.random(-lagSpread * 100, lagSpread * 100) / 100,
			math.random(-lagSpread * 100, lagSpread * 100) / 100
		)
		local aimPos = centerPos + aimOffset
		local success, shootResult, hitPos, target = pcall(function()
			setidentity(2)
			local result = {
				gunHandler.shoot({
					Shooter = plr.Character,
					Handle = handle,
					ForcedOrigin = basePos,
					AimPosition = aimPos,
					BeamColor = Color3.new(1, 0, 0),
					Range = 999999999,
				}),
			}
			setidentity(8)
			return unpack(result)
		end)
		if not success then
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
					.. plr.Name
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************'})
					.. tostring(shootResult)
			)
			return
		end
		ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
			handle,
			basePos,
			shootResult,
			hitPos,
			target
		)
		if remote then
			remote:FireServer(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
		end
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************','*************************************************************************************************','*******************************************************************************************************','******************************************************************','*********************************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************','******************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','************************************************************************************************************','*************************************************************************************************','*******************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************'})
				.. i
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
				.. plr.Name
		)
		task.wait(lagShotDelay)
	end
end

-- Get Closest Target
local function GetClosestTarget()
	local Closest, MinDist = nil, math.huge
	local MousePos = Mouse.Hit.Position
	for _, Player in ipairs(Players:GetPlayers()) do
		if Player ~= Client and Player.Character then
			local Root = Player.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
			local Humanoid = Player.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			local ISNTKNOCKED = Player.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************','***************************************************************************************************************','****************************************************************************************************','*************************************************************************************************************************','*********************************************************************','******************************************************************************************************','******************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*******************************************************************************************************************'}))
				and Player.Character.BodyEffects[dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***************************************************************************','**********************************************','*******************************************************************************'})].Value ~= true
			local ISNTGRABBED = Player.Character:FindFirstChild(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************','**********************************************************************************','*****************************************************************','******************************************************************','******************************************************************','*************************************************************************','******************************************************************************','***********************************************************************','***********************************************************************************************','*******************************************************************','*******************************************************************************','*************************************************************************','******************************************************************************','***********************************************************************************','************************************************************************************','**********************************************************************************','*****************************************************************','*************************************************************************','******************************************************************************','************************************************************************************'})
			) == nil
			if
				Root
				and Humanoid
				and Humanoid.Health > 1
				and ISNTKNOCKED
				and ISNTGRABBED
			then
				local Dist = (Root.Position - MousePos).Magnitude
				if Dist < MinDist then
					MinDist = Dist
					Closest = Player
				end
			end
		end
	end
	return Closest
end

-- Keybind Handler
UserInputService.InputBegan:Connect(function(input, processed)
	if processed or input.UserInputType ~= Enum.UserInputType.Keyboard then
		return
	end
	-- Target Lock Key
	if input.KeyCode == Keybinds.TargetLock then
		if Target then
			notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. Target.DisplayName, 3)
			Target = nil
			Line.Visible = false
			Circle.Visible = false
			Text.Visible = false
			currentText = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
			textIndex = 1
			if ViewEnabled then
				CurrentCamera.CameraSubject = Client.Character
					and Client.Character:FindFirstChildWhichIsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			end
			dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'}) .. input.KeyCode.Name)
		else
			if BulletTPEnabled then
				Target = GetClosestTarget()
				if Target then
					notify(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}),
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. Target.DisplayName,
						3
					)
					if ViewEnabled and Target.Character then
						local targetPart =
							Target.Character:FindFirstChild(TargetPart)
						if targetPart then
							CurrentCamera.CameraSubject = targetPart
						end
					end
					dbgWarn(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'})
							.. Target.DisplayName
							.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'})
							.. input.KeyCode.Name
					)
				else
					notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************','***************************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************'}), 3)
					dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************','***************************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************'}))
				end
			else
				notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*********************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','********************************','*************************************************************************************************','********************************','********************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}), 3)
				dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************'}))
			end
		end
	end
	-- TP Walk Key
	if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
		tpwalking = not tpwalking
		notify(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************'}),
			tpwalking and dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*********************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************'}) or dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************'}),
			3
		)
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************','*******************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'})
				.. input.KeyCode.Name
				.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','***********************************************************************************','********************************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','**********************************************************','********************************'})
				.. tostring(tpwalking)
		)
	end
end)

-- Mouse Input for Rapid Fire
UserInputService.InputBegan:Connect(function(input, gpe)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		local equippedGun = Client.Character
			and Client.Character:FindFirstChildOfClass(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
		if equippedGun and GUN_NAMES[equippedGun.Name] then
			mouseHeld = true
			dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','********************************************************************************************************','*****************************************************************************************************','************************************************************************************************************','****************************************************************************************************','********************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. equippedGun.Name)
		end
	end
end)

UserInputService.InputEnded:Connect(function(input, gpe)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		mouseHeld = false
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
	end
end)

-- Detect Gun Unequip
Client.Character.ChildRemoved:Connect(function(child)
	if child:IsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'})) and GUN_NAMES[child.Name] then
		mouseHeld = false
		if GunMaids[child] then
			GunMaids[child]:Destroy()
			GunMaids[child] = nil
			PatchedGuns[child] = nil
			dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************','*********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'}) .. child.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************************','****************************************************************************************************************','********************************','*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
		end
	end
end)

-- Beam Modification Logic
local recentShots = {}
local function addShot()
	local shotTime = tick()
	table.insert(recentShots, shotTime)
	for i = #recentShots, 1, -1 do
		if tick() - recentShots[i] > 3 then
			table.remove(recentShots, i)
		end
	end
end

local BeamConnections = {}
table.insert(
	BeamConnections,
	workspace.DescendantAdded:Connect(function(descendant)
		if not BeamVisualsEnabled or not descendant:IsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************','*****************************************************************************************************','*************************************************************************************************','*************************************************************************************************************'})) then
			return
		end
		local isLocalPlayerBeam = false
		local debugReason = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
		local character = Client.Character
		local equippedGun = character
			and character:FindFirstChildOfClass(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
		if descendant.Attachment0 or descendant.Attachment1 then
			local beamPart = descendant.Attachment0
					and descendant.Attachment0.Parent
				or descendant.Attachment1 and descendant.Attachment1.Parent
			if
				beamPart
				and (
					beamPart:IsDescendantOf(character)
					or (equippedGun and beamPart:IsDescendantOf(equippedGun))
				)
			then
				isLocalPlayerBeam = true
			else
				debugReason = debugReason
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','********************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','***********************************************','************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************','***********************************************************','********************************'})
			end
		end
		if
			not isLocalPlayerBeam
			and descendant.Attachment0
			and character
			and character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
		then
			local rootPos = character.HumanoidRootPart.Position
			local attachPos = descendant.Attachment0.WorldPosition
			local distance = (rootPos - attachPos).Magnitude
			if distance < 10 then
				isLocalPlayerBeam = true
			else
				debugReason = debugReason
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','********************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************','******************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************'})
					.. distance
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*******************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************************','****************************************************************************************************','*******************************************************************************************************************','***********************************************************','********************************'})
			end
		end
		if isLocalPlayerBeam then
			task.spawn(function()
				task.wait(0.01)
				if descendant.Parent then
					local beamColor = TracerRGBEffectEnabled
							and Color3.fromHSV((tick() % 5) / 5, 1, 1)
						or BeamColor
					descendant.Color = ColorSequence.new(beamColor)
					descendant.Width0 = 0.3
					descendant.Width1 = 0.3
					descendant.Texture = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************************************************','**************************************************************************************************','************************************************************************************************************************','*************************************************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************','***********************************************','***********************************************','**************************************************','****************************************************','***************************************************','*******************************************************','***************************************************','*******************************************************','******************************************************','*****************************************************','****************************************************'})
					descendant.Transparency = NumberSequence.new({
						NumberSequenceKeypoint.new(0, 0.1),
						NumberSequenceKeypoint.new(1, 0.3),
					})
					descendant.TextureSpeed = 4
					descendant.LightEmission = 1
					dbgWarn(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************','*****************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','********************************','*************************************************************************************************************','***************************************************************************************************************','****************************************************************************************************','*********************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************','******************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','*************************************************************'})
							.. descendant.Name
							.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*******************************************************************','***************************************************************************************************************','************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*************************************************************'})
							.. tostring(beamColor)
					)
				end
			end)
		else
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*********************************************','****************************************************************************','***************************************************************************************************************','***************************************************************************************************','*************************************************************************************************','************************************************************************************************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************','*****************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','********************************','*********************************************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************'})
					.. descendant.Name
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','**********************************************************************************','*****************************************************************************************************','*************************************************************************************************','*******************************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
					.. debugReason
			)
		end
	end)
)

-- Tracer, FOV, and Text Animation
RunService.Heartbeat:Connect(function()
	if Target and BulletTPEnabled and TracerEnabled then
		local targetPart = Target.Character
			and Target.Character:FindFirstChild(Settings.Aimpart)
		if targetPart then
			local predictedPos = targetPart.Position
				+ (targetPart.Velocity * Settings.Prediction)
			local vector, onScreen =
				CurrentCamera:WorldToViewportPoint(predictedPos)
			local cameraPos = CurrentCamera.CFrame.Position
			local direction = (predictedPos - cameraPos).Unit
			local forward = CurrentCamera.CFrame.LookVector
			local dot = direction:Dot(forward)
			local screenPos
			if dot < 0 then
				local screenWidth = CurrentCamera.ViewportSize.X
				local screenHeight = CurrentCamera.ViewportSize.Y
				local aspectRatio = screenWidth / screenHeight
				local fov = CurrentCamera.FieldOfView
				local tanFov = math.tan(math.rad(fov / 2))
				local cameraRight = CurrentCamera.CFrame.RightVector
				local cameraUp = CurrentCamera.CFrame.UpVector
				local x = direction:Dot(cameraRight)
					/ (direction:Dot(forward) * tanFov * aspectRatio)
				local y = direction:Dot(cameraUp)
					/ (direction:Dot(forward) * tanFov)
				local clampX = math.clamp(x, -1, 1) * screenWidth / 2
					+ screenWidth / 2
				local clampY = math.clamp(y, -1, 1) * screenHeight / 2
					+ screenHeight / 2
				screenPos = Vector2.new(clampX, clampY + Inset)
			else
				screenPos = Vector2.new(vector.X, vector.Y)
			end
			local tracerColor = TracerRGBEffectEnabled
					and Color3.fromHSV((tick() % 5) / 5, 1, 1)
				or Settings.Color
			Line.Color = tracerColor
			Line.Transparency = Settings.Transparency
			Line.Thickness = Settings.Thickness
			Line.From = Vector2.new(Mouse.X, Mouse.Y + Inset)
			Line.To = screenPos
			Line.Visible = true
		else
			Line.Visible = false
		end
	else
		Line.Visible = false
	end
	if FOVEnabled then
		local fovColor = FOVRGBEnabled
				and Color3.fromHSV((tick() % 5) / 5, 1, 1)
			or Color3.fromRGB(255, 255, 255)
		Circle.Position = Vector2.new(Mouse.X, Mouse.Y + Inset)
		Circle.Visible = true
		Circle.Thickness = Settings.FOVThickness
		Circle.Radius = Settings.FOVRadius
		Circle.Color = fovColor
		if BulletTPEnabled and Target and Target.Character then
			local targetName = Target.DisplayName or dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
			if tick() - lastTextUpdate >= textUpdateDelay then
				if textIndex <= #targetName then
					currentText = string.sub(targetName, 1, textIndex)
					textIndex = textIndex + 1
				else
					textIndex = 1
					currentText = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
				end
				lastTextUpdate = tick()
			end
			Text.Text = currentText
			Text.Size = 16
			Text.Color = fovColor
			Text.Position =
				Vector2.new(Mouse.X, Mouse.Y + Inset + Settings.FOVRadius + 10)
			Text.Visible = true
			Text.Center = true
			Text.Outline = true
		else
			Text.Visible = false
			currentText = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
			textIndex = 1
		end
	else
		Circle.Visible = false
		Text.Visible = false
		currentText = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
		textIndex = 1
	end
	-- Assist Others firing
	if AssistOthers.Enabled then
		local now = tick()
		if now - lastAssistTime >= AssistOthers.Cooldown then
			for _, id in ipairs(AssistOthers.AllowedPlayers) do
				local plr = Players:GetPlayerByUserId(id)
				if
					plr
					and plr.Character
					and plr.Character:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
					and getEquippedGun(plr)
				then
					pcall(function()
						fireGunForPlayer(plr)
					end)
				else
					dbgWarn(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*******************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','********************************','*******************************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************************','****************************************************************************************************','********************************'})
							.. id
							.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************','********************************************************************************','************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************'})
							.. tostring(plr)
							.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************'})
							.. tostring(plr and plr.Character)
					)
				end
			end
			lastAssistTime = now
		end
	end
	-- Lag bullets (all)
	if Lag.AllEnabled then
		local now = tick()
		if now - lastLagTime >= Lag.DelayTime then
			for _, plr in ipairs(Players:GetPlayers()) do
				if plr ~= LocalPlayer then
					pcall(function()
						fireLagBulletsAroundPlayer(plr)
					end)
				end
			end
			lastLagTime = now
		end
	end
end)

-- TP Walk Logic
RunService.Heartbeat:Connect(function(delta)
	if tpWalkEnabled and tpwalking then
		local chr = Client.Character
		local hum = chr and chr:FindFirstChildWhichIsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
		local root = chr and chr:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
		if chr and hum and root and hum.MoveDirection.Magnitude > 0 then
			local success, err = pcall(function()
				root.CFrame = root.CFrame
					+ (hum.MoveDirection * tpWalkSpeed * delta * 10)
			end)
			if not success then
				-- Fallback to Humanoid:MoveTo
				local targetPos = root.Position
					+ (hum.MoveDirection * tpWalkSpeed * delta * 10)
				hum:MoveTo(targetPos)
				dbgWarn(
					dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','*******************************************************************','**********************************************************************','******************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************************','********************************','*********************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*****************************************************************************','***************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','************************************************************************************','***************************************************************************************************************','**********************************************************','********************************'}) .. tostring(err)
				)
			else
				dbgWarn(
					dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','*************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************','*****************************************************************************','***************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**************************************************************************************************','*************************************************************************************************************************','********************************'})
						.. tostring(
							hum.MoveDirection * tpWalkSpeed * delta * 10
						)
				)
			end
		else
			dbgWarn(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************','********************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************','*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************'})
					.. tostring(chr)
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','*************************************************************'})
					.. tostring(hum)
					.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*************************************************************'})
					.. tostring(root)
			)
		end
	end
end)

-- Prediction Function
local function PredictPosition(targetPart, bulletSpeed)
	if not targetPart or not targetPart:IsDescendantOf(workspace) then
		return targetPart and targetPart.Position or Vector3.zero
	end
	local char = Client.Character
	local hrp = char and char:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','**********************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************'}))
	if not hrp then
		return targetPart.Position
	end
	local playerPos = hrp.Position
	local targetPos = targetPart.Position
	local targetVel = targetPart.Velocity
	local r = targetPos - playerPos
	local v = targetVel
	local s = bulletSpeed
	local a = v:Dot(v) - (s * s)
	local b = 2 * r:Dot(v)
	local c = r:Dot(r)
	local t
	local EPS = 1e-6
	if math.abs(a) < EPS then
		if math.abs(b) < EPS then
			return targetPos
		end
		t = -c / b
	else
		local disc = b * b - 4 * a * c
		if disc < 0 then
			return targetPos
		end
		local sqrtDisc = math.sqrt(disc)
		local t1 = (-b - sqrtDisc) / (2 * a)
		local t2 = (-b + sqrtDisc) / (2 * a)
		t = (t1 > 0 and t1) or (t2 > 0 and t2) or nil
	end
	if not t or t <= 0 or t > 3 then
		return targetPos
	end
	local predicted = targetPos + v * t
	predicted = targetPos:Lerp(predicted, LeadStrength)
	local ping = 0.08
	local ok, p = pcall(function()
		return Client:GetNetworkPing()
	end)
	if ok and type(p) == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','**************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}) then
		ping = p
	end
	ping = math.clamp(ping, 0.05, 0.5)
	predicted += v * ping
	if v.Magnitude > 0 then
		predicted += v.Unit * 0.1
	end
	return predicted
end

-- Bullet Hook for Target Aim
local function InstallBulletHook()
	local mt = getrawmetatable(game)
	setreadonly(mt, false)
	local previous = mt.__index
	local function hook(self, key)
		local ok, result = pcall(function()
			if
				not checkcaller()
				and self == Mouse
				and BulletTPEnabled
				and not RapidFireEnabled
				and not TargetAll
				and Target
				and (
					key == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************','********************************************************************************************************************'})
					or (key == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}) and game.PlaceId == 2788229376)
				)
			then
				local part = Target.Character
					and Target.Character:FindFirstChild(TargetPart)
				if part then
					local pos = PredictPosition(part, BulletSpeed)
					if typeof(pos) == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','***************************************************'}) then
						return CFrame.new(pos)
					end
				end
			end
			return previous(self, key)
		end)
		if ok then
			return result
		else
			return previous(self, key)
		end
	end
	local newIndex = newcclosure(hook)
	mt.__index = newIndex
	setreadonly(mt, true)
	return newIndex
end

local currentMouseIndex = InstallBulletHook()
task.spawn(function()
	while true do
		task.wait(0.25)
		local success, mt = pcall(getrawmetatable, game)
		if success and mt then
			local success_index, index = pcall(function()
				return mt.__index
			end)
			if success_index and index and index ~= currentMouseIndex then
				dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***************************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','****************************************************************************************************','***************************************************************************************************************','*******************************************************************************************************','********************************','****************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***********************************************************************************************','***********************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','************************************************************************************************************************','********************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************'}))
				currentMouseIndex = InstallBulletHook()
			end
		end
	end
end)

-- Wallbang Module
local function applyWallbang()
	local success, err = pcall(function()
		local Handler = ReplicatedStorage:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
		if not Handler then
			error(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*****************************************************************************','***************************************************************************************************************','****************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************','**************************************************************************************************************','********************************','**********************************************************************************','*****************************************************************************************************','****************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','***********************************************************************************','********************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************'}))
		end
		local Module = require(Handler)
		Module.Ignored = {
			workspace:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************','*****************************************************************************************************','********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'})),
			workspace:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*****************************************************************','********************************************************************************'})),
			workspace:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'})),
		}
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','**********************************************************','********************************','*************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**************************************************************************************','*****************************************************************************************************','********************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************','********************************','*****************************************************************************','*****************************************************************','********************************************************************************','********************************************','********************************','*************************************************************************','*******************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}))
	end)
	if not success then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************','**************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*****************************************************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************'}) .. tostring(err))
	end
end

-- Hook GunHandler Shoot
local function hookGunHandlerShoot()
	local success, err = pcall(function()
		local gunHandler =
			require(ReplicatedStorage.Modules:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})))
		local oldShoot = gunHandler.shoot
		gunHandler.shoot = function(args)
			if RapidFireEnabled or TargetAll then
				args.Range = RapidFire.InfiniteRange
				dbgWarn(
					dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************','***************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
						.. RapidFire.InfiniteRange
						.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*********************************************************************************************************','**************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'})
				)
			end
			return oldShoot(args)
		end
	end)
	if not success then
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','***********************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************','********************************'}) .. tostring(err))
	end
end

task.spawn(function()
	applyWallbang()
	hookGunHandlerShoot()
end)

-- Rapid Fire Logic
local function randomVector3(maxOffset)
	return Vector3.new(
		(math.random() - 0.5) * 2 * maxOffset,
		(math.random() - 0.5) * 2 * maxOffset,
		(math.random() - 0.5) * 2 * maxOffset
	)
end

local function patchGun(tool)
	if not GUN_NAMES[tool.Name] or PatchedGuns[tool] then
		return
	end
	PatchedGuns[tool] = true
	local ammo = tool:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}))
	local handle = tool:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
	local gunHandler =
		require(ReplicatedStorage.Modules:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','************************************************************************','*************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})))
	local remote = tool:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'}))
	local range = tool:WaitForChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************'}))
	local maid = require(ReplicatedStorage.Modules.Maid).new()
	GunMaids[tool] = maid

	applyWallbang()

	task.spawn(function()
		while tool.Parent and tool.Parent == Client.Character do
			local success, err = pcall(function()
				ammo.Value = RapidFire.InfiniteAmmo
				range.Value = RapidFire.InfiniteRange
				dbgWarn(
					dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************'})
						.. tool.Name
						.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','*************************************************************'})
						.. ammo.Value
						.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','**********************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','*************************************************************'})
						.. range.Value
				)
			end)
			if not success then
				dbgWarn(
					dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*********************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','*******************************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*************************************************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','***********************************************','******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'})
						.. tool.Name
						.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************'})
						.. tostring(err)
				)
			end
			task.wait(0.1)
		end
		maid:Destroy()
		GunMaids[tool] = nil
		PatchedGuns[tool] = nil
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. tool.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************************','********************************','***************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************************','****************************************************************************************************************','********************************','*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'})
		)
	end)

	maid:GiveTask(RunService.Heartbeat:Connect(function()
		if
			not (RapidFireEnabled or TargetAll)
			or not mouseHeld
			or ammo.Value <= 0
			or tool.Parent ~= Client.Character
		then
			return
		end
		local success, err = pcall(function()
			range.Value = RapidFire.InfiniteRange
			applyWallbang()
			local muzzle = tool:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************','*****************************************************************************************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','********************************************************************************************************************'}))
				and tool.Default:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************'}))
				and tool.Default.Mesh:FindFirstChild(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*****************************************************************************','*********************************************************************************************************************','**************************************************************************************************************************','**************************************************************************************************************************','************************************************************************************************************','*****************************************************************************************************'}))
			local position = muzzle and muzzle.WorldPosition
				or handle.CFrame.Position
			if TargetAll then
				for _, p in pairs(Players:GetPlayers()) do
					if
						p ~= LocalPlayer
						and p.Character
						and p.Character:FindFirstChild(TargetPart)
					then
						local targetPart =
							p.Character:FindFirstChild(TargetPart)
						for i = 1, RapidFire.BulletsPerShot do
							local targetPos =
								PredictPosition(targetPart, BulletSpeed)
							local aimOffset =
								randomVector3(RapidFire.SpreadAmount)
							local finalPos = targetPos + aimOffset
							pcall(function()
								setidentity(2)
							end)
							local vShoot, v2, v3 = gunHandler.shoot({
								Shooter = LocalPlayer.Character,
								Handle = handle,
								ForcedOrigin = position,
								AimPosition = finalPos,
								BeamColor = Color3.new(1, 0.545, 0.149),
								Range = RapidFire.InfiniteRange,
							})
							pcall(function()
								setidentity(8)
							end)
							addShot()
							ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
								dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
								handle,
								position,
								vShoot,
								v2,
								v3
							)
							remote:FireServer(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
							dbgWarn(
								dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','*****************************************************************','************************************************************************************************************','************************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','*************************************************************************************************','********************************************************************************************************************','********************************'})
									.. p.Name
									.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
									.. tostring(finalPos)
							)
						end
					end
				end
			elseif
				RapidFireEnabled
				and Target
				and Target.Character
				and Target.Character:FindFirstChild(TargetPart)
			then
				for i = 1, RapidFire.BulletsPerShot do
					local targetPos = PredictPosition(
						Target.Character:FindFirstChild(TargetPart),
						BulletSpeed
					)
					local aimOffset = randomVector3(RapidFire.SpreadAmount)
					local finalPos = targetPos + aimOffset
					pcall(function()
						setidentity(2)
					end)
					local vShoot, v2, v3 = gunHandler.shoot({
						Shooter = LocalPlayer.Character,
						Handle = handle,
						ForcedOrigin = position,
						AimPosition = finalPos,
						BeamColor = Color3.new(1, 0.545, 0.149),
						Range = RapidFire.InfiniteRange,
					})
					pcall(function()
						setidentity(8)
					end)
					addShot()
					ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
						handle,
						position,
						vShoot,
						v2,
						v3
					)
					remote:FireServer(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
					dbgWarn(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','*************************************************************************************************','********************************************************************************************************************','********************************'})
							.. Target.Name
							.. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
							.. tostring(finalPos)
					)
				end
			elseif RapidFireEnabled then
				for i = 1, RapidFire.BulletsPerShot do
					local aimOffset = randomVector3(RapidFire.SpreadAmount)
					local targetPos = position
						+ gunHandler.getAim(position) * RapidFire.InfiniteRange
						+ aimOffset
					pcall(function()
						setidentity(2)
					end)
					local vShoot, v2, v3 = gunHandler.shoot({
						Shooter = LocalPlayer.Character,
						Handle = handle,
						ForcedOrigin = position,
						AimPosition = targetPos,
						BeamColor = Color3.new(1, 0.545, 0.149),
						Range = RapidFire.InfiniteRange,
					})
					pcall(function()
						setidentity(8)
					end)
					addShot()
					ReplicatedStorage.MainRemotes.MainRemoteEvent:FireServer(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','***********************************************************************','*********************************************************************************************************************','**************************************************************************************************************'}),
						handle,
						position,
						vShoot,
						v2,
						v3
					)
					remote:FireServer(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'}))
					dbgWarn(
						dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','********************************','*************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','*******************************************************************************************************************','*****************************************************************************************************','********************************','*******************************************************************************************************************','********************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************************','********************************','*****************************************************************','*********************************************************************************************************','*************************************************************************************************************','********************************************************************************','***************************************************************************************************************','*******************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','*************************************************************'})
							.. tostring(targetPos)
					)
				end
			end
		end)
		if not success then
			dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************','********************************','*****************************************************************************************************','******************************************************************************************************************','******************************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. tool.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************','********************************'}) .. tostring(err))
		end
	end))

	notify(
		dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************'}),
		dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************'}) .. tool.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','********************************','*******************************************','********************************','*************************************************************************','**************************************************************************************************************','******************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*********************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','********************************','*****************************************************************','*************************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************'}),
		3
	)
	dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************'}) .. tool.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','**********************************************************************************','*************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************'}))
	return maid
end

-- Auto Patch New Guns
Client.Backpack.ChildAdded:Connect(function(child)
	if GUN_NAMES[child.Name] then
		task.wait(0.1)
		patchGun(child)
	end
end)

-- Patch Existing Gun
local equippedGun = Client.Character
	and Client.Character:FindFirstChildOfClass(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
if equippedGun and GUN_NAMES[equippedGun.Name] then
	patchGun(equippedGun)
end

-- Prevent Duplicate Firing
local oldFireServer
oldFireServer = hookmetamethod(game, dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************************','***********************************************************************************************','**************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','*************************************************************************************************','************************************************************************************************************','************************************************************************************************************'}), function(self, ...)
	local method = getnamecallmethod()
	local args = { ... }
	if
		method == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})
		and self.Name == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************'})
		and args[1] == dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************'})
		and (RapidFireEnabled or TargetAll)
	then
		dbgWarn(
			dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'******************************************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','****************************************************************************************************','*****************************************************************************************************','******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************************','************************************************************************************************************','********************************************************************************************************************','********************************','**********************************************************************************','*****************************************************************************************************','*************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*********************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','**********************************************************','**********************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','***********************************************************************************','*****************************************************************************************************','******************************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','***********************************************************************************','********************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','****************************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','********************************','****************************************************************************************************','*********************************************************************************************************************','****************************************************************************************************************','************************************************************************************************************','*********************************************************************************************************','***************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','********************************','******************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************'})
		)
		return
	end
	return oldFireServer(self, ...)
end)

-- Cleanup and Re-patch on Respawn
Client.CharacterAdded:Connect(function(newCharacter)
	character = newCharacter
	tpwalking = false
	mouseHeld = false
	if
		ViewEnabled
		and Target
		and Target.Character
		and Target.Character:FindFirstChild(TargetPart)
	then
		local targetPart = Target.Character:FindFirstChild(TargetPart)
		if targetPart then
			CurrentCamera.CameraSubject = targetPart
			notify(
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}),
				dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*****************************************************************************************************','*********************************************','************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************','***********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','********************************','***************************************************************************************************************','**************************************************************************************************************','********************************'}) .. Target.DisplayName .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','*************************************************************************************************','******************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'}),
				3
			)
		else
			ViewEnabled = false
			CurrentCamera.CameraSubject = newCharacter
				and newCharacter:FindFirstChildWhichIsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
			notify(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************'}), dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************','*****************************************************************************************************','********************************************************************************************************************','********************************','****************************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','********************************','**************************************************************************************************************','***************************************************************************************************************','********************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','****************************************************************************************************','********************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','****************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************'}), 3)
		end
	else
		CurrentCamera.CameraSubject = newCharacter
			and newCharacter:FindFirstChildWhichIsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************','*********************************************************************************************************************','*************************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************'}))
	end
	Text.Visible = false
	Circle.Visible = false
	currentText = dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({})
	textIndex = 1
	for tool, maid in pairs(GunMaids) do
		maid:Destroy()
		GunMaids[tool] = nil
		PatchedGuns[tool] = nil
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************','************************************************************************************************************','*****************************************************************************************************','*************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*********************************************************************************************************************','****************************************************************************************************************','********************************','*****************************************************************************','*************************************************************************************************','*********************************************************************************************************','****************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************'}) .. tool.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','***************************************************************************************************************','**************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'}))
	end
	local equippedGun = newCharacter
		and newCharacter:FindFirstChildOfClass(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'}))
	if equippedGun and GUN_NAMES[equippedGun.Name] then
		patchGun(equippedGun)
		dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'**********************************************************************************','*****************************************************************************************************','*********************************************','****************************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*****************************************************************************************************','*****************************************************************************************************************','*********************************************************************************************************************','*********************************************************************************************************','****************************************************************************************************************','****************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. equippedGun.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','***************************************************************************************************************','**************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************'}))
	end
	local childAddedConnection
	childAddedConnection = newCharacter.ChildAdded:Connect(function(child)
		if child:IsA(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','************************************************************************************************************'})) and GUN_NAMES[child.Name] then
			task.wait(0.1)
			patchGun(child)
			dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**************************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','********************************'}) .. child.Name .. dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************','***************************************************************************************************************','**************************************************************************************************************','********************************','**************************************************************************************************************','*****************************************************************************************************','***********************************************************************************************************************','********************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
		end
	end)
	newCharacter.AncestryChanged:Connect(function()
		if not newCharacter:IsDescendantOf(workspace) then
			childAddedConnection:Disconnect()
			dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'********************************************************************','*********************************************************************************************************','*******************************************************************************************************************','***************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************','********************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','****************************************************************************************************','*****************************************************************','****************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','************************************************************************************************************','*********************************************************************************************************','*******************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','********************************','***************************************************************************************************************','************************************************************************************************************','****************************************************************************************************','********************************','***************************************************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'}))
		end
	end)
	applyWallbang()
	hookGunHandlerShoot()
	dbgWarn(dMrcqShXrbxajBtGjBkRNedqxsadITFLDv({'*******************************************************************','********************************************************************************************************','*************************************************************************************************','******************************************************************************************************************','*************************************************************************************************','***************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','****************************************************************************************************************','*************************************************************************************************','***********************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************************','********************************','******************************************************************************************************************','*****************************************************************************************************','*********************************************','****************************************************************************************************************','*************************************************************************************************','********************************************************************************************************************','***************************************************************************************************','********************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************','*********************************************************************************************************************','**************************************************************************************************************','*******************************************************************************************************************'}))
end)
    
