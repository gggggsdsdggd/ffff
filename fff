local ignoreList = {}
if Module then
    ignoreList = {
        workspace:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({9030,10605,10920,11025,10395,11340,10605,12075})) or Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7350,11655,11340,10500,10605,11970})),
        workspace:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,6825,8400})) or Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7350,11655,11340,10500,10605,11970})),
        workspace:FindFirstChild(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7665,10815,11550,11655,11970,10605,10500})) or Instance.new(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({7350,11655,11340,10500,10605,11970}))
    }
    Module.Ignored = ignoreList
    dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8610,10185,11760,11025,10500,3360,7350,11025,11970,10605,3360,11025,10815,11550,11655,11970,10605,3360,11340,11025,12075,12180,3360,12075,10605,12180,6090,3360,9030,10605,10920,11025,10395,11340,10605,12075,4620,3360,8085,6825,8400,4620,3360,7665,10815,11550,11655,11970,10605,10500}))
else
    dbgWarn(gCXSTIbtVnWlyaRjQoHxOcLLODzyJeFZewTNHctDQapSOhaQwhbCMnfllhGBCBJarMrGRjLvGhbxYTF({8085,10185,11025,11550,8085,11655,10500,12285,11340,10605,3360,11550,11655,12180,3360,10710,11655,12285,11550,10500,3360,11025,11550,3360,8610,10605,11760,11340,11025,10395,10185,12180,10605,10500,8715,12180,11655,11970,10185,10815,10605}))
