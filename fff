		)
	end
end)

-- Mouse Input for Rapid Fire
UserInputService.InputBegan:Connect(function(input, gpe)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		local equippedGun = Client.Character
			and Client.Character:FindFirstChildOfClass(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340}))
		if equippedGun and GUN_NAMES[equippedGun.Name] then
			mouseHeld = true
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,11655,12285,12075,10605,3360,10920,10605,11340,10500,4620,3360,10815,12285,11550,3360,10605,11865,12285,11025,11760,11760,10605,10500,6090,3360}) .. equippedGun.Name)
		end
	end
end)

UserInputService.InputEnded:Connect(function(input, gpe)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		mouseHeld = false
		dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8085,11655,12285,12075,10605,3360,11970,10605,11340,10605,10185,12075,10605,10500}))
	end
end)

-- Detect Gun Unequip
Client.Character.ChildRemoved:Connect(function(child)
	if child:IsA(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({8820,11655,11655,11340})) and GUN_NAMES[child.Name] then
		mouseHeld = false
		if GunMaids[child] then
			GunMaids[child]:Destroy()
			GunMaids[child] = nil
			PatchedGuns[child] = nil
			dbgWarn(YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({7455,12285,11550,3360,12285,11550,10605,11865,12285,11025,11760,11760,10605,10500,6090,3360}) .. child.Name .. YIDfdHNeiGgFHSVvEcnftRISmcXlYnQBbppIdqdKJfuKERvWHDAMloKvPCKdEodgFiJE({4620,3360,10395,11340,10605,10185,11550,10605,10500,3360,12285,11760,3360,8085,10185,11025,10500}))
		end
	end
end)

-- Beam Modification Logic
local recentShots = {}
local function addShot()
	local shotTime = tick()
	table.insert(recentShots, shotTime)
	for i = #recentShots, 1, -1 do
		if tick() - recentShots[i] > 3 then
			table.remove(recentShots, i)
		end
	end
end

