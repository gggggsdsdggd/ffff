local function GetClosestTarget()
    local Closest, MinDist = nil, math.huge
    local MousePos = Vector2.new(Mouse.X, Mouse.Y)
    for _, Player in ipairs(Players:GetPlayers()) do
        if Player ~= Client and Player.Character then
            local Root = Player.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,12285,11445,10185,11550,11655,11025,10500,8610,11655,11655,12180,8400,10185,11970,12180}))
            local Humanoid = Player.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7560,12285,11445,10185,11550,11655,11025,10500}))
            local ISNTKNOCKED = Player.Character:FindFirstChild(CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({6930,11655,10500,12705,7245,10710,10710,10605,10395,12180,12075}))
                and Player.Character.BodyEffects[CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7875,4830,8295})].Value ~= true
            local ISNTGRABBED = Player.Character:FindFirstChild(
                CuPPYUQjCoARnbhTkKjDRGxClIqHwCDMfkDAXKmXYPaAHXRWasDyrVVgPfdIEJHBAHMOtLtHgvZCTqZbzgqdwWuWVUWw({7455,8610,6825,6930,6930,7665,8190,7455,9975,7035,8295,7665,8190,8715,8820,8610,6825,7665,8190,8820})
            ) == nil
            if
                Root
                and Humanoid
                and Humanoid.Health > 1
                and ISNTKNOCKED
                and ISNTGRABBED
            then
                local screenPos, onScreen =
                    CurrentCamera:WorldToViewportPoint(Root.Position)
                if onScreen then
                    local Dist = (
                        Vector2.new(screenPos.X, screenPos.Y) - MousePos
                    ).Magnitude
                    if Dist < MinDist then
                        MinDist = Dist
                        Closest = Player
                    end
                end
            end
        end
    end
    return Closest
end

