			end
		end
	end
	-- TP Walk Key
	if input.KeyCode == Keybinds.TPWalk and tpWalkEnabled then
		tpwalking = not tpwalking
		notify(
			cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'************************************************************************************','********************************************************************************','********************************','***************************************************************************************','*************************************************************************************************','************************************************************************************************************','***********************************************************************************************************'}),
			tpwalking and cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'*********************************************************************','**************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************************************************'}) or cXzHpnzqyhdhtzuGclbuiFDGFlOaAvhBRlRHBOCUrceWdngDnilNDZeCSCzOmQxGEJWjgwErrTUjzxXCmiJF({'********************************************************************','*********************************************************************************************************','*******************************************************************************************************************','*************************************************************************************************','**************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','**********************************************************************************************************************','*********************************************************************************************************','*************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','****************************************************************