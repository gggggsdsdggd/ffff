-- Utility Functions
local function findPlayer(name)
	name = name and name:lower()
	if not name then
		dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##############################################################################################################','####################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','#####################################################################','#############################################################################################################','################################################################################################################','####################################################################################################################','#########################################################################################################################','################################','##############################################################################################################','#################################################################################################','#############################################################################################################','#####################################################################################################','################################','################################################################################################################','##################################################################################################################','###############################################################################################################','######################################################################################################################','#########################################################################################################','####################################################################################################','#####################################################################################################','####################################################################################################'}))
		return nil
	end
	for _, plr in ipairs(Players:GetPlayers()) do
		if
			plr.Name:lower():find(name) or plr.DisplayName:lower():find(name)
		then
			dbgWarn(
				hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##############################################################################################################','####################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','######################################################################','###############################################################################################################','#####################################################################################################################','##############################################################################################################','####################################################################################################','################################','################################################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','################################'}) .. plr.Name .. hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'################################','######################################################################################################','###############################################################################################################','##################################################################################################################','################################','#########################################################################################################','##############################################################################################################','################################################################################################################','#####################################################################################################################','####################################################################################################################','################################'}) .. name
			)
			return plr
		end
	end
	dbgWarn(hVtLUVyBXkKCoZwEpuMNXQEiTnaSnHhdjNaFrlDoykjMdjetZvzGh({'######################################################################################################','#########################################################################################################','##############################################################################################################','####################################################################################################','################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','#####################################################################################################','##################################################################################################################','##########################################################','################################','##############################################################################','###############################################################################################################','################################','################################################################################################################','############################################################################################################','#################################################################################################','#########################################################################################################################','###########################################################################################